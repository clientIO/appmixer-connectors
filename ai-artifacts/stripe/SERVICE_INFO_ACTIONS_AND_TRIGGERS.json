        {
            "title": "Create Payment Intent",
            "description": "Creates a Payment Intent to initiate a payment process with specified parameters like amount and currency.",
            "type": "action",
            "documentation": "https://stripe.com/docs/api/payment_intents/create",
            "endpointUrl": "https://api.stripe.com/v1/payment_intents",
            "apiBaseUrl": "https://api.stripe.com",
            "method": "POST",
            "sampleResponse": {
                "id": "pi_1Example",
                "object": "payment_intent",
                "amount": 1000,
                "currency": "usd",
                "status": "requires_confirmation"
            },
            "inputSchema": {
                "type": "object",
                "description": "Input parameters for creating a Payment Intent",
                "properties": {
                    "amount": {
                        "type": "number",
                        "description": "Amount to be collected, in the smallest currency unit."
                    },
                    "currency": {
                        "type": "string",
                        "description": "Three-letter ISO currency code, in lowercase."
                    },
                    "payment_method": {
                        "type": "string",
                        "description": "Optional payment method ID to attach to the Payment Intent."
                    },
                    "description": {
                        "type": "string",
                        "description": "Optional description to attach to the Payment Intent."
                    }
                },
                "required": [
                    "amount",
                    "currency"
                ]
            },
            "outputType": {
                "outputType": "object",
                "arrayPropertyLabel": "",
                "arrayPropertyValue": ""
            }
        },
