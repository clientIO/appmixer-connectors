                { "label": "Name", "value": "name" },
                { "label": "Created time", "value": "btime" },
                { "label": "Modified time", "value": "mtime" },
                { "label": "Flow Descriptor", "value": "flow" },
                { "label": "Thumbnail Data URI", "value": "thumbnail" },
                { "label": "Started time", "value": "started" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
