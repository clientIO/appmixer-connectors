{
    "version": "1.0.0",
    "name": "appmixer.system.core.OnAnyFlowComponentError",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Triggered every time there is an error in any flow in the entire Appmixer tenant.",
    "private": true,
    "quota": {},
    "webhook": true,
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Error Details", "value":  "err" },
                { "label": "Type", "value": "type" },
                { "label": "Flow ID", "value": "flowId" },
                { "label": "Flow Name", "value": "flowName" },
                { "label": "User ID", "value": "userId" },
                { "label": "Component ID", "value": "componentId" },
                { "label": "Component Type", "value": "componentType" },
                { "label": "Input Message(s)", "value": "inputMessages" }
            ]
        }
    ],
    "properties": {
        "schema": {
            "properties": {},
            "required": []
        },
        "inspector": {
            "inputs": {}
        }
    },
    "icon": "data:image/svg+xml;base64,"
}
