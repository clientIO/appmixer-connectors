{
    "name": "appmixer.facebook.place.FindPlace",
    "author": "Martin Krcmar <martin@client.io>",
    "description": "Find place (and it's ID) according to name.",
    "private": true,
    "auth": {
        "service": "appmixer:facebook"
    },
    "quota": {
        "manager": "appmixer:facebook",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "properties": {
        "schema": {
            "properties": {
                "place": {
                    "type": "string"
                }
            },
            "required": [ "place" ]
        }
    },
    "inPorts": [ "in" ],
    "outPorts": [ "place" ],
    "icon": "data:image/svg+xml;base64,"
}
