    { "op": "add", "jsonpath": "$.paths.*[?(@.operationId == 'createImage' || @.operationId == 'listModels' || @.operationId == 'listFiles' || @.operationId == 'listPaginatedFineTuningJobs' || @.operationId == 'listFineTuningEvents')]", "path": "x-connector-pagination", "value": {
