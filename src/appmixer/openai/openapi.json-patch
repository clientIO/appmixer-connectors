    } },
    { "op": "add", "path": "/paths/~1images~1generations/post/requestBody/content/application~1json/schema/properties/model/x-connector-source", "value": {
        "operationId": "listModels",
        "transform": "result[].{value:id, label:id}"
    } },
    { "op": "add", "path": "/paths/~1audio~1speech/post/requestBody/content/application~1json/schema/properties/model/x-connector-source", "value": {
        "operationId": "listModels",
        "transform": "result[].{value:id, label:id}"
    } },
    { "op": "add", "path": "/paths/~1fine_tuning~1jobs/post/requestBody/content/application~1json/schema/properties/model/x-connector-source", "value": {
        "operationId": "listModels",
        "transform": "result[].{value:id, label:id}"
    } },
    { "op": "add", "path": "/paths/~1chat~1completions/post/requestBody/content/application~1json/schema/properties/prompt_role", "value": {
        "type": "string",
