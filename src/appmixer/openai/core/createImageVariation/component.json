{
    "version": "1.0.0",
    "name": "appmixer.openai.core.createImageVariation",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Creates a variation of a given image.</p></label>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "required": [
                    "image"
                ],
                "properties": {
                    "image": {
                        "description": "The image to use as the basis for the variation(s). Must be a valid PNG file, less than 4MB, and square.",
                        "type": "string",
                        "path": "image"
                    },
                    "model": {
                        "x-oaiTypeLabel": "string",
                        "default": "dall-e-2",
                        "example": "dall-e-2",
                        "nullable": true,
                        "description": "The model to use for image generation. Only `dall-e-2` is supported at this time.",
                        "type": "string",
                        "path": "model"
                    },
                    "n": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 10,
                        "default": 1,
                        "example": 1,
                        "nullable": true,
                        "description": "The number of images to generate. Must be between 1 and 10. For `dall-e-3`, only `n=1` is supported.",
                        "path": "n"
                    },
                    "response_format": {
                        "type": "string",
                        "enum": [
                            "url",
                            "b64_json"
                        ],
                        "default": "url",
                        "example": "url",
                        "nullable": true,
                        "description": "The format in which the generated images are returned. Must be one of `url` or `b64_json`.",
                        "path": "response_format"
                    },
                    "size": {
                        "type": "string",
                        "enum": [
                            "256x256",
                            "512x512",
                            "1024x1024"
                        ],
                        "default": "1024x1024",
                        "example": "1024x1024",
                        "nullable": true,
                        "description": "The size of the generated images. Must be one of `256x256`, `512x512`, or `1024x1024`.",
                        "path": "size"
                    },
                    "user": {
                        "type": "string",
                        "example": "user-1234",
                        "description": "A unique identifier representing your end-user, which can help OpenAI to monitor and detect abuse. [Learn more](/docs/guides/safety-best-practices/end-user-ids).\n",
                        "path": "user"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "image": {
                        "type": "filepicker",
                        "index": 0,
                        "label": "Image",
                        "tooltip": "<p>The image to use as the basis for the variation(s). Must be a valid PNG file, less than 4MB, and square.</p>"
                    },
                    "model": {
                        "type": "text",
                        "index": 1,
                        "label": "Model",
                        "tooltip": "<p>The model to use for image generation. Only <code>dall-e-2</code> is supported at this time. Example: dall-e-2</p>",
                        "defaultValue": "dall-e-2"
                    },
                    "n": {
                        "type": "number",
                        "index": 2,
                        "label": "N",
                        "tooltip": "<p>The number of images to generate. Must be between 1 and 10. For <code>dall-e-3</code>, only <code>n=1</code> is supported. Example: 1</p>",
                        "defaultValue": 1,
                        "min": 1,
                        "max": 10
                    },
                    "response_format": {
                        "type": "select",
                        "index": 3,
                        "label": "Response Format",
                        "tooltip": "<p>The format in which the generated images are returned. Must be one of <code>url</code> or <code>b64_json</code>. Example: url</p>",
                        "options": [
                            {
                                "content": "url",
                                "value": "url"
                            },
                            {
                                "content": "b64_json",
                                "value": "b64_json"
                            }
                        ],
                        "defaultValue": "url"
                    },
                    "size": {
                        "type": "select",
                        "index": 4,
                        "label": "Size",
                        "tooltip": "<p>The size of the generated images. Must be one of <code>256x256</code>, <code>512x512</code>, or <code>1024x1024</code>. Example: 1024x1024</p>",
                        "options": [
                            {
                                "content": "256x256",
                                "value": "256x256"
                            },
                            {
                                "content": "512x512",
                                "value": "512x512"
                            },
                            {
                                "content": "1024x1024",
                                "value": "1024x1024"
                            }
                        ],
                        "defaultValue": "1024x1024"
                    },
                    "user": {
                        "type": "text",
                        "index": 5,
                        "label": "User",
                        "tooltip": "<p>A unique identifier representing your end-user, which can help OpenAI to monitor and detect abuse. <a href=\"https://platform.openai.com/docs/guides/safety-best-practices/end-user-ids\" rel=\"noopener noreferrer\" target=\"_blank\">Learn more</a>.\n Example: user-1234</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Created",
                    "value": "created"
                },
                {
                    "label": "Data",
                    "value": "data",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "description": "Represents the url or the content of an image generated by the OpenAI API.",
                            "x-oaiMeta": {
                                "name": "The image object",
                                "example": "{\n  \"url\": \"...\",\n  \"revised_prompt\": \"...\"\n}\n"
                            },
                            "properties": {
                                "b64_json": {
                                    "type": "string",
                                    "description": "The base64-encoded JSON of the generated image, if `response_format` is `b64_json`."
                                },
                                "url": {
                                    "type": "string",
                                    "description": "The URL of the generated image, if `response_format` is `url` (default)."
                                },
                                "revised_prompt": {
                                    "type": "string",
                                    "description": "The prompt that was used to generate the image, if there was any revision to the prompt."
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:openai"
    },
    "icon": "data:image/svg+xml;base64,"
}