{
    "version": "1.0.0",
    "name": "appmixer.openai.core.createTranscription",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Transcribes audio into the input language.</p></label>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "required": [
                    "file",
                    "model"
                ],
                "properties": {
                    "file": {
                        "description": "The audio file object (not file name) to transcribe, in one of these formats: flac, mp3, mp4, mpeg, mpga, m4a, ogg, wav, or webm.\n",
                        "type": "string",
                        "x-oaiTypeLabel": "file",
                        "path": "file"
                    },
                    "model": {
                        "description": "ID of the model to use. Only `whisper-1` is currently available.\n",
                        "example": "whisper-1",
                        "x-oaiTypeLabel": "string",
                        "type": "string",
                        "path": "model"
                    },
                    "language": {
                        "description": "The language of the input audio. Supplying the input language in [ISO-639-1](https://en.wikipedia.org/wiki/List_of_ISO_639-1_codes) format will improve accuracy and latency.\n",
                        "type": "string",
                        "path": "language"
                    },
                    "prompt": {
                        "description": "An optional text to guide the model's style or continue a previous audio segment. The [prompt](/docs/guides/speech-to-text/prompting) should match the audio language.\n",
                        "type": "string",
                        "path": "prompt"
                    },
                    "response_format": {
                        "description": "The format of the transcript output, in one of these options: `json`, `text`, `srt`, `verbose_json`, or `vtt`.\n",
                        "type": "string",
                        "enum": [
                            "json",
                            "text",
                            "srt",
                            "verbose_json",
                            "vtt"
                        ],
                        "default": "json",
                        "path": "response_format"
                    },
                    "temperature": {
                        "description": "The sampling temperature, between 0 and 1. Higher values like 0.8 will make the output more random, while lower values like 0.2 will make it more focused and deterministic. If set to 0, the model will use [log probability](https://en.wikipedia.org/wiki/Log_probability) to automatically increase the temperature until certain thresholds are hit.\n",
                        "type": "number",
                        "default": 0,
                        "path": "temperature"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "file": {
                        "type": "filepicker",
                        "index": 0,
                        "label": "File",
                        "tooltip": "<p>The audio file object (not file name) to transcribe, in one of these formats: flac, mp3, mp4, mpeg, mpga, m4a, ogg, wav, or webm.</p>"
                    },
                    "model": {
                        "type": "text",
                        "index": 1,
                        "label": "Model",
                        "tooltip": "<p>ID of the model to use. Only <code>whisper-1</code> is currently available.\n Example: whisper-1</p>"
                    },
                    "language": {
                        "type": "text",
                        "index": 2,
                        "label": "Language",
                        "tooltip": "<p>The language of the input audio. Supplying the input language in <a href=\"https://en.wikipedia.org/wiki/List_of_ISO_639-1_codes\" rel=\"noopener noreferrer\" target=\"_blank\">ISO-639-1</a> format will improve accuracy and latency.</p>"
                    },
                    "prompt": {
                        "type": "text",
                        "index": 3,
                        "label": "Prompt",
                        "tooltip": "<p>An optional text to guide the model's style or continue a previous audio segment. The <a href=\"https://platform.openai.com/docs/guides/speech-to-text/prompting\" rel=\"noopener noreferrer\" target=\"_blank\">prompt</a> should match the audio language.</p>"
                    },
                    "response_format": {
                        "type": "select",
                        "index": 4,
                        "label": "Response Format",
                        "tooltip": "<p>The format of the transcript output, in one of these options: <code>json</code>, <code>text</code>, <code>srt</code>, <code>verbose_json</code>, or <code>vtt</code>.</p>",
                        "options": [
                            {
                                "content": "json",
                                "value": "json"
                            },
                            {
                                "content": "text",
                                "value": "text"
                            },
                            {
                                "content": "srt",
                                "value": "srt"
                            },
                            {
                                "content": "verbose_json",
                                "value": "verbose_json"
                            },
                            {
                                "content": "vtt",
                                "value": "vtt"
                            }
                        ],
                        "defaultValue": "json"
                    },
                    "temperature": {
                        "type": "number",
                        "index": 5,
                        "label": "Temperature",
                        "tooltip": "<p>The sampling temperature, between 0 and 1. Higher values like 0.8 will make the output more random, while lower values like 0.2 will make it more focused and deterministic. If set to 0, the model will use <a href=\"https://en.wikipedia.org/wiki/Log_probability\" rel=\"noopener noreferrer\" target=\"_blank\">log probability</a> to automatically increase the temperature until certain thresholds are hit.</p>",
                        "defaultValue": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Text",
                    "value": "text"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:openai"
    },
    "icon": "data:image/svg+xml;base64,"
}