{
    "name": "appmixer.discord.core.CreateaChannelInvite",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new invite for the channel. It works only with a connection with permissions to manage invites.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "channelId": {
                        "type": "string",
                        "description": "ID of the channel"
                    },
                    "max_age": {
                        "type": "integer",
                        "description": "Duration of invite in seconds before expiry (0 = never)"
                    },
                    "max_uses": {
                        "type": "integer",
                        "description": "Max number of uses"
                    },
                    "temporary": {
                        "type": "boolean",
                        "description": "Whether this invite only grants temporary membership"
                    },
                    "unique": {
                        "type": "boolean",
                        "description": "Create a unique invite, or use similar one"
                    },
                    "target_type": {
                        "type": "integer",
                        "description": "Type of target for stream invite"
                    },
                    "target_user_id": {
                        "type": "string",
                        "description": "User ID for stream invite"
                    },
                    "target_application_id": {
                        "type": "string",
                        "description": "Application ID for embedded application invite"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "channelId": {
                        "type": "text",
                        "tooltip": "ID of the channel",
                        "label": "ChannelId",
                        "index": 0
                    },
                    "max_age": {
                        "type": "number",
                        "tooltip": "Duration of invite in seconds before expiry (0 = never)",
                        "label": "Max Age",
                        "index": 1
                    },
                    "max_uses": {
                        "type": "number",
                        "tooltip": "Max number of uses",
                        "label": "Max Uses",
                        "index": 2
                    },
                    "temporary": {
                        "type": "toggle",
                        "tooltip": "Whether this invite only grants temporary membership",
                        "label": "Temporary",
                        "index": 3
                    },
                    "unique": {
                        "type": "toggle",
                        "tooltip": "Create a unique invite, or use similar one",
                        "label": "Unique",
                        "index": 4
                    },
                    "target_type": {
                        "type": "number",
                        "tooltip": "Type of target for stream invite",
                        "label": "Target Type",
                        "index": 5
                    },
                    "target_user_id": {
                        "type": "text",
                        "tooltip": "User ID for stream invite",
                        "label": "Target User Id",
                        "index": 6
                    },
                    "target_application_id": {
                        "type": "text",
                        "tooltip": "Application ID for embedded application invite",
                        "label": "Target Application Id",
                        "index": 7
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Code",
                    "value": "code",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Guild",
                    "value": "guild",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "title": "Guild.Id"
                            },
                            "name": {
                                "type": "string",
                                "title": "Guild.Name"
                            }
                        }
                    }
                },
                {
                    "label": "Channel",
                    "value": "channel",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "title": "Channel.Id"
                            },
                            "name": {
                                "type": "string",
                                "title": "Channel.Name"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}