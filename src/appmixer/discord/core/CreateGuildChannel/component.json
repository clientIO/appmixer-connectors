{
    "name": "appmixer.discord.core.CreateGuildChannel",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new channel for the guild.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "type": {
                        "type": "integer"
                    },
                    "topic": {
                        "type": "string"
                    },
                    "rate_limit_per_user": {
                        "type": "integer"
                    },
                    "position": {
                        "type": "integer"
                    },
                    "parent_id": {
                        "type": "string"
                    },
                    "nsfw": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "name", "type"
                ]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the channel",
                        "label": "Name",
                        "index": 1
                    },
                    "type": {
                        "type": "select",
                        "tooltip": "Type of channel",
                        "label": "Type",
                        "index": 2,
                        "options": [
                            { "label": "Guild Text", "value": 0 },
                            { "label": "DM", "value": 1 },
                            { "label": "Group DM", "value": 3 },
                            { "label": "Guild Category", "value": 4 },
                            { "label": "Guild Announcement", "value": 5 }
                        ]
                    },
                    "topic": {
                        "type": "text",
                        "tooltip": "Channel topic",
                        "label": "Topic",
                        "index": 3
                    },
                    "rate_limit_per_user": {
                        "type": "number",
                        "tooltip": "Enter the amount of seconds a user has to wait before sending another message (0 - 21600).",
                        "label": "Rate Limit Per User",
                        "index": 4
                    },
                    "position": {
                        "type": "number",
                        "tooltip": "Sorting position of the channel",
                        "label": "Position",
                        "index": 5
                    },
                    "parent_id": {
                        "type": "text",
                        "tooltip": "ID of the parent category",
                        "label": "Parent ID",
                        "index": 6
                    },
                    "nsfw": {
                        "type": "toggle",
                        "tooltip": "Whether the channel is NSFW",
                        "label": "Nsfw",
                        "index": 7
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Channel ID",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Type",
                    "value": "type",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Last Message ID",
                    "value": "last_message_id",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Flags",
                    "value": "flags",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Guild ID",
                    "value": "guild_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Parent ID",
                    "value": "parent_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Rate Limit Per User",
                    "value": "rate_limit_per_user",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Topic",
                    "value": "topic",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Position",
                    "value": "position",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Permission Overwrites",
                    "value": "permission_overwrites",
                    "schema": {
                        "type": "array",
                        "items": {}
                    }
                },
                {
                    "label": "Nsfw",
                    "value": "nsfw",
                    "schema": {
                        "type": "boolean"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
