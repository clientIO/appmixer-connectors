{
    "name": "appmixer.discord.core.CreateChannelInvite",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new invite for the channel.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "channelId": {
                        "type": "string"
                    },
                    "maxAge": {
                        "type": "integer"
                    },
                    "maxUses": {
                        "type": "integer"
                    },
                    "temporary": {
                        "type": "boolean"
                    },
                    "unique": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "channelId", "maxAge"
                ]
            },
            "inspector": {
                "inputs": {
                    "channelId": {
                        "type": "text",
                        "tooltip": "ID of the channel",
                        "label": "Channel ID",
                        "index": 0,
                        "source": {
                            "url": "/component/appmixer/discord/core/ListChannels?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "transform": "./ListChannels#toSelectArray"
                            }
                        }
                    },
                    "maxAge": {
                        "type": "number",
                        "tooltip": "Duration of invite in seconds before expiry (0 = never).",
                        "label": "Max Age",
                        "index": 1
                    },
                    "maxUses": {
                        "type": "number",
                        "tooltip": "Max number of uses (0 = unlimited).",
                        "label": "Max Uses",
                        "index": 2
                    },
                    "temporary": {
                        "type": "toggle",
                        "tooltip": "Whether this invite only grants temporary membership.",
                        "label": "Temporary",
                        "index": 3
                    },
                    "unique": {
                        "type": "toggle",
                        "tooltip": "Create a unique invite, or use similar one.",
                        "label": "Unique",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
             "options": [
                {
                    "label": "Type",
                    "value": "type",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Code",
                    "value": "code",
                    "schema": {
                        "type": "string"
                    }
                },
                {
