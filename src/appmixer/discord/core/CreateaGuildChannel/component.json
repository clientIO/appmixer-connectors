{
    "name": "appmixer.discord.core.CreateaGuildChannel",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new channel for the guild.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "guildId": {
                        "type": "string",
                        "description": "ID of the guild"
                    },
                    "name": {
                        "type": "string",
                        "description": "Name of the channel"
                    },
                    "type": {
                        "type": "integer",
                        "description": "Type of channel"
                    },
                    "topic": {
                        "type": "string",
                        "description": "Channel topic"
                    },
                    "bitrate": {
                        "type": "integer",
                        "description": "Bitrate (for voice channels)"
                    },
                    "user_limit": {
                        "type": "integer",
                        "description": "User limit (for voice channels)"
                    },
                    "rate_limit_per_user": {
                        "type": "integer",
                        "description": "Slowmode rate limit in seconds"
                    },
                    "position": {
                        "type": "integer",
                        "description": "Sorting position of the channel"
                    },
                    "permission_overwrites": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "Permission overwrites"
                    },
                    "parent_id": {
                        "type": "string",
                        "description": "ID of the parent category"
                    },
                    "nsfw": {
                        "type": "boolean",
                        "description": "Whether the channel is NSFW"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "guildId": {
                        "type": "text",
                        "tooltip": "ID of the guild",
                        "label": "GuildId",
                        "index": 0
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the channel",
                        "label": "Name",
                        "index": 1
                    },
                    "type": {
                        "type": "number",
                        "tooltip": "Type of channel",
                        "label": "Type",
                        "index": 2
                    },
                    "topic": {
                        "type": "text",
                        "tooltip": "Channel topic",
                        "label": "Topic",
                        "index": 3
                    },
                    "bitrate": {
                        "type": "number",
                        "tooltip": "Bitrate (for voice channels)",
                        "label": "Bitrate",
                        "index": 4
                    },
                    "user_limit": {
                        "type": "number",
                        "tooltip": "User limit (for voice channels)",
                        "label": "User Limit",
                        "index": 5
                    },
                    "rate_limit_per_user": {
                        "type": "number",
                        "tooltip": "Slowmode rate limit in seconds",
                        "label": "Rate Limit Per User",
                        "index": 6
                    },
                    "position": {
                        "type": "number",
                        "tooltip": "Sorting position of the channel",
                        "label": "Position",
                        "index": 7
                    },
                    "permission_overwrites": {
                        "type": "text",
                        "tooltip": "Permission overwrites",
                        "label": "Permission Overwrites",
                        "index": 8
                    },
                    "parent_id": {
                        "type": "text",
                        "tooltip": "ID of the parent category",
                        "label": "Parent Id",
                        "index": 9
                    },
                    "nsfw": {
                        "type": "toggle",
                        "tooltip": "Whether the channel is NSFW",
                        "label": "Nsfw",
                        "index": 10
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Type",
                    "value": "type",
                    "schema": {
                        "type": "number"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}