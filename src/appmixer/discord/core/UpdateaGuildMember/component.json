{
    "name": "appmixer.discord.core.UpdateaGuildMember",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Updates an existing guild member.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "guildId": {
                        "type": "string",
                        "description": "ID of the guild"
                    },
                    "userId": {
                        "type": "string",
                        "description": "ID of the user"
                    },
                    "nick": {
                        "type": "string",
                        "description": "Nickname"
                    },
                    "roles": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "Array of role IDs"
                    },
                    "mute": {
                        "type": "boolean",
                        "description": "Mute status"
                    },
                    "deaf": {
                        "type": "boolean",
                        "description": "Deafened status"
                    },
                    "channel_id": {
                        "type": "string",
                        "description": "Voice channel ID"
                    },
                    "communication_disabled_until": {
                        "type": "string",
                        "description": "Timeout until ISO8601"
                    },
                    "flags": {
                        "type": "integer",
                        "description": "User flags"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "guildId": {
                        "type": "text",
                        "tooltip": "ID of the guild",
                        "label": "GuildId",
                        "index": 0
                    },
                    "userId": {
                        "type": "text",
                        "tooltip": "ID of the user",
                        "label": "UserId",
                        "index": 1
                    },
                    "nick": {
                        "type": "text",
                        "tooltip": "Nickname",
                        "label": "Nick",
                        "index": 2
                    },
                    "roles": {
                        "type": "text",
                        "tooltip": "Array of role IDs",
                        "label": "Roles",
                        "index": 3
                    },
                    "mute": {
                        "type": "toggle",
                        "tooltip": "Mute status",
                        "label": "Mute",
                        "index": 4
                    },
                    "deaf": {
                        "type": "toggle",
                        "tooltip": "Deafened status",
                        "label": "Deaf",
                        "index": 5
                    },
                    "channel_id": {
                        "type": "text",
                        "tooltip": "Voice channel ID",
                        "label": "Channel Id",
                        "index": 6
                    },
                    "communication_disabled_until": {
                        "type": "text",
                        "tooltip": "Timeout until ISO8601",
                        "label": "Communication Disabled Until",
                        "index": 7
                    },
                    "flags": {
                        "type": "number",
                        "tooltip": "User flags",
                        "label": "Flags",
                        "index": 8
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "User",
                    "value": "user",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "title": "User.Id"
                            },
                            "username": {
                                "type": "string",
                                "title": "User.Username"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}