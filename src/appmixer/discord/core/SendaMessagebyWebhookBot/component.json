{
    "name": "appmixer.discord.core.SendaMessagebyWebhookBot",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Sends a new message by executing a (custom) webhook bot.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "webhookId": {
                        "type": "string",
                        "description": "ID of the webhook"
                    },
                    "webhookToken": {
                        "type": "string",
                        "description": "Token of the webhook"
                    },
                    "content": {
                        "type": "string",
                        "description": "Message content"
                    },
                    "username": {
                        "type": "string",
                        "description": "Override username"
                    },
                    "avatar_url": {
                        "type": "string",
                        "description": "Override avatar URL"
                    },
                    "tts": {
                        "type": "boolean",
                        "description": "Whether this is a TTS message"
                    },
                    "embeds": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "Embedded rich content"
                    },
                    "allowed_mentions": {
                        "type": "object",
                        "description": "Allowed mentions"
                    },
                    "components": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "Message components"
                    },
                    "threadId": {
                        "type": "string",
                        "description": "Thread ID"
                    },
                    "flags": {
                        "type": "integer",
                        "description": "Message flags"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "webhookId": {
                        "type": "text",
                        "tooltip": "ID of the webhook",
                        "label": "WebhookId",
                        "index": 0
                    },
                    "webhookToken": {
                        "type": "text",
                        "tooltip": "Token of the webhook",
                        "label": "WebhookToken",
                        "index": 1
                    },
                    "content": {
                        "type": "text",
                        "tooltip": "Message content",
                        "label": "Content",
                        "index": 2
                    },
                    "username": {
                        "type": "text",
                        "tooltip": "Override username",
                        "label": "Username",
                        "index": 3
                    },
                    "avatar_url": {
                        "type": "text",
                        "tooltip": "Override avatar URL",
                        "label": "Avatar Url",
                        "index": 4
                    },
                    "tts": {
                        "type": "toggle",
                        "tooltip": "Whether this is a TTS message",
                        "label": "Tts",
                        "index": 5
                    },
                    "embeds": {
                        "type": "text",
                        "tooltip": "Embedded rich content",
                        "label": "Embeds",
                        "index": 6
                    },
                    "allowed_mentions": {
                        "type": "text",
                        "tooltip": "Allowed mentions",
                        "label": "Allowed Mentions",
                        "index": 7
                    },
                    "components": {
                        "type": "text",
                        "tooltip": "Message components",
                        "label": "Components",
                        "index": 8
                    },
                    "threadId": {
                        "type": "text",
                        "tooltip": "Thread ID",
                        "label": "ThreadId",
                        "index": 9
                    },
                    "flags": {
                        "type": "number",
                        "tooltip": "Message flags",
                        "label": "Flags",
                        "index": 10
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Content",
                    "value": "content",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}