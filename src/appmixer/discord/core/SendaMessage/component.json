{
    "name": "appmixer.discord.core.SendaMessage",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Sends a message to a specified channel, thread, or guild member.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "channelId": {
                        "type": "string",
                        "description": "ID of the channel"
                    },
                    "content": {
                        "type": "string",
                        "description": "Message content"
                    },
                    "tts": {
                        "type": "boolean",
                        "description": "Whether this is a TTS message"
                    },
                    "embeds": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "Embedded rich content"
                    },
                    "allowed_mentions": {
                        "type": "object",
                        "description": "Allowed mentions"
                    },
                    "message_reference": {
                        "type": "object",
                        "description": "Reference to another message"
                    },
                    "components": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "Message components"
                    },
                    "sticker_ids": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "IDs of stickers to send"
                    },
                    "flags": {
                        "type": "integer",
                        "description": "Message flags"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "channelId": {
                        "type": "text",
                        "tooltip": "ID of the channel",
                        "label": "ChannelId",
                        "index": 0
                    },
                    "content": {
                        "type": "text",
                        "tooltip": "Message content",
                        "label": "Content",
                        "index": 1
                    },
                    "tts": {
                        "type": "toggle",
                        "tooltip": "Whether this is a TTS message",
                        "label": "Tts",
                        "index": 2
                    },
                    "embeds": {
                        "type": "text",
                        "tooltip": "Embedded rich content",
                        "label": "Embeds",
                        "index": 3
                    },
                    "allowed_mentions": {
                        "type": "text",
                        "tooltip": "Allowed mentions",
                        "label": "Allowed Mentions",
                        "index": 4
                    },
                    "message_reference": {
                        "type": "text",
                        "tooltip": "Reference to another message",
                        "label": "Message Reference",
                        "index": 5
                    },
                    "components": {
                        "type": "text",
                        "tooltip": "Message components",
                        "label": "Components",
                        "index": 6
                    },
                    "sticker_ids": {
                        "type": "text",
                        "tooltip": "IDs of stickers to send",
                        "label": "Sticker Ids",
                        "index": 7
                    },
                    "flags": {
                        "type": "number",
                        "tooltip": "Message flags",
                        "label": "Flags",
                        "index": 8
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Content",
                    "value": "content",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}