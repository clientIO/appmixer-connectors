{
    "name": "appmixer.discord.core.CreateChannelThread",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new channel thread.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:discord"
    },
    "quota": {
        "manager": "appmixer:discord",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "channelId": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "channelId", 
                    "name"
                ]
            },
            "inspector": {
                "inputs": {
                    "channelId": {
                        "type": "text",
                        "tooltip": "ID of the channel",
                        "label": "Channel ID",
                        "index": 0,
                        "source": {
                            "url": "/component/appmixer/discord/core/ListChannels?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "transform": "./ListChannels#toSelectArray"
                            }
                        }
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the thread",
                        "label": "Name",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Thread ID",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Type",
                    "value": "type",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Flags",
                    "value": "flags",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Guild ID",
                    "value": "guild_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Parent ID",
                    "value": "parent_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Rate Limit Per User",
                    "value": "rate_limit_per_user",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Bitrate",
                    "value": "bitrate",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "User Limit",
                    "value": "user_limit",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Owner ID",
                    "value": "owner_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Thread Metadata",
                    "value": "thread_metadata",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "archived": {
                                "type": "boolean",
                                "title": "Thread Metadata.Archived"
                            },
                            "archive_timestamp": {
                                "type": "string",
                                "title": "Thread Metadata.Archive Timestamp"
                            },
                            "auto_archive_duration": {
                                "type": "number",
                                "title": "Thread Metadata.Auto Archive Duration"
                            },
                            "locked": {
                                "type": "boolean",
                                "title": "Thread Metadata.Locked"
                            },
                            "create_timestamp": {
                                "type": "string",
                                "title": "Thread Metadata.Create Timestamp"
                            },
                            "invitable": {
                                "type": "boolean",
                                "title": "Thread Metadata.Invitable"
                            }
                        }
                    }
                },
                {
                    "label": "Message Count",
                    "value": "message_count",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Member Count",
                    "value": "member_count",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Total Message Sent",
                    "value": "total_message_sent",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Member",
                    "value": "member",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "title": "Member.ID"
                            },
                            "user_id": {
                                "type": "string",
                                "title": "Member.User ID"
                            },
                            "join_timestamp": {
                                "type": "string",
                                "title": "Member.Join Timestamp"
                            },
                            "flags": {
                                "type": "number",
                                "title": "Member.Flags"
                            },
                            "muted": {
                                "type": "boolean",
                                "title": "Member.Muted"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
