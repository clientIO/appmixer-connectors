{
    "name": "appmixer.slack.reactions.ListReactions",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "List all items (file, file comment, channel message, group message, or direct message) with reactions made by the user.",
    "private": false,
    "auth": {
        "service": "appmixer:slack",
        "scope": [
            "reactions:read"
        ]
    },
    "quota": {
        "manager": "appmixer:slack:list",
        "resources": "postMessage",
        "maxWait": 30000,
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string"
                    },
                    "outputType": {
                        "type": "string",
                        "enum": ["first", "array", "object", "file"]
                    },
                    "limit": {
                        "type": "number", "max": 999, "min": 1
                    }
                },
                "required": [
                    "userId"
                ]
            },
            "inspector": {
                "inputs": {
                    "userId": {
                        "type": "select",
                        "label": "User",
                        "tooltip": "User to list reactions for.",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/slack/list/ListUsers?outPort=out",
                            "data": {
                                "transform": "./transformers#usersToSelectArray",
                                "messages": {
                                    "in/outputType": "array",
                                    "in/isSource": true
                                }
                            }
                        }
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 2,
                        "defaultValue": "first",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or one item at a time or stream the items to a file. For large datasets, streaming the rows to a file is the most efficient method.",
                        "options": [
                            { "label": "First Found", "value": "first" },
                            { "label": "All at once", "value": "array" },
                            { "label": "One at a time", "value": "object" },
                            { "label": "File", "value": "file" }
                        ]
                    },
                    "limit": {
                        "type": "number",
                        "label": "Limit",
                        "tooltip": "The maximum number of items to return. Fewer than the requested number of items may be returned, even if the end of the list hasn't been reached. Must be an integer with a max value of 999. Default is 100.",
                        "index": 3,
                        "defaultValue": 100
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/slack/reactions/ListReactions?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType",
                        "in/userId": "1"
                    }
                }
            }
        }
    ]
}
