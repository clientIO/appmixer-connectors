{
	"name": "appmixer.slack.files.UploadFile",
	"author": "Appmixer <info@appmixer.com>",
	"description": "Upload a new file.",
	"version": "1.0.0",
	"private": false,
	"auth": {
		"service": "appmixer:slack",
		"scope": [
			"files:write"
		]
	},
	"inPorts": [
		{
			"name": "in",
			"schema": {
				"type": "object",
				"properties": {
					"channel": {
						"type": "string"
					},
					"fileId": {
						"type": "string"
					},
					"filename": {
						"type": "string"
					},
					"altTxt": {
						"type": "string"
					},
					"initialComment": {
						"type": "string"
					},
					"snippetType": {
						"type": "string"
					}
				},
				"required": [
					"channel", "fileId"
				]
			},
			"inspector": {
				"inputs": {
					"channel": {
						"type": "select",
						"group": "config",
						"label": "Channel",
						"tooltip": "The channel to post the message to.",
						"index": 1,
						"source": {
							"url": "/component/appmixer/slack/list/ListChannels?outPort=channels",
							"data": {
								"transform": "ListChannels#channelsToSelectArray"
							}
						}
					},
					"fileId": {
						"type": "filepicker",
						"index": 2,
						"label": "File ID (Appmixer)",
						"tooltip": "File ID of the file you want to upload."
					},
					"filename": {
						"type": "text",
						"index": 3,
						"label": "Filename",
						"tooltip": "Name of the file being uploaded. If omitted, the Appmixer filename will be used."
					},
					"altTxt": {
						"type": "text",
						"index": 4,
						"label": "Alternative Text",
						"tooltip": "Description of image for screen-reader. Example: 'Aerial view of the Bixby Bridge and coastline of the Big Sur area in California.'"
					},
					"initialComment": {
						"type": "text",
						"index": 5,
						"label": "Initial Comment",
						"tooltip": "The message text introducing the file in specified channel."
					},
					"snippetType": {
						"type": "text",
						"index": 6,
						"label": "Snippet Type",
						"tooltip": "Syntax type of the snippet being uploaded. Example: python."
					}
				}
			}
		}
	],
	"outPorts": [
		{
			"name": "out",
			"options": [
				{
					"label": "ID",
					"value": "id"
				},
				{
					"label": "Created",
					"value": "created"
				},
				{
					"label": "Timestamp",
					"value": "timestamp"
				},
				{
					"label": "Name",
					"value": "name"
				},
				{
					"label": "Title",
					"value": "title"
				},
				{
					"label": "MIME Type",
					"value": "mimeType"
				},
				{
					"label": "File Type",
					"value": "fileType"
				},
				{
					"label": "Pretty Type",
					"value": "pretty_type"
				},
				{
					"label": "User",
					"value": "user"
				},
				{
					"label": "User Team",
					"value": "user_team"
				},
				{
					"label": "Editable",
					"value": "editable"
				},
				{
					"label": "Size",
					"value": "size"
				},
				{
					"label": "Mode",
					"value": "mode"
				},
				{
					"label": "Is External",
					"value": "is_external"
				},
				{
					"label": "External Type",
					"value": "external_type"
				},
				{
					"label": "Is Public",
					"value": "is_public"
				},
				{
					"label": "Public URL Shared",
					"value": "public_url_shared"
				},
				{
					"label": "Display As Bot",
					"value": "display_as_bot"
				},
				{
					"label": "Username",
					"value": "username"
				},
				{
					"label": "URL Private",
					"value": "url_private"
				},
				{
					"label": "URL Private Download",
					"value": "url_private_download"
				},
				{
					"label": "Media Display Type",
					"value": "media_display_type"
				},
				{
					"label": "Permalink",
					"value": "permalink"
				},
				{
					"label": "Permalink Public",
					"value": "permalink_public"
				},
				{
					"label": "Comments Count",
					"value": "comments_count"
				},
				{
					"label": "Is Starred",
					"value": "is_starred"
				},
				{
					"label": "Shares",
					"value": "shares"
				},
				{
					"label": "Channels",
					"value": "channels"
				},
				{
					"label": "Groups",
					"value": "groups"
				},
				{
					"label": "ims",
					"value": "ims"
				},
				{
					"label": "Has More Shares",
					"value": "has_more_shares"
				},
				{
					"label": "Has Rich Preview",
					"value": "has_rich_preview"
				},
				{
					"label": "File Access",
					"value": "file_access"
				}
			]
		}
	],
	"icon": "data:image/svg+xml;base64,"
}
