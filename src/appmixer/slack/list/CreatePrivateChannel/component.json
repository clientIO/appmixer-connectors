{
    "name": "appmixer.slack.list.CreatePrivateChannel",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Create a new private channel.",
    "private": false,
    "auth": {
        "service": "appmixer:slack",
        "scope": [
            "groups:write"
        ]
    },
    "inPorts": [
        {
            "name": "channel",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 80,
                        "pattern": "^[a-z0-9-_]{1,80}$"
                    }
                },
                "required": [
                    "name"
                ]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "label": "Channel name",
                        "index": 1,
                        "tooltip": "Name of your channel. It can contain maximum of 21 characters consisting of lowercase letters, numbers, underscores and dashes."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newChannel",
            "options": [
                {
                    "label": "Channel ID",
                    "value": "id"
                },
                {
                    "label": "Channel Name",
                    "value": "name"
                },
                {
                    "label": "Is Channel",
                    "value": "is_channel"
                },
                {
                    "label": "Is Group",
                    "value": "is_group"
                },
                {
                    "label": "Created",
                    "value": "created"
                },
                {
                    "label": "Creator",
                    "value": "creator"
                },
                {
                    "label": "Is Archived",
                    "value": "is_archived"
                },
                {
                    "label": "Is General",
                    "value": "is_general"
                },
                {
                    "label": "Name Normalized",
                    "value": "name_normalized"
                },
                {
                    "label": "Is Shared",
                    "value": "is_shared"
                },
                {
                    "label": "Is Org Shared",
                    "value": "is_org_shared"
                },
                {
                    "label": "Is Ext Shared",
                    "value": "is_ext_shared"
                },
                {
                    "label": "Is Member",
                    "value": "is_member"
                },
                {
                    "label": "Is Private",
                    "value": "is_private"
                },
                {
                    "label": "Last Read",
                    "value": "last_read"
                },
                {
                    "label": "Latest",
                    "value": "latest"
                },
                {
                    "label": "Unread Count",
                    "value": "unread_count"
                },
                {
                    "label": "Unread Count Display",
                    "value": "unread_count_display"
                },
                {
                    "label": "Members",
                    "value": "members"
                },
                {
                    "label": "Topic",
                    "value": "topic"
                },
                {
                    "label": "Topic Value",
                    "value": "topic.value"
                },
                {
                    "label": "Topic Creator",
                    "value": "topic.creator"
                },
                {
                    "label": "Topic Last Set",
                    "value": "topic.last_set"
                },
                {
                    "label": "Purpose",
                    "value": "purpose"
                },
                {
                    "label": "Purpose Value",
                    "value": "purpose.value"
                },
                {
                    "label": "Purpose Creator",
                    "value": "purpose.creator"
                },
                {
                    "label": "Purpose Last Set",
                    "value": "purpose.last_set"
                },
                {
                    "label": "Previous Names",
                    "value": "previous_names"
                },
                {
                    "label": "Priority",
                    "value": "priority"
                }
            ]
        }
    ]
}