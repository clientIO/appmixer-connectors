{
    "name": "appmixer.slack.list.CreatePrivateChannel",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Create a new private channel.",
    "private": false,
    "auth": {
        "service": "appmixer:slack",
        "scope": [
            "groups:write"
        ]
    },
    "inPorts": [
        {
            "name": "channel",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 80,
                        "pattern": "^[a-z0-9-_]{1,80}$"
                    }
                },
                "required": [
                    "name"
                ]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "label": "Channel name",
                        "index": 1,
                        "tooltip": "Name of your channel. It can contain maximum of 21 characters consisting of lowercase letters, numbers, underscores and dashes."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newChannel",
            "options": [
                { "label": "id", "value": "id" },
                { "label": "name", "value": "name" },
                { "label": "is_channel", "value": "is_channel" },
                { "label": "is_group", "value": "is_group" },
                { "label": "created", "value": "created" },
                { "label": "creator", "value": "creator" },
                { "label": "is_archived", "value": "is_archived" },
                { "label": "is_general", "value": "is_general" },
                { "label": "name_normalized", "value": "name_normalized" },
                { "label": "is_shared", "value": "is_shared" },
                { "label": "is_org_shared", "value": "is_org_shared" },
                { "label": "is_ext_shared", "value": "is_ext_shared" },
                { "label": "is_member", "value": "is_member" },
                { "label": "is_private", "value": "is_private" },
                { "label": "last_read", "value": "last_read" },
                { "label": "latest", "value": "latest" },
                { "label": "unread_count", "value": "unread_count" },
                { "label": "unread_count_display", "value": "unread_count_display" },
                { "label": "members", "value": "members" },
                { "label": "topic", "value": "topic" },
                { "label": "topic.value", "value": "topic.value" },
                { "label": "topic.creator", "value": "topic.creator" },
                { "label": "topic.last_set", "value": "topic.last_set" },
                { "label": "purpose", "value": "purpose" },
                { "label": "purpose.value", "value": "purpose.value" },
                { "label": "purpose.creator", "value": "purpose.creator" },
                { "label": "purpose.last_set", "value": "purpose.last_set" },
                { "label": "previous_names", "value": "previous_names" },
                { "label": "priority", "value": "priority" }
            ]
        }
    ]
}
