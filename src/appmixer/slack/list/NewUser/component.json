{
    "name": "appmixer.slack.list.NewUser",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "This trigger fires every time a new user is created in or joins your team.",
    "private": false,
    "tick": true,
    "auth": {
        "service": "appmixer:slack",
        "scope": [
            "users:read"
        ]
    },
    "outPorts": [
        {
            "name": "user",
            "options": [
                {
                    "label": "User Id",
                    "value": "id"
                },
                {
                    "label": "Team Id",
                    "value": "team_id"
                },
                {
                    "label": "Name",
                    "value": "name"
                },
                {
                    "label": "Color",
                    "value": "color"
                },
                {
                    "label": "Real Name",
                    "value": "real_name"
                },
                {
                    "label": "Tz",
                    "value": "tz"
                },
                {
                    "label": "Tz Label",
                    "value": "tz_label"
                },
                {
                    "label": "Tz Offset",
                    "value": "tz_offset"
                },
                {
                    "label": "Is Admin",
                    "value": "is_admin"
                },
                {
                    "label": "Is Owner",
                    "value": "is_owner"
                },
                {
                    "label": "Is Primary Owner",
                    "value": "is_primary_owner"
                },
                {
                    "label": "Is Restricted",
                    "value": "is_restricted"
                },
                {
                    "label": "Is Ultra Restricted",
                    "value": "is_ultra_restricted"
                },
                {
                    "label": "Is Bot",
                    "value": "is_bot"
                },
                {
                    "label": "Is App User",
                    "value": "is_app_user"
                },
                {
                    "label": "Updated",
                    "value": "update"
                },
                {
                    "label": "Profile Email",
                    "value": "profile.email"
                },
                {
                    "label": "Profile First Name",
                    "value": "profile.first_name"
                },
                {
                    "label": "Profile Last Name",
                    "value": "profile.last_name"
                },
                {
                    "label": "Profile Real Name",
                    "value": "profile.real_name"
                },
                {
                    "label": "Profile Real Name Normalized",
                    "value": "profile.real_name_normalized"
                },
                {
                    "label": "Profile Display Name",
                    "value": "profile.display_name"
                },
                {
                    "label": "Profile Display Name Normalized",
                    "value": "profile.display_name_normalized"
                }
            ]
        }
    ]
}