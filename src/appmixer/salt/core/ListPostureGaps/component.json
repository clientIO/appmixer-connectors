{
    "name": "appmixer.salt.core.ListPostureGaps",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve a list of posture gaps from the Salt service.",
    "version": "1.1.0",
    "private": false,
    "auth": {
        "service": "appmixer:salt"
    },
    "quota": {
        "manager": "appmixer:salt",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "limit": { "type": "number" },
                    "status": {
                        "type": "array",
                        "items": { "type": "string" }
                    },
                    "severity": {
                        "type": "array",
                        "items": { "type": "string" }
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "severity": {
                        "type": "multiselect",
                        "label": "Severity",
                        "tooltip": "Filter by the severity of the posture gaps. The Severities are: Critical/High/Medium/Low/Info. Filtering by multiple severities is supported. If left blank, all severities will be fetched.",
                        "options": [
                            "Info",
                            "Low",
                            "Medium",
                            "High",
                            "Critical"
                        ],
                        "index": 0
                    },
                    "status": {
                        "type": "multiselect",
                        "label": "Status",
                        "tooltip": "Filtering posture gaps by status. The values can be: Open/InProcess/Resolved/Ignored. Filtering by multiple statuses is supported. If left blank, all statuses will be fetched.",
                        "options": [
                            "Open",
                            "InProcess",
                            "Resolved",
                            "Ignored"
                        ],
                        "index": 1
                    },
                    "limit": {
                        "type": "number",
                        "label": "Limit",
                        "tooltip": "The maximum number of records to fetch. If left blank, all records will be fetched.",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Posture Gap Id",
                    "value": "postureGapId",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Severity",
                    "value": "severity",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Categories",
                    "value": "categories",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                {
                    "label": "Posture Gap Name",
                    "value": "postureGapName",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Host",
                    "value": "host",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Method",
                    "value": "method",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Endpoint Path",
                    "value": "endpointPath",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Tickets",
                    "value": "tickets",
                    "schema": {
                        "type": "array",
                        "items": {}
                    }
                },
                {
                    "label": "Detected",
                    "value": "detected",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Last Activity",
                    "value": "lastActivity",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Endpoint Labels",
                    "value": "endpointLabels",
                    "schema": {
                        "type": "array",
                        "items": {}
                    }
                },
                {
                    "label": "Description",
                    "value": "description",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Remediation Suggestion",
                    "value": "remediationSuggestion",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Parameters",
                    "value": "parameters",
                    "schema": {
                        "type": "array",
                        "items": {}
                    }
                },
                {
                    "label": "Posture Gap Link",
                    "value": "postureGapLink",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Cwe Id",
                    "value": "cweId",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}