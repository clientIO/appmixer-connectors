{
    "version": "1.0.0",
    "name": "appmixer.smartsheet.core.GetSheet",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Get Sheet</p></label><br/><p>Gets a sheet in the format specified, based on the sheet Id.</p>",
    "private": false,
   
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "required": [
                    "sheetId"
                ],
                "properties": {
                    
                    "sheetId": {
                        "type": "number"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "sheetId": {
                        "type": "number",
                        "index": 0,
                        "label": "Sheet Id",
                        "tooltip": "<p>Sheet Id of the sheet being accessed.</p>",
                        "source": {
                            "url": "/component/appmixer/smartsheet/core/ListSheets?outPort=out",
                            "data": {
                                "transform": "./ListSheets#sheetsToSelectArray"
                            }
                        }
                    }
                }
            }
        }
    ],
