{
    "name": "appmixer.asana.projects.CreateProject",
    "author": "Tomasz Hencel <thencel@ntrc.eu>",
    "description": "This component creates a new project.",
    "private": false,
    "auth": {
        "service": "appmixer:asana"
    },
    "quota": {
        "manager": "appmixer:asana",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "project",
            "schema": {
                "type": "object",
                "properties": {
                    "workspace": { "type": "string" },
                    "team": { "type": "string" },
                    "name": { "type": "string" },
                    "note": { "type": "string" },
                    "dueDate": {
                        "oneOf": [
                            {
                                "$ref": "#/definitions/Date"
                            },
                            {
                                "$ref": "#/definitions/DateTime"
                            }
                        ]
                    },
                    "color": {
                        "enum": [
                            "null",
                            "dark-pink",
                            "dark-green",
                            "dark-blue",
                            "dark-red",
                            "dark-orange",
                            "dark-purple",
                            "dark-warm-gray",
                            "light-pink",
                            "light-green",
                            "light-blue",
                            "light-yellow",
                            "light-orange",
                            "light-purple",
                            "light-warm-gray"
                        ]
                    }
                },
                "required": [
                    "workspace"
                ],
                "definitions": {
                    "Date": {
                        "type": "string",
                        "pattern": "^[0-9]{4}-[0|1][0-9]-[0-3][0-9]$"
                    },
                    "DateTime": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "workspace": {
                        "type": "select",
                        "group": "config",
                        "label": "Workspace",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/asana/tasks/ListWorkspaces?outPort=workspaces",
                            "data": {
                                "transform": "./transformers#workspacesToSelectArray"
                            }
                        },
                        "tooltip": "Select workspace."
                    },
                    "team": {
                        "type": "select",
                        "group": "config",
                        "label": "Team",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/asana/tasks/ListTeams?outPort=teams",
                            "data": {
                                "messages": {
                                    "in/workspace": "properties/workspace"
                                },
                                "transform": "./transformers#teamsToSelectArray"
                            }
                        },
                        "tooltip": "Select team (if creating in an organization select specific team to create the project in)."
                    },
                    "name": {
                        "type": "text",
                        "group": "transformation",
                        "tooltip": "The name of the project.",
                        "label": "Name",
                        "index": 1
                    },
                    "note": {
                        "type": "textarea",
                        "group": "transformation",
                        "tooltip": "More detailed, free-form textual information associated with the project.",
                        "label": "Notes",
                        "index": 2
                    },
                    "dueDate": {
                        "type": "date-time",
                        "group": "transformation",
                        "tooltip": "The day on which this project is due.",
                        "label": "Due date",
                        "index": 3
                    },
                    "color": {
                        "type": "color-palette",
                        "options": [
                            {
                                "value": "null",
                                "content": "Null",
                                "icon": "data:image/png;base64,"
                            },
                            {
                                "value": "dark-pink",
                                "content": "#EA4E9D"
                            },
                            {
                                "value": "dark-green",
                                "content": "#62D26F"
                            },
                            {
                                "value": "dark-blue",
                                "content": "#4186E0"
                            },
                            {
                                "value": "dark-red",
                                "content": "#E8384F"
                            },
                            {
                                "value": "dark-orange",
                                "content": "#FD612C"
                            },
                            {
                                "value": "dark-purple",
                                "content": "#7A6FF0"
                            },
                            {
                                "value": "dark-warm-gray",
                                "content": "#8DA3A6"
                            },
                            {
                                "value": "light-pink",
                                "content": "#E362E3"
                            },
                            {
                                "value": "light-green",
                                "content": "#A4CF30"
                            },
                            {
                                "value": "light-blue",
                                "content": "#4186E0"
                            },
                            {
                                "value": "light-yellow",
                                "content": "#EEC300"
                            },
                            {
                                "value": "light-orange",
                                "content": "#FD9A00"
                            },
                            {
                                "value": "light-purple",
                                "content": "#AA62E3"
                            },
                            {
                                "value": "light-warm-gray",
                                "content": "#8DA3A6"
                            }
                        ],
                        "defaultValue": "null",
                        "group": "transformation",
                        "label": "The color of the project.",
                        "index": 4
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newProject",
            "options": [
                { "label": "gid", "value": "gid" },
                { "label": "created_at", "value": "created_at" },
                { "label": "modified_at", "value": "modified_at" },
                { "label": "due_date", "value": "due_on" },
                { "label": "current_status", "value": "current_status" },
                { "label": "public", "value": "public" },
                { "label": "name", "value": "name" },
                { "label": "notes", "value": "notes" },
                { "label": "archived", "value": "archived" },
                { "label": "workspace", "value": "workspace" },
                { "label": "workspace.gid", "value": "workspace.gid" },
                { "label": "workspace.name", "value": "workspace.name" },
                { "label": "owner", "value": "owner" },
                { "label": "owner.gid", "value": "owner.gid" },
                { "label": "owner.name", "value": "owner.name" },
                { "label": "followers", "value": "followers" },
                { "label": "color", "value": "color" },
                { "label": "members", "value": "members" }
            ]
        }
    ],
    "properties": {
        "schema": {
            "properties": {
                "workspace": { "type": "string" }
            },
            "required": [
                "workspace"
            ]
        },
        "inspector": {
            "inputs": {
                
            },
            "groups": {
                "config": {
                    "label": "Configuration",
                    "index": 1
                }
            }
        }
    },
    "icon": "data:image/svg+xml;base64,"
}
