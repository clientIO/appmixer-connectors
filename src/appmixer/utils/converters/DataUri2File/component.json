{
    "name": "appmixer.utils.converters.DataUri2File",
    "author": "David Durman <david@client.io>",
    "description": "Transform a Data URI to file.",
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "properties": {
                    "dataUri": { "type": "string" },
                    "fileName": { "type": "string" }
                },
                "required": ["dataUri"]
            },
            "inspector": {
                "inputs": {
                    "dataUri": {
                        "type": "textarea",
                        "label": "Data URI",
                        "index": 1,
                        "tooltip": "A <a target=_blank href=\"https://en.wikipedia.org/wiki/Data_URI_scheme\">Data URI</a> representation of a file."
                    },
                    "fileName": {
                        "type": "text",
                        "label": "File Name",
                        "index": 2,
                        "tooltip": "Enter a file name. If you don't include a file extension, one will be automatically added based on the content type of the Data URI. If you leave the file name blank completely, the resulting file name will be 'result.[GUESSED_EXTENSION]'."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "File ID", "value": "fileId" },
                { "label": "File Name", "value": "fileName" }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
