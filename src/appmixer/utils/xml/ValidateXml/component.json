{
    "name": "appmixer.utils.xml.ValidateXml",
    "author": "David Durman <david@client.io>",
    "description": "Validate XML.",
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "xml": { "type": "string" },
                    "allowBooleanAttributes": { "type": "boolean" },
                    "unpairedTags": { "type": "string" }
                }
            },
            "inspector": {
                "inputs": {
                    "xml": {
                        "type": "textarea",
                        "label": "XML",
                        "index": 1,
                        "tooltip": "XML to be validated."
                    },
                    "allowBooleanAttributes": {
                        "type": "toggle",
                        "label": "Allow Boolean Attributes",
                        "defaultValue": false,
                        "index": 2,
                        "tooltip": "Set if a tag can have boolean attributes."
                    },
                    "unpairedTags": {
                        "type": "text",
                        "label": "Unpaired Tags",
                        "index": 3,
                        "tooltip": "Unpaired Tags are the tags which don't have a matching closing tag. Eg <br> in HTML. Parse unpaired tags by providing their comma separated list."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Is Valid", "value": "isValid" },
                { "label": "Error Code", "value": "err.code" },
                { "label": "Error Message", "value": "err.msg" },
                { "label": "Error Line Number", "value": "err.line" },
                { "label": "Error Column Number", "value": "err.col" }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
