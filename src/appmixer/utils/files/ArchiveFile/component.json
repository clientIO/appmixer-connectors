{
    "name": "appmixer.utils.files.ArchiveFile",
    "author": "Zbynek Pelunek <zbynek.pelunek@client.io>",
    "description": "Archive file from File ID.",
    "version": "1.0.0",
    "private": false,
    "quota": {
        "manager": "appmixer:utils:files",
        "resources": "storeFile",
        "maxWait": "120000"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "fileId": {
                        "type": "string"
                    }
                },
                "required": [
                    "fileId"
                ]
            },
            "inspector": {
                "inputs": {
                    "fileId": {
                        "type": "text",
                        "index": 1,
                        "label": "File ID"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "File ID",
                    "value": "fileId"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
