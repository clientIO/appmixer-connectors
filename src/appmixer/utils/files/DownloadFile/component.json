{
    "name": "appmixer.utils.files.DownloadFile",
    "author": "Harsha Surisetty <harsha@client.io>",
    "description": "Download file from link.",
    "version": "1.0.0",
    "private": false,
    "quota": {
        "manager": "appmixer:utils:files",
        "resources": "storeFile",
        "maxWait": "120000"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "link": {
                        "type": "string"
                    },
                    "customFileName": {
                        "type": "string"
                    }
                },
                "required": [
                   "link"
                ]
            },
            "inspector": {
                "inputs": {
                    "link": {
                        "type": "text",
                        "index": 1,
                        "label": "File link",
                        "tooltip": "Downloadble file link."
                    },
                    "customFileName": {
                        "type": "text",
                        "index": 2,
                        "label": "Custom File name",
                        "tooltip": "Provide a custom name for the downloaded file, including its extension (e.g., file.png)."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "File ID",
                    "value": "fileId"
                },
                {
                    "label": "File Name",
                    "value": "filename"
                },
                {
                    "label": "Content Type",
                    "value": "contentType"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
