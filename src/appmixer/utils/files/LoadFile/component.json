{
    "name": "appmixer.utils.files.LoadFile",
    "author": "Martin Krčmář <martin@client.io>",
    "description": "Read file and send content to output port.",
    "version": "1.1.0",
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "properties": {
                    "fileId": {
                        "type": "string"
                    }
                },
                "required": [
                    "fileId"
                ]
            },
            "inspector": {
                "inputs": {
                    "fileId": {
                        "type": "filepicker",
                        "label": "File ID",
                        "index": 1,
                        "tooltip": "ID of the file."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "content",
            "options": [
                {
                    "label": "content",
                    "value": "content"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
