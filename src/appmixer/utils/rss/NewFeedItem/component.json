{
    "name": "appmixer.utils.rss.NewFeedItem",
    "author": "David Durman <david@client.io>",
    "description": "Triger on new RSS feed item.",
    "tick": true,
    "properties": {
        "schema": {
            "properties": {
                "url": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "required": [
                "url"
            ]
        },
        "inspector": {
            "inputs": {
                "url": {
                    "type": "text",
                    "label": "URL",
                    "index": 1,
                    "tooltip": "A URL to a RSS feed. Example <code>https://www.nasa.gov/rss/dyn/breaking_news.rss</code>."
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Title",
                    "value": "title"
                },
                {
                    "label": "Link",
                    "value": "link"
                },
                {
                    "label": "Publication Date",
                    "value": "pubDate"
                },
                {
                    "label": "Content",
                    "value": "content"
                },
                {
                    "label": "Content Snippet",
                    "value": "contentSnippet"
                },
                {
                    "label": "GUID",
                    "value": "guid"
                },
                {
                    "label": "Categories",
                    "value": "categories"
                },
                {
                    "label": "ISO Date",
                    "value": "isoDate"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
