{
    "name": "appmixer.utils.ai.DescribeImages",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Answer questions about the content of one or more images.",
    "inPorts": [{
        "name": "in",
        "schema": {
            "type": "object",
            "properties": {
                "prompt": { "type": "string" },
                "images": {}
            },
            "required": ["prompt"]
        },
        "inspector": {
            "inputs": {
                "prompt": {
                    "label": "Prompt",
                    "type": "textarea",
                    "index": 1,
                    "tooltip": "The question to answer about the content of one or more images."
                },
                "images": {
                    "label": "Images",
                    "type": "expression",
                    "index": 2,
                    "levels": ["ADD"],
                    "fields": {
                        "fileId": {
                            "type": "filepicker",
                            "label": "File ID",
                            "tooltip": "The image file to attach. Only PNG, JPEG, non-animated GIF and WEBP image types are supported.",
                            "index": 1
                        }
                    }
                }
            }
        }
    }],
    "outPorts": [{
        "name": "out",
        "options": [{
            "label": "Answer",
            "value": "answer",
            "schema": { "type": "string" }
        }, {
            "label": "Prompt",
            "value": "prompt",
            "schema": { "type": "string" }
        }]
    }],
    "icon": "data:image/png;base64,"
}
