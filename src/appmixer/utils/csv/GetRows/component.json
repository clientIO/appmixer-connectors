{
    "name": "appmixer.utils.csv.GetRows",
    "author": "Camilo Manrique <camilo@client.io>",
    "description": "Get rows from a CSV file.",
    "version": "1.0.1",
    "dependencies": {
        "module": "1.0.1"
    },
    "private": false,
    "properties": {
        "schema": {
            "properties": {
                "withHeaders": {
                    "type": "boolean"
                },
                "filterRows": {
                    "type": "boolean"
                }
            },
            "required": [
                "withHeaders",
                "filterRows"
            ]
        },
        "inspector": {
            "inputs": {
                "withHeaders": {
                    "type": "toggle",
                    "label": "Use column names",
                    "index": 1,
                    "defaultValue": true,
                    "attrs": {
                        "label": {
                            "data-tooltip": "Set to true if the first row represents column names (CSV header) and you want to use them to identify the columns."
                        }
                    }
                },
                "filterRows": {
                    "type": "toggle",
                    "label": "Filter rows",
                    "index": 2,
                    "defaultValue": true,
                    "tooltip": "Set to true if you want to define filters to return only rows that match your filter criteria."
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object"
            },
            "source": {
                "url": "/component/appmixer/utils/csv/CreateInspector?outPort=out",
                "data": {
                    "properties": {
                        "withHeaders": "properties/withHeaders",
                        "filterRows": "properties/filterRows"
                    },
                    "transform": "./CreateInspector#getRowsInspector"
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "fileId",
                    "value": "fileId"
                },
                {
                    "label": "rows",
                    "value": "rows"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
