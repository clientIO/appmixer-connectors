{
    "name": "appmixer.utils.filters.NotEqual",
    "author": "Tomáš Waldauf <tomas@client.io>",
    "description": "A message passes through this filter if a field from a previous connector contains a value that does not equal the value specified. Values can be both numbers or dates.",
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "properties": {
                    "sourceData": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "sourceData": {
                        "type": "text",
                        "label": "Data",
                        "index": 1,
                        "tooltip": "The data that we want to check. You can select data from any previous connector via the select box."
                    },
                    "value": {
                        "type": "text",
                        "label": "Does not equal",
                        "index": 2,
                        "tooltip": "Any text. If the data under <i>Data</i> from the previous connector does not equal the value specified, the filter passes the message through. Otherwise, it ignores the message."
                    }
                }
            }
        }
    ],
    "outPorts": [ "notEquals", "equals" ],
    "icon": "data:image/svg+xml;base64,"
}
