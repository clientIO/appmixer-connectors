{
    "name": "appmixer.utils.storage.Clear",
    "description": "Clear Storage.",
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "storeId": { "type": "string" }
                },
                "required": [ "storeId" ]
            },
            "inspector": {
                "inputs": {
                    "storeId": {
                        "type": "select",
                        "group": "transformation",
                        "label": "Storage",
                        "index": 1,
                        "source": {

                            "url": "/component/appmixer/utils/storage/ListStores?outPort=out",
                            "data": {
                                "transform": "./ListStores#toSelectArray"
                            }
                        },
                        "tooltip": "Select id of storage to be cleared."
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "cleared",
            "options": [
                { "label": "Deleted count", "value": "deletedCount" },
                { "label": "Store Id", "value": "storeId" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
