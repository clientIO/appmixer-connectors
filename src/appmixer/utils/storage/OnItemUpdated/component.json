{
    "name": "appmixer.utils.storage.OnItemUpdated",
    "author": "Tomas Waldauf <tomas@client.io>",
    "description": "This trigger fires any time item has changed.",
    "webhook": true,
    "private": false,
    "properties": {
        "schema": {
            "properties": {
                "storeId": {
                    "type": "string"
                }
            },
            "required": ["storeId"]
        },
        "inspector": {
            "inputs": {
                "storeId": {
                    "type": "select",
                    "label": "Storage",
                    "index": 1,
                    "source": {

                        "url": "/component/appmixer/utils/storage/ListStores?outPort=out",
                        "data": {
                            "transform": "./ListStores#toSelectArray"
                        }
                    },
                    "tooltip": "Select storage."
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "item",
            "options": [
                { "label": "Key", "value": "key" },
                { "label": "Store Id", "value": "storeId" },
                { "label": "Value", "value": "value" },
                { "label": "Old value", "value": "oldValue" },
                { "label": "Updated at", "value": "updatedAt" },
                { "label": "Created at", "value": "createdAt" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
