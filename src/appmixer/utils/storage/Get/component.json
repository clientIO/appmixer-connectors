{
    "name": "appmixer.utils.storage.Get",
    "description": "Get value from Storage.",
    "properties": {
        "schema": {
            "properties": {
                "storeId": {
                    "type": "string"
                }
            },
            "required": ["storeId"]
        },
        "inspector": {
            "inputs": {
                "storeId": {
                    "type": "select",
                    "label": "Storage",
                    "index": 1,
                    "source": {

                        "url": "/component/appmixer/utils/storage/ListStores?outPort=out",
                        "data": {
                            "transform": "./ListStores#toSelectArray"
                        }
                    },
                    "tooltip": "Select storage."
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "key": { "type": "string" }
                },
                "required": [ "key" ]
            },
            "inspector": {
                "inputs": {
                    "key": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Key",
                        "index": 1,
                        "attrs": {
                            "label": {
                                "data-tooltip": "Key which will be used to find the value."
                            }
                        }
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Key", "value": "key" },
                { "label": "Value", "value": "value" },
                { "label": "Created at", "value": "createdAt" },
                { "label": "Updated at", "value": "updatedAt" }
            ]
        },
        {
            "name": "notFound",
            "options": [
                { "label": "Key", "value": "key" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
