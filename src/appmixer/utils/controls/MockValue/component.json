{
    "name": "appmixer.utils.controls.MockValue",
    "author": "Camilo Manrique <camilo@client.io>",
    "description": "Simple component to put a string value on the out port. Intended only for testing",
    "icon": "data:image/svg+xml;base64,",
    "private": true,
    "inPorts": [
        {
            "name": "in",
            "maxConnections": 1,
            "schema": {
                "type": "object",
                "properties": {
                    "value": { "type": "string" }
                },
                "required": [
                    "value"
                ]
            },
            "inspector": {
                "inputs": {
                    "value": {
                        "type": "textarea",
                        "group": "transformation",
                        "label": "Value",
                        "tooltip": "Mock value",
                        "index": 1
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "schema": {
                "properties": {
                    "outvalue": {
                        "type": "string"
                    }
                }
            },
            "options": [
                { "label": "Output value", "value": "outvalue" }
            ]
        }
    ]
}
