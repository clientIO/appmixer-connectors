{
    "name": "appmixer.utils.subflows.OnFlowCall",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Triggered when this flow is called with the 'Call Flow' utility.",
    "private": false,
    "properties": {
        "schema": {
            "type": "object",
            "properties": {
                "input": { "type": "object" }
            }
        },
        "inspector": {
            "inputs": {
                "input": {
                    "type": "expression",
                    "label": "Input Fields",
                    "index": 1,
                    "tooltip": "Define the expected input fields. These input fields will be populated with values at the time the 'Call Flow' utility is actually called in a running flow.",
                    "levels": ["ADD"],
                    "fields": {
                        "name": {
                            "type": "text",
                            "label": "Name"
                        },
                        "type": {
                            "type": "select",
                            "label": "Type",
                            "options": [
                                { "value": "string", "label": "String" },
                                { "value": "number", "label": "Number" },
                                { "value": "boolean", "label": "Boolean" },
                                { "value": "object", "label": "Object" },
                                { "value": "array", "label": "Array" }
                            ]
                        }
                    },
                    "source": {
                        "url": "/component/appmixer/utils/subflows/OnFlowCall?outPort=out",
                        "data": {
                            "properties": {
                                "triggerUpdate": true,
                                "input": "properties/input"
                            }
                        }
                    }
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/utils/subflows/OnFlowCall?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true,
                        "input": "properties/input"
                    }
                }
            }
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
