{
    "name": "appmixer.utils.subflows.CallFlow",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Trigger another flow. Note that the other flow must use the 'On Call Flow' utility for this to work.",
    "icon": "data:image/svg+xml;base64,",
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object"
            },
            "source": {
                "url": "/component/appmixer/utils/subflows/CallFlow?outPort=out",
                "data": {
                    "transform": "./CallFlow#generateInspector",
                    "properties": {
                        "generateInspector": true
                    },
                    "messages": {
                        "in/flowId": "inputs/in/flowId",
                        "in/componentId": "inputs/in/componentId"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/utils/subflows/CallFlow?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/flowId": "inputs/in/flowId",
                        "in/componentId": "inputs/in/componentId"
                    }
                }
            }
        }
    ]
}