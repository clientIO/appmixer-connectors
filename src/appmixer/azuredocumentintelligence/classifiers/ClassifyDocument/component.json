{
    "name": "appmixer.azuredocumentintelligence.classifiers.ClassifyDocument",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Classifies document with document classifier. Use one of the following input options: <ul><li>1. <b>Appmixer File ID</b> of the document to classify.</li> <li>2. <b>URL</b> of the document to classify.</li> <li>3. <b>Base64</b> encoded source of the document to classify.</li>",
    "label": "Classify Document",
    "auth": {
        "service": "appmixer:azuredocumentintelligence"
    },
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "classifierId": {
                        "type": "string"
                    },
                    "fileId": {
                        "type": "string"
                    },
                    "fileUrl": {
                        "type": "string"
                    },
                    "base64Source": {
                        "type": "string"
                    }
                },
                "oneOf": [
                    { "required": ["classifierId", "fileId"] },
                    { "required": ["classifierId", "fileUrl"] },
                    { "required": ["classifierId", "base64Source"] }
                ]
            },
            "inspector": {
                "inputs": {
                    "classifierId": {
                        "type": "select",
                        "label": "Classifier ID",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/azuredocumentintelligence/classifiers/ListClassifiers?outPort=out",
                            "data": {
                                "transform": "./ListClassifiers#toSelectArray"
                            }
                        }
                    },
                    "fileId": {
                        "type": "filepicker",
                        "index": 2,
                        "label": "File ID",
                        "tooltip": "File ID of the document to classify."
                    },
                    "fileUrl": {
                        "type": "text",
                        "index": 3,
                        "label": "File URL",
                        "tooltip": "URL of the document to classify."
                    },
                    "base64Source": {
                        "type": "textarea",
                        "index": 4,
                        "label": "Base64 Source",
                        "tooltip": "Base64 encoded source of the document to classify."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Api Version",
                    "value": "apiVersion"
                },
                {
                    "label": "Model Id",
                    "value": "modelId"
                },
                {
                    "label": "String Index Type",
                    "value": "stringIndexType"
                },
                {
                    "label": "Content",
                    "value": "content"
                },
                {
                    "label": "Pages",
                    "value": "pages",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "pageNumber": {
                                    "label": "Page Number",
                                    "value": "pageNumber"
                                },
                                "angle": {
                                    "label": "Angle",
                                    "value": "angle"
                                },
                                "width": {
                                    "label": "Width",
                                    "value": "width"
                                },
                                "height": {
                                    "label": "Height",
                                    "value": "height"
                                },
                                "unit": {
                                    "label": "Unit",
                                    "value": "unit"
                                },
                                "words": {
                                    "label": "Words",
                                    "value": "words",
                                    "schema": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {}
                                        }
                                    }
                                },
                                "lines": {
                                    "label": "Lines",
                                    "value": "lines",
                                    "schema": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {}
                                        }
                                    }
                                },
                                "spans": {
                                    "label": "Spans",
                                    "value": "spans",
                                    "schema": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "offset": {
                                                    "label": "Offset",
                                                    "value": "offset"
                                                },
                                                "length": {
                                                    "label": "Length",
                                                    "value": "length"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Documents",
                    "value": "documents",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "docType": {
                                    "label": "docType",
                                    "value": "docType"
                                },
                                "boundingRegions": {
                                    "label": "boundingRegions",
                                    "value": "boundingRegions",
                                    "schema": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "pageNumber": {
                                                    "label": "pageNumber",
                                                    "value": "pageNumber"
                                                },
                                                "polygon": {
                                                    "label": "polygon",
                                                    "value": "polygon",
                                                    "schema": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "object",
                                                            "properties": {}
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                },
                                "confidence": {
                                    "label": "confidence",
                                    "value": "confidence"
                                },
                                "spans": {
                                    "label": "spans",
                                    "value": "spans",
                                    "schema": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "offset": {
                                                    "label": "offset",
                                                    "value": "offset"
                                                },
                                                "length": {
                                                    "label": "length",
                                                    "value": "length"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Content Format",
                    "value": "contentFormat"
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
