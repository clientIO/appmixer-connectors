{
    "name": "appmixer.trello.checklist.UpdateChecklist",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Update an existing checklist.",
    "private": false,
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:trello"
    },
    "quota": {
        "manager": "appmixer:trello",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "boardId": { "type": "string" },
                    "boardListCardId": { "type": "string" },
                    "checklistId": { "type": "string" },
                    "name": { "type": "string", "maxLength": 16384 }
                },
                "required": [ "checklistId", "name" ]
            },
            "inspector": {
                "inputs": {
                    "boardId": {
                        "type": "select",
                        "label": "Board",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoards?outPort=boards",
                            "data": {
                                "transform": "./transformers#boardsToSelectArray"
                            }
                        },
                        "tooltip": "Select a board to choose a card in the following field. Or use the Checklist ID directly and skip this field."
                    },
                    "boardListCardId": {
                        "type": "select",
                        "label": "Card",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardsCards?outPort=cards",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId",
                                    "in/isSource": true
                                },
                                "transform": "./transformers#cardListsToSelectArray"
                            }
                        },
                        "tooltip": "Select a card from a board from the previous component. Or use the Checklist ID directly and skip this field."
                    },
                    "checklistId": {
                        "type": "select",
                        "label": "Checklist",
                        "source": {
                            "url": "/component/appmixer/trello/checklist/ListCardChecklists?outPort=out",
                            "data": {
                                "messages": {
                                    "in/boardListCardId": "inputs/in/boardListCardId",
                                    "in/isSource": true
                                },
                                "transform": "./transformers#cardChecklistsToSelectArray"
                            }
                        },
                        "tooltip": "The ID of the checklist to update.",
                        "index": 3
                    },
                    "name": {
                        "type": "text",
                        "label": "Checklist Name",
                        "index": 4,
                        "tooltip": "Name of the new checklist being created."
                    }
                }
            }
        }
    ],
    "outPorts": [
    {
            "name": "out",
            "options": [
                { "label": "Checklist ID", "value": "idChecklist" },
                { "label": "Card ID", "value": "idCard" },
                { "label": "Board ID", "value": "idBoard" },
                { "label": "Checklist Name", "value": "name" },
                { "label": "Position", "value": "pos" },
                { "label": "Checklist Items", "value": "items" }
            ]
        }
    ]
}
