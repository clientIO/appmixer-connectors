{
    "name": "appmixer.trello.list.AddLabelToCard",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Add an existing label to a specific card.",
    "private": false,
    "version": "1.0.2",
    "auth": {
        "service": "appmixer:trello"
    },
    "quota": {
        "manager": "appmixer:trello",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "boardId": { "type": "string" },
                    "boardListCardId": { "type": "string" },
                    "labelId": { "type": "string" },
                    "labelColor": {
                        "enum": [
                            "none",
                            "green",
                            "yellow",
                            "orange",
                            "red",
                            "purple",
                            "blue",
                            "turquoise",
                            "light green",
                            "pink",
                            "black"
                        ]
                    },
                    "labelName": { "type": "string", "maxLength": 16384 }
                },
                "oneOf": [
                    { "required": [ "boardListCardId", "labelId" ] },
                    { "required": [ "boardListCardId", "labelColor", "labelName"] }
                ]
            },
            "inspector": {
                "inputs": {
                    "boardId": {
                        "type": "select",
                        "label": "Board",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoards?outPort=boards",
                            "data": {
                                "transform": "./transformers#boardsToSelectArray"
                            }
                        },
                        "tooltip": "Select a board."
                    },
                    "boardListCardId": {
                        "type": "select",
                        "label": "Card",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardsCards?outPort=cards",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId",
                                    "in/isSource": true
                                },
                                "transform": "./transformers#cardListsToSelectArray"
                            }
                        },
                        "tooltip": "Select a card or card ID from previous component."
                    },
                    "labelId": {
                        "type": "select",
                        "label": "Label",
                        "index": 3,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardLabels?outPort=labels",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId",
                                    "in/isSource": true
                                },
                                "transform": "./transformers#cardLabelsToSelectArray"
                            }
                        },
                        "tooltip": "Select a label or label ID from previous component or create new label with color and name using the two inputs below."
                    },
                    "labelColor": {
                        "type": "color-palette",
                        "options": [
                            {
                                "value": "none",
                                "content": "None",
                                "icon": "data:image/png;base64,"
                            },
                            { "value": "green", "content": "Green" },
                            { "value": "yellow", "content": "Yellow" },
                            { "value": "orange", "content": "Orange" },
                            { "value": "red", "content": "Red" },
                            { "value": "purple", "content": "Purple" },
                            { "value": "blue", "content": "Blue" },
                            { "value": "turquoise", "content": "Turquoise" },
                            { "value": "light green", "content": "LightGreen" },
                            { "value": "pink", "content": "Pink" },
                            { "value": "black", "content": "Black" }
                        ],
                        "defaultValue": "none",
                        "label": "Label color",
                        "index": 4
                    },
                    "labelName": {
                        "type": "text",
                        "label": "Label name",
                        "index": 5
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "label",
            "options": [
                { "label": "idLabel", "value": "id" },
                { "label": "idCard", "value": "idCard" },
                { "label": "idBoard", "value": "idBoard" },
                { "label": "name", "value": "name" },
                { "label": "color", "value": "color" },
                { "label": "uses", "value": "uses" }
            ]
        }
    ]
}
