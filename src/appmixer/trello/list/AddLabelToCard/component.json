{
    "name": "appmixer.trello.list.AddLabelToCard",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Add an existing label to a specific card or create a new label by providing a name and color.",
    "private": false,
    "version": "1.0.2",
    "auth": {
        "service": "appmixer:trello"
    },
    "quota": {
        "manager": "appmixer:trello",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "boardId": { "type": "string" },
                    "boardListCardId": { "type": "string" },
                    "labelId": { "type": "string" },
                    "labelColor": {
                        "enum": [
                            "none",
                            "green",
                            "yellow",
                            "orange",
                            "red",
                            "purple",
                            "blue",
                            "#006988",
                            "lime",
                            "pink",
                            "black"
                        ]
                    },
                    "labelName": { "type": "string", "maxLength": 16384 }
                },
                "anyOf": [
                    {
                        "required": ["labelId"]
                    },
                    {
                        "required": ["labelColor", "labelName"]
                    }
                ]
            },
            "inspector": {
                "inputs": {
                    "boardId": {
                        "type": "select",
                        "label": "Board",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoards?outPort=boards",
                            "data": {
                                "transform": "./transformers#boardsToSelectArray"
                            }
                        },
                        "tooltip": "Select a board. You can skip this step if you provide a Card ID dynamically in the next input."
                    },
                    "boardListCardId": {
                        "type": "select",
                        "label": "Card",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardsCards?outPort=cards",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId",
                                    "in/isSource": true
                                },
                                "transform": "./transformers#cardListsToSelectArray"
                            }
                        },
                        "tooltip": "Select a card (make sure you selected a Board in the previous step as well). Alternatively, you can provide a Card ID directly in a variable."
                    },
                    "labelId": {
                        "type": "select",
                        "label": "Select Existing Label",
                        "index": 3,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardLabels?outPort=labels",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId",
                                    "in/isSource": true
                                },
                                "transform": "./transformers#cardLabelsToSelectArray"
                            }
                        },
                        "tooltip": "Enter a Label ID or select an existing label from the list. If you want to create a new label instead, leave this field empty and fill in 'Label Name' and 'Label Color' below."
                    },
                    "labelColor": {
                        "type": "color-palette",
                        "options": [
                            {
                                "value": "none",
                                "content": "None",
                                "icon": "data:image/png;base64,"
                            },
                            { "value": "green", "content": "Green" },
                            { "value": "yellow", "content": "Yellow" },
                            { "value": "orange", "content": "Orange" },
                            { "value": "red", "content": "Red" },
                            { "value": "purple", "content": "Purple" },
                            { "value": "blue", "content": "Blue" },
                            { "value": "#006988", "content": "Sky" },
                            { "value": "lime", "content": "Lime" },
                            { "value": "pink", "content": "Pink" },
                            { "value": "black", "content": "Black" }
                        ],
                        "defaultValue": "none",
                        "label": "Label Color",
                        "tooltip": "Choose a color for the new label. Only used if you're creating a new label. Leave empty if you're selecting an existing label.",
                        "index": 4
                    },
                    "labelName": {
                        "type": "text",
                        "label": "Label Name",
                        "tooltip": "Enter a name for the new label. Only used if you're creating a new label. Leave empty if you're selecting an existing label.",
                        "index": 5
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "label",
            "options": [
                { "label": "Label ID", "value": "id" },
                { "label": "Card ID", "value": "idCard" },
                { "label": "Board ID", "value": "idBoard" },
                { "label": "Name", "value": "name" },
                { "label": "Color", "value": "color" },
                { "label": "Uses", "value": "uses" }
            ]
        }
    ]
}
