{
    "name": "appmixer.trello.list.RemoveLabelFromCard",
    "author": "Pawel Putko <pputko@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Remove existing label from a specific card.",
    "private": false,
    "version": "1.0.2",
    "auth": {
        "service": "appmixer:trello"
    },
    "quota": {
        "manager": "appmixer:trello",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "boardId": { "type": "string" },
                    "boardListCardId": { "type": "string" },
                    "labelId": { "type": "string" }
                },
                "required": [
                    "labelId",
                    "boardListCardId"
                ]
            },
            "inspector": {
                "inputs": {
                    "boardId": {
                        "type": "select",
                        "label": "Board",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoards?outPort=boards",
                            "data": {
                                "transform": "./transformers#boardsToSelectArray"
                            }
                        },
                        "tooltip": "Select a board."
                    },
                    "boardListCardId": {
                        "type": "select",
                        "label": "Card",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardsCards?outPort=cards",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId"
                                },
                                "transform": "./transformers#cardListsToSelectArray"
                            }
                        },
                        "tooltip": "Select a card."
                    },
                    "labelId": {
                        "type": "select",
                        "label": "Label",
                        "index": 3,
                        "source": {
                            "url": "/component/appmixer/trello/list/ListBoardLabels?outPort=labels",
                            "data": {
                                "messages": {
                                    "in/boardId": "inputs/in/boardId"
                                },
                                "transform": "./transformers#cardLabelsToSelectArray"
                            }
                        },
                        "tooltip": "Select a label or label ID from previous component."
                    }
                }
            }
        }
    ],
    "outPorts": [{ "name": "out" }]
}
