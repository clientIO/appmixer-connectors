{
    "name": "appmixer.postgres.db.UpdateRow",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update an existing row in PostgreSQL database table.",
    "private": false,
    "auth": {
        "service": "appmixer:postgres"
    },
    "properties": {
        "schema": {
            "properties": {
                "table": {
                    "type": "string"
                }
            },
            "required": [
                "table"
            ]
        },
        "inspector": {
            "inputs": {
                "table": {
                    "type": "select",
                    "label": "Table",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/postgres/db/ListTables?outPort=tables",
                        "data": {
                            "transform": "./ListTables#toSelectArray"
                        }
                    }
                }
            }
        }
    },
    "inPorts": [
        {
        "name": "row",
        "inspector": {
            "inputs": {
                "filter": {
                    "type": "expression",
                    "label": "Filter",
                    "tooltip": "Specify a WHERE condition.",
                    "index": 1,
                    "levels": ["AND", "OR"],
                    "fields": {
                        "column": {
                            "type": "text",
                            "label": "Column",
                            "index": 1,
                            "source": {
                                "url": "/component/appmixer/postgres/db/ListColumns?outPort=columns",
                                "data": {
                                    "properties": {
                                        "table": "properties/table"
                                    },
                                    "transform": "./ListColumns#columnsToInspector"
                                }
                            }
                        },
                        "operator": {
                            "type": "select",
                            "label": "Operator",
                            "defaultValue": "=",
                            "index": 2,
                            "options": [
                                { "label": "Equal", "value": "=" },
                                { "label": "Greater than", "value": ">" },
                                { "label": "Less than", "value": "<" },
                                { "label": "Greater than or equal", "value": ">=" },
                                { "label": "Less than or equal", "value": "<=" },
                                { "label": "Not equal", "value": "<>" },
                                { "label": "Contains", "value": "CONTAINS" },
                                { "label": "Does not contain", "value": "NOT CONTAINS" },
                                { "label": "Starts with", "value": "STARTS WITH" },
                                { "label": "Does not start with", "value": "NOT STARTS WITH" },
                                { "label": "Ends with", "value": "ENDS WITH" },
                                { "label": "Does not end with", "value": "NOT ENDS WITH" },
                                { "label": "Matches any value in a list", "value": "IN" },
                                { "label": "Matches any value not in a list", "value": "NOT IN" },
                                { "label": "Value is between a range of values", "value": "BETWEEN" },
                                { "label": "Value is not between a range of values", "value": "NOT BETWEEN" },
                                { "label": "Matches a pattern", "value": "LIKE" },
                                { "label": "Does not matche a pattern", "value": "NOT LIKE" },
                                { "label": "Is Null", "value": "IS NULL" },
                                { "label": "Is Not Null", "value": "IS NOT NULL" }
                            ]
                        },
                        "value": {
                            "type": "text",
                            "label": "Value",
                            "index": 3,
                            "when": {
                                "in": {
                                    "./operator": [
                                        "=", ">", "<", ">=", "<=", "<>",
                                        "CONTAINS", "NOT CONTAINS", "STARTS WITH", "NOT STARTS WITH", "ENDS WITH", "NOT ENDS WITH",
                                        "IN", "NOT IN", "BETWEEN", "NOT BETWEEN", "LIKE", "NOT LIKE"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "source": {
            "url": "/component/appmixer/postgres/db/ListColumns?outPort=columns",
            "data": {
                "properties": {
                    "table": "properties/table"
                },
                "transform": "./ListColumns#columnsToInspector"
            }
        }
    }
    ],
    "outPorts": [
        {
        "name": "out",
        "options": [
            { "label": "Row Count", "value": "rowCount", "schema": { "type": "integer" } }
        ]
    }
    ],
    "icon": "data:image/png;base64,"
}

