{
    "name": "appmixer.pipedrive.crm.UpdateNote",
    "author": "Pawel Putko <pputko@ntrc.eu>",
    "description": "This action updates existing note to Pipedrive.",
    "inPorts": [
        {
            "name": "note",
            "schema": {
                "type": "object",
                "properties": {
                    "id": { "type": "number" },
                    "note": { "type": "string" },
                    "deal_id": { "type": "number" },
                    "org_id": { "type": "number" },
                    "person_id": { "type": "number" },
                    "pinned_to_deal_flag": {},
                    "pinned_to_organization_flag": {},
                    "pinned_to_person_flag": {}
                },
                "required": [
                    "id",
                    "note"
                ]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "group": "transformation",
                        "index": 1,
                        "label": "Note id",
                        "attrs": {
                            "label": { "data-tooltip": "Note id." }
                        },
                        "source": {
                            "url": "/component/appmixer/pipedrive/crm/ListNotes?outPort=out",
                            "data": {
                                "transform": "./transformers#notesToSelectArray"
                            }
                        }
                    },
                    "note": {
                        "type": "text",
                        "group": "transformation",
                        "index": 1,
                        "label": "Content",
                        "attrs": {
                            "label": { "data-tooltip": "Content of the note." }
                        }
                    },
                    "deal_id": {
                        "type": "text",
                        "group": "transformation",
                        "index": 2,
                        "label": "Deal id",
                        "attrs": {
                            "label": { "data-tooltip": "Id of the deal the note will be attached to." }
                        },
                        "source": {
                            "url": "/component/appmixer/pipedrive/crm/ListDeals?outPort=out",
                            "data": {
                                "transform": "./transformers#dealsToSelectArray"
                            }
                        }
                    },
                    "org_id": {
                        "type": "text",
                        "group": "transformation",
                        "index": 3,
                        "label": "Organization id",
                        "attrs": {
                            "label": { "data-tooltip": "Id of the organization this note will be attached to" }
                        },
                        "source": {
                            "url": "/component/appmixer/pipedrive/crm/ListOrganizations?outPort=out",
                            "data": {
                                "transform": "./transformers#organizationsToSelectArray"
                            }
                        }
                    },
                    "person_id": {
                        "type": "text",
                        "group": "transformation",
                        "index": 4,
                        "label": "Person id",
                        "attrs": {
                            "label": { "data-tooltip": "Id of the person this note will be attached to." }
                        },
                        "source": {
                            "url": "/component/appmixer/pipedrive/crm/ListPeople?outPort=out",
                            "data": {
                                "transform": "./transformers#peopleToSelectArray"
                            }
                        }
                    },
                    "pinned_to_deal_flag": {
                        "type": "select",
                        "group": "transformation",
                        "index": 5,
                        "label": "Pinned to deal flag",
                        "attrs": {
                            "label": { "data-tooltip": "If set, then results are filtered by note to deal pinning state (deal_id is also required)." }
                        },
                        "options": [
                            { "content": "Yes", "value": 1 },
                            { "content": "No", "value": 0 }
                        ]
                    },
                    "pinned_to_organization_flag": {
                        "type": "select",
                        "group": "transformation",
                        "index": 6,
                        "label": "Pinned to organization flag",
                        "attrs": {
                            "label": { "data-tooltip": "If set, then results are filtered by note to organization pinning state (org_id is also required)." }
                        },
                        "options": [
                            { "content": "Yes", "value": 1 },
                            { "content": "No", "value": 0 }
                        ]
                    },
                    "pinned_to_person_flag": {
                        "type": "select",
                        "group": "transformation",
                        "index": 7,
                        "label": "Pinned to person flag",
                        "attrs": {
                            "label": { "data-tooltip": "If set, then results are filtered by note to person pinning state (person_id is also required)." }
                        },
                        "options": [
                            { "content": "Yes", "value": 1 },
                            { "content": "No", "value": 0 }
                        ]
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "updatedNote",
            "options": [
                { "label": "id", "value": "id" },
                { "label": "active_flag", "value": "active_flag" },
                { "label": "add_time", "value": "add_time" },
                { "label": "content", "value": "content" },
                { "label": "deal.title", "value": "deal.title" },
                { "label": "deal_id", "value": "deal_id" },
                { "label": "last_update_user_id", "value": "last_update_user_id" },
                { "label": "organization.name", "value": "organization.name" },
                { "label": "org_id", "value": "org_id" },
                { "label": "person.name", "value": "person.name" },
                { "label": "person_id", "value": "person_id" },
                { "label": "pinned_to_deal_flag", "value": "pinned_to_deal_flag" },
                { "label": "pinned_to_organization_flag", "value": "pinned_to_organization_flag" },
                { "label": "pinned_to_person_flag", "value": "pinned_to_person_flag" },
                { "label": "update_time", "value": "update_time" },
                { "label": "user.email", "value": "user.email" },
                { "label": "user.icon_url", "value": "user['icon_url']" },
                { "label": "user.is_you", "value": "user['is_you']" },
                { "label": "user.name", "value": "user.name" },
                { "label": "user_id", "value": "user_id" }
            ]
        }
    ],
    "auth": {
        "service": "appmixer:pipedrive"
    },
    "quota": {
        "manager": "appmixer:pipedrive",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "icon": "data:image/png;base64,"
}
