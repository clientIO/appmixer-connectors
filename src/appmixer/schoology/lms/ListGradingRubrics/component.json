{
    "name": "appmixer.schoology.lms.ListGradingRubrics",
    "author": "Camilo Manrique <camilo@client.io>",
    "description": "List the grading rubrics of a section.",
    "icon": "data:image/svg+xml;base64,",
    "version": "1.0.0",
    "private": true,
    "auth": {
        "service": "appmixer:schoology"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "courseId": { "type": "string" },
                    "sectionId": { "type": "string" }
                },
                "required": ["sectionId"]
            },
            "inspector": {
                "inputs": {
                    "courseId": {
                        "type": "select",
                        "label": "Course",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/schoology/lms/ListCourses?outPort=out",
                            "data": {
                                "transform": "./ListCourses#toSelectArray"
                            }
                        },
                        "tooltip": "Select a course."
                    },
                    "sectionId": {
                        "type": "select",
                        "label": "Section",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/schoology/lms/ListSections?outPort=out",
                            "data": {
                                "messages": {
                                    "in/courseId": "inputs/in/courseId"
                                },
                                "transform": "./ListSections#toSelectArray"
                            }
                        },
                        "tooltip": "Select a course section."
                    }

                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Grading rubrics", "value": "gradingRubrics" }
            ]
        }
    ]
}
