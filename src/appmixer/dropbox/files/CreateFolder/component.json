{
    "name": "appmixer.dropbox.files.CreateFolder",
    "author": "Pawel Putko <pputko@ntrc.eu>",
    "description": "This action creates folder.",
    "private": false,
    "auth": {
        "service": "appmixer:dropbox"
    },
    "inPorts": [
        {
            "name": "folder",
            "schema": {
                "type": "object",
                "properties": {
                    "path": {
                        "type": "string",
                        "pattern": "(/(.|[\\r\\n])*)|(ns:[0-9]+(/.*)?)"
                    },
                    "autorename": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "path"
                ]
            },
            "inspector": {
                "inputs": {
                    "path": {
                        "type": "text",
                        "index": 1,
                        "label": "Path",
                        "tooltip": "Create a folder at a given path. Path has to begin with /."
                    },
                    "autorename": {
                        "type": "toggle",
                        "tooltip": "If there's a conflict, have the Dropbox server try to rename folder to avoid conflict?",
                        "label": "Auto rename",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newFolder",
            "options": [
                {
                    "label": "id",
                    "value": "id"
                },
                {
                    "label": "name",
                    "value": "name"
                },
                {
                    "label": "path_display",
                    "value": "path_display"
                },
                {
                    "label": "path_lower",
                    "value": "path_lower"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
