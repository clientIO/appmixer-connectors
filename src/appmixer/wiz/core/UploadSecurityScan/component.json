{
    "name": "appmixer.wiz.core.UploadSecurityScan",
    "author": "Appmixer <info@appmixer.com>",
    "description": "TBD",
    "private": false,
    "auth": {
        "service": "appmixer:wiz"
    },
    "quota": {
        "manager": "appmixer:wiz",
        "resources": "requests"
    },
    "version": "1.0.0",
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "filename": { "type": "string" },
                    "fileId": { "type": "string" }
                },
                "required": [ "fileId"]
            },
            "inspector": {
                "inputs": {
                    "filename": {
                        "type": "text",
                        "label": "File Name",
                        "index": 0
                    },
                    "fileId": {
                        "type": "filepicker",
                        "index": 1,
                        "label": "File ID",
                        "tooltip": "Select an existing file to upload. Or provide File ID of the file you want to upload."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out"
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
