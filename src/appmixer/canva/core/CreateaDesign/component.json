{
    "name": "appmixer.canva.core.CreateaDesign",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new design.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:canva"
    },
    "quota": {
        "manager": "appmixer:canva",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "Name of the design"
                    },
                    "template_id": {
                        "type": "string",
                        "description": "Optional template ID to base the design on"
                    },
                    "dimensions": {
                        "type": "object",
                        "properties": {
                            "width": {
                                "type": "integer",
                                "description": "Width in pixels"
                            },
                            "height": {
                                "type": "integer",
                                "description": "Height in pixels"
                            }
                        },
                        "description": "Dimensions of the design"
                    },
                    "width": {
                        "type": "integer",
                        "description": "Width in pixels"
                    },
                    "height": {
                        "type": "integer",
                        "description": "Height in pixels"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the design",
                        "label": "Name",
                        "index": 0
                    },
                    "template_id": {
                        "type": "text",
                        "tooltip": "Optional template ID to base the design on",
                        "label": "Template Id",
                        "index": 1
                    },
                    "dimensions": {
                        "type": "text",
                        "tooltip": "Dimensions of the design",
                        "label": "Dimensions",
                        "index": 2
                    },
                    "width": {
                        "type": "number",
                        "tooltip": "Width in pixels",
                        "label": "Width",
                        "index": 3
                    },
                    "height": {
                        "type": "number",
                        "tooltip": "Height in pixels",
                        "label": "Height",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "created_at",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}