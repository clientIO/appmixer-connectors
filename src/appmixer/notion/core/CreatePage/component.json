{
    "name": "appmixer.notion.core.CreatePage",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Create a new page under a parent page.",
    "auth": {
        "service": "appmixer:notion"
    },
    "private": false,
    "quota": {
        "manager": "appmixer:notion",
        "resources": "general",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "parentId": {
                        "type": "string"
                    },
                    "emoji": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "coverUrl": {
                        "type": "string"
                    },
                    "content": {
                        "type": "string"
                    }
                },
                "required": [
                    "parentId",
                    "title"
                ]
            },
            "inspector": {
                "inputs": {
                    "parentId": {
                        "type": "text",
                        "index": 1,
                        "label": "Parent Page ID",
                        "tooltip": "Enter the parent page's ID."
                    },
                    "emoji": {
                        "type": "text",
                        "index": 2,
                        "label": "Emoji",
                        "tooltip": "Enter an emoji for the page icon."
                    },
                    "title": {
                        "type": "text",
                        "index": 3,
                        "label": "Title",
                        "tooltip": "Enter the title of the page."
                    },
                    "coverUrl": {
                        "type": "text",
                        "index": 4,
                        "label": "Cover Image URL",
                        "tooltip": "Enter a Publicly accessible URL for the cover image."
                    },
                    "content": {
                        "type": "text",
                        "index": 5,
                        "label": "Page Content",
                        "tooltip": "Enter the content for the page."
                    }
                }
            }
        }
    ],
    "outPorts": [
    {
        "name": "out",
        "options": [
            { "label": "Object", "value": "object", "schema": { "type": "string" } },
            { "label": "ID", "value": "id", "schema": { "type": "string" } },
            { "label": "Created Time", "value": "created_time", "schema": { "type": "string" } },
            { "label": "Last Edited Time", "value": "last_edited_time", "schema": { "type": "string" } },
            {
                "label": "Created By",
                "value": "created_by",
                "schema": {
                    "type": "object",
                    "properties": {
                        "object": { "title": "Created By.Object", "type": "string" },
                        "id": { "title": "Created By.ID", "type": "string" }
                    }
                }
            },
            {
                "label": "Last Edited By",
                "value": "last_edited_by",
                "schema": {
                    "type": "object",
                    "properties": {
                        "object": { "title": "Last Edited By.Object", "type": "string" },
                        "id": { "title": "Last Edited By.ID", "type": "string" }
                    }
                }
            },
            {
                "label": "Cover",
                "value": "cover",
                "schema": {
                    "type": "object",
                    "properties": {
                        "type": { "title": "Cover.Type", "type": "string" },
                        "external": {
                            "title": "Cover.External",
                            "type": "object",
                            "properties": {
                                "url": { "title": "Cover.External.URL", "type": "string" }
                            }
                        }
                    }
                }
            },
            {
                "label": "Icon",
                "value": "icon",
                "schema": {
                    "type": "object",
                    "properties": {
                        "type": { "title": "Icon.Type", "type": "string" },
                        "emoji": { "title": "Icon.Emoji", "type": "string" }
                    }
                }
            },
            {
                "label": "Parent",
                "value": "parent",
                "schema": {
                    "type": "object",
                    "properties": {
                        "type": { "title": "Parent.Type", "type": "string" },
                        "page_id": { "title": "Parent.Page ID", "type": "string" }
                    }
                }
            },
            { "label": "Archived", "value": "archived", "schema": { "type": "boolean" } },
            { "label": "In Trash", "value": "in_trash", "schema": { "type": "boolean" } },
            {
                "label": "Properties",
                "value": "properties",
                "schema": {
                    "type": "object",
                    "properties": {
                        "title": {
                            "title": "Properties.Title",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Title.ID", "type": "string" },
                                "type": { "title": "Properties.Title.Type", "type": "string" },
                                "title": {
                                    "title": "Properties.Title.Title",
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "type": { "title": "Properties.Title.Title.Type", "type": "string" },
                                            "text": {
                                                "title": "Properties.Title.Title.Text",
                                                "type": "object",
                                                "properties": {
                                                    "content": { "title": "Properties.Title.Title.Text.Content", "type": "string" },
                                                    "link": { "title": "Properties.Title.Title.Text.Link", "type": "string" }
                                                }
                                            },
                                            "annotations": {
                                                "title": "Properties.Title.Title.Annotations",
                                                "type": "object",
                                                "properties": {
                                                    "bold": { "title": "Properties.Title.Title.Annotations.Bold", "type": "boolean" },
                                                    "italic": { "title": "Properties.Title.Title.Annotations.Italic", "type": "boolean" },
                                                    "strikethrough": { "title": "Properties.Title.Title.Annotations.Strikethrough", "type": "boolean" },
                                                    "underline": { "title": "Properties.Title.Title.Annotations.Underline", "type": "boolean" },
                                                    "code": { "title": "Properties.Title.Title.Annotations.Code", "type": "boolean" },
                                                    "color": { "title": "Properties.Title.Title.Annotations.Color", "type": "string" }
                                                }
                                            },
                                            "plain_text": { "title": "Properties.Title.Title.Plain Text", "type": "string" },
                                            "href": { "title": "Properties.Title.Title.Href", "type": "string" }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            { "label": "URL", "value": "url", "schema": { "type": "string" } },
            { "label": "Public URL", "value": "public_url", "schema": { "type": "string" } },
            { "label": "Request ID", "value": "request_id", "schema": { "type": "string" } }
        ]
    }
]

}