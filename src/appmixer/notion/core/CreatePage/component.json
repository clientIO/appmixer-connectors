{
    "name": "appmixer.notion.core.CreatePage",
    "author": "Sayam Nasir <sayam@client.io>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Create a new page under a parent page",
    "auth": {
        "service": "appmixer:notion"
    },
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "parentId": {
                        "type": "string"
                    },
                    "emoji": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "coverUrl": {
                        "type": "string"
                    },
                    "content": {
                        "type": "string"
                    }
                },
                "required": [
                    "parentId",
                    "title"
                ]
            },
            "inspector": {
                "inputs": {
                    "parentId": {
                        "type": "text",
                        "index": 1,
                        "label": "Parent Page ID",
                        "tooltip": "Select the parent page from the list or enter a page ID",
                        "source": {
                            "url": "/component/appmixer/notion/core/ListPages?outPort=out",
                            "data": {
                                "properties": {
                                    "sendWholeArray": true
                                },
                                "transform": "./ListPages#pageToSelectArray"
                            }
                        }
                    },
                    "emoji": {
                        "type": "text",
                        "index": 2,
                        "label": "Emoji",
                        "tooltip": "Enter an emoji for the page icon"
                    },
                    "title": {
                        "type": "text",
                        "index": 3,
                        "label": "Title",
                        "tooltip": "Enter the title of the page"
                    },
                    "coverUrl": {
                        "type": "text",
                        "index": 4,
                        "label": "Cover Image URL",
                        "tooltip": "Enter a Publicly accessible URL for the cover image"
                    },
                    "content": {
                        "type": "text",
                        "index": 5,
                        "label": "Page Content",
                        "tooltip": "Enter the content for the page"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Object",
                    "value": "object"
                },
                {
                    "label": "Page ID",
                    "value": "id"
                },
                {
                    "label": "Created Time",
                    "value": "created_time"
                },
                {
                    "label": "Last Edited Time",
                    "value": "last_edited_time"
                },
                {
                    "label": "Created By",
                    "value": "created_by",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "object": {
                                "label": "Creator",
                                "value": "object"
                            },
                            "id": {
                                "label": "Creator ID",
                                "value": "id"
                            }
                        }
                    }
                },
                {
                    "label": "Last Edited By",
                    "value": "last_edited_by",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "object": {
                                "label": "Editor",
                                "value": "object"
                            },
                            "id": {
                                "label": "Editor ID",
                                "value": "id"
                            }
                        }
                    }
                },
                {
                    "label": "Cover",
                    "value": "cover",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "label": "Type",
                                "value": "type"
                            },
                            "external": {
                                "label": "External",
                                "value": "external",
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "url": {
                                            "label": "URL",
                                            "value": "url"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Icon",
                    "value": "icon",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "label": "Type",
                                "value": "type"
                            },
                            "emoji": {
                                "label": "Emoji",
                                "value": "emoji"
                            }
                        }
                    }
                },
                {
                    "label": "Parent",
