{
    "name": "appmixer.notion.core.NewDatabaseItem",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Triggers when a new Item is added inside a selected database.",
    "tick": true,
    "auth": {
        "service": "appmixer:notion"
    },
    "quota": {
        "manager": "appmixer:notion",
        "resources": "general",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "private": false,
    "properties": {
        "schema": {
            "properties": {
                "databseId": {
                    "type": "string"
                }
            },
            "required": [
                "databaseId"
            ]
        },
        "inspector": {
            "inputs": {
                "databaseId": {
                    "type": "text",
                    "label": "Database ID",
                    "index": 1,
                    "tooltip": "Select a database from the picker or enter a Database ID.",
                    "source": {
                        "url": "/component/appmixer/notion/core/ListDatabases?outPort=out",
                        "data": {
                            "properties": {
                                "sendWholeArray": true
                            },
                            "transform": "./ListDatabases#databaseToSelectArray"
                        }
                    }
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Object", "value": "object", "schema": { "type": "string" } },
                { "label": "Object ID", "value": "id", "schema": { "type": "string" } },
                { "label": "Created time", "value": "created_time", "schema": { "type": "string" } },
                { "label": "Last edited time", "value": "last_edited_time", "schema": { "type": "string" } },
                {
                    "label": "Created by",
                    "value": "created_by",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "object": { "title": "Created by.object", "type": "string" },
                            "id": { "title": "Created by.ID", "type": "string" }
                        }
                    }
                },
                {
                    "label": "Last edited by",
                    "value": "last_edited_by",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "object": { "title": "Last edited by.object", "type": "string" },
                            "id": { "title": "Last edited by.ID", "type": "string" }
                        }
                    }
                },
                { "label": "Cover", "value": "cover", "schema": { "type": "string" } },
                { "label": "Icon", "value": "icon", "schema": { "type": "string" } },
                {
                    "label": "Parent",
                    "value": "parent",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "type": { "title": "Parent.type", "type": "string" },
                            "database_id": { "title": "Parent.databaseID", "type": "string" }
                        }
                    }
                },
                { "label": "Archived", "value": "archived", "schema": { "type": "boolean" } },
                { "label": "In trash", "value": "in_trash", "schema": { "type": "boolean" } },
                {
                    "label": "Properties",
                    "value": "properties",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "title": {
                                "title": "Properties.title",
                                "type": "object",
                                "properties": {
                                    "id": { "title": "Properties.title.ID", "type": "string" },
                                    "type": { "title": "Properties.title.type", "type": "string" },
                                    "title": {
                                        "title": "Properties.title.title",
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "type": { "title": "Properties.title.title.type", "type": "string" },
                                                "text": {
                                                    "title": "Properties.title.title.text",
                                                    "type": "object",
                                                    "properties": {
                                                        "content": { "title": "Properties.title.title.text.content", "type": "string" },
                                                        "link": { "title": "Properties.title.title.text.link", "type": "string" }
                                                    }
                                                },
                                                "annotations": {
                                                    "title": "Properties.title.title.annotations",
                                                    "type": "object",
                                                    "properties": {
                                                        "bold": { "title": "Properties.title.title.annotations.bold", "type": "boolean" },
                                                        "italic": { "title": "Properties.title.title.annotations.italic", "type": "boolean" },
                                                        "strikethrough": { "title": "Properties.title.title.annotations.strikethrough", "type": "boolean" },
                                                        "underline": { "title": "Properties.title.title.annotations.underline", "type": "boolean" },
                                                        "code": { "title": "Properties.title.title.annotations.code", "type": "boolean" },
                                                        "color": { "title": "Properties.title.title.annotations.color", "type": "string" }
                                                    }
                                                },
                                                "plain_text": { "title": "Properties.title.title.plain_text", "type": "string" },
                                                "href": { "title": "Properties.title.title.href", "type": "string" }
                                            }
                                        }
                                    }
                                }
                            },
                            "tags": {
                                "title": "Properties.tags",
                                "type": "object",
                                "properties": {
                                    "id": { "title": "Properties.tags.ID", "type": "string" },
                                    "type": { "title": "Properties.tags.type", "type": "string" },
                                    "multi_select": {
                                        "title": "Properties.tags.multi_select",
                                        "type": "array",
                                        "items": { "type": "object" }
                                    }
                                }
                            },
                            "created_by": {
                                "title": "Properties.created_by",
                                "type": "object",
                                "properties": {
                                    "id": { "title": "Properties.created_by.ID", "type": "string" },
                                    "type": { "title": "Properties.created_by.type", "type": "string" },
                                    "created_by": {
                                        "title": "Properties.created_by.created_by",
                                        "type": "object",
                                        "properties": {
                                            "object": { "title": "Properties.created_by.created_by.object", "type": "string" },
                                            "id": { "title": "Properties.created_by.created_by.ID", "type": "string" }
                                        }
                                    }
                                }
                            },
                            "created_time": {
                                "title": "Properties.created_time",
                                "type": "object",
                                "properties": {
                                    "id": { "title": "Properties.created_time.ID", "type": "string" },
                                    "type": { "title": "Properties.created_time.type", "type": "string" },
                                    "created_time": { "title": "Properties.created_time.created_time", "type": "string" }
                                }
                            },
                            "last_edited_by": {
                                "title": "Properties.last_edited_by",
                                "type": "object",
                                "properties": {
                                    "id": { "title": "Properties.last_edited_by.ID", "type": "string" },
                                    "type": { "title": "Properties.last_edited_by.type", "type": "string" },
                                    "last_edited_by": {
                                        "title": "Properties.last_edited_by.last_edited_by",
                                        "type": "object",
                                        "properties": {
                                            "object": { "title": "Properties.last_edited_by.last_edited_by.object", "type": "string" },
                                            "id": { "title": "Properties.last_edited_by.last_edited_by.ID", "type": "string" }
                                        }
                                    }
                                }
                            },
                            "last_edited_time": {
                                "title": "Properties.last_edited_time",
                                "type": "object",
                                "properties": {
                                    "id": { "title": "Properties.last_edited_time.ID", "type": "string" },
                                    "type": { "title": "Properties.last_edited_time.type", "type": "string" },
                                    "last_edited_time": { "title": "Properties.last_edited_time.last_edited_time", "type": "string" }
                                }
                            }
                        }
                    }
                },
                { "label": "URL", "value": "url", "schema": { "type": "string" } },
                { "label": "Public URL", "value": "public_url", "schema": { "type": "string" } }
            ]
        }
        
    ]
}