{
    "name": "appmixer.notion.core.CreateDatabaseItem",
    "author": "Sayam Nasir <sayam@client.io>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Create a new item in a specified Notion database.",
    "auth": {
        "service": "appmixer:notion"
    },
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "parentId": {
                        "type": "string"
                    },
                    "emoji": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "coverUrl": {
                        "type": "string"
                    },
                    "content": {
                        "type": "string"
                    },
                    "rawJson": {
                        "type": "boolean"
                    },
                    "json": {
                        "type": "string"
                    }
                },
                "required": [
                    "parentId",
                    "title"
                ]
            },
            "inspector": {
                "inputs": {
                    "rawJson": {
                        "type": "toggle",
                        "index": 1,
                        "label": "Use Raw JSON",
                        "tooltip": "Toggle to input the raw JSON",
                        "defaultValue": false
                    },
                    "json": {
                        "type": "textarea",
                        "index": 2,
                        "label": "Raw JSON",
                        "tooltip": "Provide the JSON structure for the item",
                        "when": {
                            "eq": {
                                "./rawJson": true
                            }
                        }
                    },
                    "parentId": {
                        "type": "text",
                        "index": 3,
                        "label": "Parent Database ID",
                        "tooltip": "Select the parent database or enter a database ID",
                        "source": {
                            "url": "/component/appmixer/notion/core/ListDatabases?outPort=out",
                            "data": {
                                "properties": {
                                    "sendWholeArray": true
                                },
                                "transform": "./ListDatabases#databaseToSelectArray"
                            }
                        }
                    },
                    "emoji": {
                        "type": "text",
                        "index": 4,
                        "label": "Emoji",
                        "tooltip": "Enter an emoji for the item icon",
                        "when": {
                            "eq": {
                                "./rawJson": false
                            }
                        }
                    },
                    "title": {
                        "type": "text",
                        "index": 5,
                        "label": "Title",
                        "tooltip": "Enter the title of the item",
                        "when": {
                            "eq": {
                                "./rawJson": false
                            }
                        }
                    },
                    "coverUrl": {
                        "type": "text",
                        "index": 6,
                        "label": "Cover Image URL",
                        "tooltip": "Enter a Publicly accessible URL for the cover image",
                        "when": {
                            "eq": {
                                "./rawJson": false
                            }
                        }
                    },
                    "content": {
                        "type": "text",
                        "index": 7,
                        "label": "Content",
                        "tooltip": "Enter the content for the item",
                        "when": {
                            "eq": {
                                "./rawJson": false
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out"
        }
    ]
}