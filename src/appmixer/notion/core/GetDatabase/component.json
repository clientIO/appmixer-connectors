{
    "name": "appmixer.notion.core.GetDatabase",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Gets a selected Database using its ID.",
    "auth": {
        "service": "appmixer:notion"
    },
    "quota": {
        "manager": "appmixer:notion",
        "resources": "general",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "databaseId": {
                        "type": "string"
                    }
                },
                "required": [
                    "databaseId"
                ]
            },
            "inspector": {
                "inputs": {
                    "databaseId": {
                        "type": "text",
                        "index": 1,
                        "label": "Database ID",
                        "tooltip": "Enter the ID of the database that you want to get."
                    }
                }
            }
        }
    ],
    "outPorts": [
    {
        "name": "out",
        "options": [
            { "label": "Object", "value": "object", "schema": { "type": "string" } },
            { "label": "Database ID", "value": "id", "schema": { "type": "string" } },
            { "label": "Cover", "value": "cover", "schema": { "type": "string" } },
            {
                "label": "Icon",
                "value": "icon",
                "schema": {
                    "type": "object",
                    "properties": {
                        "type": { "title": "Icon.Type", "type": "string" },
                        "external": {
                            "title": "Icon.External",
                            "type": "object",
                            "properties": {
                                "url": { "title": "Icon.External.URL", "type": "string" }
                            }
                        }
                    }
                }
            },
            { "label": "Created Time", "value": "created_time", "schema": { "type": "string" } },
            {
                "label": "Created By",
                "value": "created_by",
                "schema": {
                    "type": "object",
                    "properties": {
                        "object": { "title": "Created By.Object", "type": "string" },
                        "id": { "title": "Created By.ID", "type": "string" }
                    }
                }
            },
            {
                "label": "Last Edited By",
                "value": "last_edited_by",
                "schema": {
                    "type": "object",
                    "properties": {
                        "object": { "title": "Last Edited By.Object", "type": "string" },
                        "id": { "title": "Last Edited By.ID", "type": "string" }
                    }
                }
            },
            { "label": "Last Edited Time", "value": "last_edited_time", "schema": { "type": "string" } },
            {
                "label": "Title",
                "value": "title",
                "schema": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "type": { "title": "Title.Type", "type": "string" },
                            "text": {
                                "title": "Title.Text",
                                "type": "object",
                                "properties": {
                                    "content": { "title": "Title.Text.Content", "type": "string" },
                                    "link": { "title": "Title.Text.Link", "type": "string" }
                                }
                            },
                            "annotations": {
                                "title": "Title.Annotations",
                                "type": "object",
                                "properties": {
                                    "bold": { "title": "Title.Annotations.Bold", "type": "boolean" },
                                    "italic": { "title": "Title.Annotations.Italic", "type": "boolean" },
                                    "strikethrough": { "title": "Title.Annotations.Strikethrough", "type": "boolean" },
                                    "underline": { "title": "Title.Annotations.Underline", "type": "boolean" },
                                    "code": { "title": "Title.Annotations.Code", "type": "boolean" },
                                    "color": { "title": "Title.Annotations.Color", "type": "string" }
                                }
                            },
                            "plain_text": { "title": "Title.Plain_Text", "type": "string" },
                            "href": { "title": "Title.Href", "type": "string" }
                        }
                    }
                }
            },
            { "label": "Description", "value": "description", "schema": { "type": "array", "items": { "type": "object" } } },
            { "label": "Is Inline", "value": "is_inline", "schema": { "type": "boolean" } },
            {
                "label": "Properties",
                "value": "properties",
                "schema": {
                    "type": "object",
                    "properties": {
                        "text": {
                            "title": "Properties.Text",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Text.ID", "type": "string" },
                                "name": { "title": "Properties.Text.Name", "type": "string" },
                                "type": { "title": "Properties.Text.Type", "type": "string" },
                                "rich_text": { "title": "Properties.Text.Rich_Text", "type": "object" }
                            }
                        },
                        "title": {
                            "title": "Properties.Title",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Title.ID", "type": "string" },
                                "name": { "title": "Properties.Title.Name", "type": "string" },
                                "type": { "title": "Properties.Title.Type", "type": "string" },
                                "title": { "title": "Properties.Title.Title", "type": "object" }
                            }
                        },
                        "tags": {
                            "title": "Properties.Tags",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Tags.ID", "type": "string" },
                                "name": { "title": "Properties.Tags.Name", "type": "string" },
                                "type": { "title": "Properties.Tags.Type", "type": "string" },
                                "multi_select": {
                                    "title": "Properties.Tags.Multi_Select",
                                    "type": "object",
                                    "properties": {
                                        "options": {
                                            "title": "Properties.Tags.Multi_Select.Options",
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": { "title": "Properties.Tags.Multi_Select.Options.ID", "type": "string" },
                                                    "name": { "title": "Properties.Tags.Multi_Select.Options.Name", "type": "string" },
                                                    "color": { "title": "Properties.Tags.Multi_Select.Options.Color", "type": "string" },
                                                    "description": { "title": "Properties.Tags.Multi_Select.Options.Description", "type": "string" }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "created_by": {
                            "title": "Properties.Created_By",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Created_By.ID", "type": "string" },
                                "name": { "title": "Properties.Created_By.Name", "type": "string" },
                                "type": { "title": "Properties.Created_By.Type", "type": "string" },
                                "created_by": {
                                    "title": "Properties.Created_By.Created_By",
                                    "type": "object",
                                    "properties": {
                                        "object": { "title": "Properties.Created_By.Created_By.Object", "type": "string" },
                                        "id": { "title": "Properties.Created_By.Created_By.ID", "type": "string" }
                                    }
                                }
                            }
                        },
                        "created_time": {
                            "title": "Properties.Created_Time",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Created_Time.ID", "type": "string" },
                                "type": { "title": "Properties.Created_Time.Type", "type": "string" },
                                "created_time": { "title": "Properties.Created_Time.Created_Time", "type": "string" }
                            }
                        },
                        "last_edited_by": {
                            "title": "Properties.Last_Edited_By",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Last_Edited_By.ID", "type": "string" },
                                "name": { "title": "Properties.Last_Edited_By.Name", "type": "string" },
                                "type": { "title": "Properties.Last_Edited_By.Type", "type": "string" },
                                "last_edited_by": {
                                    "title": "Properties.Last_Edited_By.Last_Edited_By",
                                    "type": "object",
                                    "properties": {
                                        "object": { "title": "Properties.Last_Edited_By.Last_Edited_By.Object", "type": "string" },
                                        "id": { "title": "Properties.Last_Edited_By.Last_Edited_By.ID", "type": "string" }
                                    }
                                }
                            }
                        },
                        "last_edited_time": {
                            "title": "Properties.Last_Edited_Time",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Last_Edited_Time.ID", "type": "string" },
                                "type": { "title": "Properties.Last_Edited_Time.Type", "type": "string" },
                                "last_edited_time": { "title": "Properties.Last_Edited_Time.Last_Edited_Time", "type": "string" }
                            }
                        }
                    }
                }
            },
            {
                "label": "Parent",
                "value": "parent",
                "schema": {
                    "type": "object",
                    "properties": {
                        "type": { "title": "Parent.Type", "type": "string" },
                        "workspace": { "title": "Parent.Workspace", "type": "boolean" }
                    }
                }
            },
            { "label": "URL", "value": "url", "schema": { "type": "string" } },
            { "label": "Public URL", "value": "public_url", "schema": { "type": "string" } },
            { "label": "Archived", "value": "archived", "schema": { "type": "boolean" } },
            { "label": "In Trash", "value": "in_trash", "schema": { "type": "boolean" } },
            { "label": "Request ID", "value": "request_id", "schema": { "type": "string" } }
        ]
    }
]




}