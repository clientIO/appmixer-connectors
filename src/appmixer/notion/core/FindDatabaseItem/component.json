{
    "name": "appmixer.notion.core.FindDatabaseItem",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Find an item in a specified Notion database.",
    "auth": {
        "service": "appmixer:notion"
    },
    "private": false,
    "quota": {
        "manager": "appmixer:notion",
        "resources": "general",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "properties": {
        "schema": {
            "properties": {
                "databaseId": {
                    "type": "string"
                }
            },
            "required": [
                "databaseId"
            ]
        },
        "inspector": {
            "inputs": {
                "databaseId": {
                    "type": "text",
                    "label": "Database ID",
                    "index": 1,
                    "tooltip": "Select a database from the picker or enter a Database ID.",
                    "source": {
                        "url": "/component/appmixer/notion/core/ListDatabases?outPort=out",
                        "data": {
                            "properties": {
                                "sendWholeArray": true
                            },
                            "transform": "./ListDatabases#databaseToSelectArray"
                        }
                    }
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "source": {
                "url": "/component/appmixer/notion/core/FindDatabaseItem?outPort=out",
                "data": {
                    "properties": {
                        "generateInspector": true,
                        "databaseId": "properties/databaseId"
                    }
                }
            }
        }
    ],
    "outPorts": [
    {
        "name": "out",
        "options": [
            { "label": "Object", "value": "object", "schema": { "type": "string" } },
            { "label": "Object ID", "value": "id", "schema": { "type": "string" } },
            { "label": "Created Time", "value": "created_time", "schema": { "type": "string" } },
            { "label": "Last Edited Time", "value": "last_edited_time", "schema": { "type": "string" } },
            {
                "label": "Created By",
                "value": "created_by",
                "schema": {
                    "type": "object",
                    "properties": {
                        "object": { "title": "Created By.Object", "type": "string" },
                        "id": { "title": "Created By.ID", "type": "string" }
                    }
                }
            },
            {
                "label": "Last Edited By",
                "value": "last_edited_by",
                "schema": {
                    "type": "object",
                    "properties": {
                        "object": { "title": "Last Edited By.Object", "type": "string" },
                        "id": { "title": "Last Edited By.ID", "type": "string" }
                    }
                }
            },
            { "label": "Cover", "value": "cover", "schema": { "type": "string" } },
            { "label": "Icon", "value": "icon", "schema": { "type": "string" } },
            {
                "label": "Parent",
                "value": "parent",
                "schema": {
                    "type": "object",
                    "properties": {
                        "type": { "title": "Parent.Type", "type": "string" },
                        "database_id": { "title": "Parent.Database ID", "type": "string" }
                    }
                }
            },
            { "label": "Archived", "value": "archived", "schema": { "type": "boolean" } },
            { "label": "In Trash", "value": "in_trash", "schema": { "type": "boolean" } },
            {
                "label": "Properties.Status",
                "value": "properties.status",
                "schema": {
                    "type": "object",
                    "properties": {
                        "id": { "title": "Properties.Status.ID", "type": "string" },
                        "name": { "title": "Properties.Status.Name", "type": "string" },
                        "color": { "title": "Properties.Status.Color", "type": "string" }
                    }
                }
            },
            {
                "label": "Properties.Phone",
                "value": "properties.phone_number",
                "schema": { "type": "string" }
            },
            {
                "label": "Properties.Text",
                "value": "properties.rich_text",
                "schema": { "type": "string" }
            },
            {
                "label": "Properties.Email",
                "value": "properties.email",
                "schema": { "type": "string" }
            },
            {
                "label": "Properties.URL",
                "value": "properties.url",
                "schema": { "type": "string" }
            },
            {
                "label": "Properties.Files",
                "value": "properties.files",
                "schema": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": { "title": "Files Name", "type": "string" },
                            "external.url": { "title": "Files External URL", "type": "string" }
                        }
                    }
                }
            },
            {
                "label": "Properties.Multi-select",
                "value": "properties.multi_select",
                "schema": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": { "title": "Multi-select Name", "type": "string" },
                            "color": { "title": "Multi-select Color", "type": "string" }
                        }
                    }
                }
            },
            {
                "label": "Properties.Date",
                "value": "properties.date",
                "schema": {
                    "type": "object",
                    "properties": {
                        "start": { "title": "Date Start", "type": "string" },
                        "end": { "title": "Date End", "type": "string" }
                    }
                }
            },
            {
                "label": "Properties.Title",
                "value": "properties.title",
                "schema": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "text.content": { "title": "Title Content", "type": "string" }
                        }
                    }
                }
            },
            { "label": "URL", "value": "url", "schema": { "type": "string" } }
        ]
    },
    {
        "name": "notFound"
    }
]


}
