{
    "name": "appmixer.notion.core.ListDatabases",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Returns a list of databases. This component will return a maximum of 100 records.",
    "auth": {
        "service": "appmixer:notion"
    },
    "quota": {
        "manager": "appmixer:notion",
        "resources": "general",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "private": true,
    "inPorts": ["in"],
    "outPorts": [
        {
        "name": "out",
        "options": [
            { "label": "Databases", "value": "databases",
                "schema": { "type": "array",
                    "items": { "type": "object",
                        "properties": {
                            "id": { "label": "Database ID", "value": "id" },
                            "title": { "label": "Title", "value": "title" },
                            "created_time": { "label": "Created Time", "value": "created_time" },
                            "last_edited_time": { "label": "Last Edited time", "value": "last_edited_time" }
                        }
                    }
                }
            }
        ]
    }
]
}

