                                "type": { "title": "Properties.Created By.Type", "type": "string" },
                                "created_by": {
                                    "title": "Properties.Created By.Created By",
                                    "type": "object",
                                    "properties": {
                                        "object": { "title": "Properties.Created By.Created By.Object", "type": "string" },
                                        "id": { "title": "Properties.Created By.Created By.ID", "type": "string" }
                                    }
                                }
                            }
                        },
                        "created_time": {
                            "title": "Properties.Created Time",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Created Time.ID", "type": "string" },
                                "type": { "title": "Properties.Created Time.Type", "type": "string" },
                                "created_time": { "title": "Properties.Created Time.Created Time", "type": "string" }
                            }
                        },
                        "last_edited_by": {
                            "title": "Properties.Last Edited By",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Last Edited By.ID", "type": "string" },
                                "type": { "title": "Properties.Last Edited By.Type", "type": "string" },
                                "last_edited_by": {
                                    "title": "Properties.Last Edited By.Last Edited By",
                                    "type": "object",
                                    "properties": {
                                        "object": { "title": "Properties.Last Edited By.Last Edited By.Object", "type": "string" },
                                        "id": { "title": "Properties.Last Edited By.Last Edited By.ID", "type": "string" }
                                    }
                                }
                            }
                        },
                        "last_edited_time": {
                            "title": "Properties.Last Edited Time",
                            "type": "object",
                            "properties": {
                                "id": { "title": "Properties.Last Edited Time.ID", "type": "string" },
                                "type": { "title": "Properties.Last Edited Time.Type", "type": "string" },
                                "last_edited_time": { "title": "Properties.Last Edited Time.Last Edited Time", "type": "string" }
