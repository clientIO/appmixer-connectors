{
    "name": "appmixer.notion.core.GetPage",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Gets a page or a database item using its ID.",
    "auth": {
        "service": "appmixer:notion"
    },
    "quota": {
        "manager": "appmixer:notion",
        "resources": "general",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "private": false,
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "pageId": {
                        "type": "string"
                    }
                },
                "required": [
                    "pageId"
                ]
            },
            "inspector": {
                "inputs": {
                    "pageId": {
                        "type": "text",
                        "index": 1,
                        "label": "Page ID",
                        "tooltip": "Enter the ID of the page that you want to get."
                    }
                }
            }
        }
    ],
    "outPorts": [
    {
        "name": "out",
        "options": [
            { "label": "Object", "value": "object", "schema": { "type": "string" } },
            { "label": "Page ID", "value": "id", "schema": { "type": "string" } },
