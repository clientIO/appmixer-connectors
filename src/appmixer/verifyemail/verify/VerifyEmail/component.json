{
    "name": "appmixer.verifyemail.verify.VerifyEmail",
    "author": "David Durman <david@client.io>",
    "icon": "data:image/png;base64,",
    "description": "Verify if an email address exists or not.",
    "private": false,
    "auth": {
        "service": "appmixer:verifyemail"
    },
    "inPorts": [
        {
            "name": "email",
            "schema": {
                "type": "object",
                "properties": {
                    "email": { "type": "string", "format": "email" }
                },
                "required": [
                    "email"
                ]
            },
            "inspector": {
                "inputs": {
                    "email": {
                        "type": "text",
                        "label": "Email Address",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "ok",
            "options": [
                { "label": "Email", "value": "email" },
                { "label": "Status", "value": "status" },
                { "label": "Status Description", "value": "status_description" },
                { "label": "SMTP code", "value": "smtp_code" },
                { "label": "SMTP log", "value": "smtp_log" },
                { "label": "Credits", "value": "credits" }
            ]
        },
        {
            "name": "bad",
            "options": [
                { "label": "Email", "value": "email" },
                { "label": "Status", "value": "status" },
                { "label": "Status Description", "value": "status_description" },
                { "label": "SMTP code", "value": "smtp_code" },
                { "label": "SMTP log", "value": "smtp_log" },
                { "label": "Credits", "value": "credits" }
            ]
        }
    ]
}
