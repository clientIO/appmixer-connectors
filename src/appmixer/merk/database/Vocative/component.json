{
    "name": "appmixer.merk.database.Vocative",
    "author": "David Durman <david@client.io>",
    "description": "This action returns vocative and sex for given first, last or both names.",
    "inPorts": [
        {
            "name": "query",
            "schema": {
                "type": "object",
                "properties": {
                    "first_name": { "type": "string" },
                    "last_name": { "type": "string" }
                },
                "anyOf": [
                    {
                        "required": ["first_name"]
                    },
                    {
                        "required": ["last_name"]
                    }
                ]
            },
            "inspector": {
                "inputs": {
                    "first_name": {
                        "type": "text",
                        "group": "transformation",
                        "index": 1,
                        "label": "First name",
                        "attrs": {
                            "label": { "data-tooltip": "First name" }
                        }
                    },
                    "last_name": {
                        "type": "text",
                        "group": "transformation",
                        "index": 2,
                        "label": "Last name",
                        "attrs": {
                            "label": { "data-tooltip": "Last name" }
                        }
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "vocative",
            "options": [
                { "label": "First name", "value": "first_name" },
                { "label": "Last name", "value": "last_name" },
                { "label": "Sex (m/f)", "value": "sex" }
            ]
        }
    ],
    "auth": {
        "service": "appmixer:merk"
    },
    "icon": "data:image/svg+xml;base64,"
}
