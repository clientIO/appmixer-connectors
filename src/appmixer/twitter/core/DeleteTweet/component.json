{
    "name": "appmixer.twitter.core.DeleteTweet",
    "author": "Appmixer <infor@appmixer.com>",
    "description": "When triggered, deletes a new tweet.",
    "auth": {
        "service": "appmixer:twitter",
        "scope": [ "tweet.write" ]
    },
    "quota": {
        "manager": "appmixer:twitter",
        "resources": "manage_tweets",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "tweetId": { "type": "string" }
                },
                "required": [ "tweetId" ]
            },
            "inspector": {
                "inputs": {
                    "tweetId": {
                        "label": "Tweet ID",
                        "type": "text",
                        "index": 1,
                        "tooltip": "Tweet ID"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Is Deleted", "value": "deleted" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
