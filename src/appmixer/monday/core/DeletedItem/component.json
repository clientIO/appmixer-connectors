{
    "name": "appmixer.monday.core.DeletedItem",
    "author": "Harsha Surisetty <harsha@client.io>",
    "icon": "data:image/png;base64,",
    "description": "Triggers when an item has been deleted in a board.",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:monday"
    },
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Item Id",
                    "value": "itemId"
                }
            ]
        }
    ],
    "properties": {
        "schema": {
            "properties": {
                "boardId": {
                    "type": "string"
                }
            },
            "required": ["boardId"]
        },
        "inspector": {
            "inputs": {
                "boardId": {
                    "type": "select",
                    "label": "Board",
                    "index": 1,
                    "tooltip": "Select a board.",
                    "source": { 
                        "url": "/component/appmixer/monday/core/ListBoards?outPort=out",
                        "data": {
                            "properties": { "sendWholeArray": true },
                            "transform": "./ListBoards#boardsToSelectArray"
                        }
                    }
                }
            }
        }
    }
}
