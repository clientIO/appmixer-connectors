                    "custom_body": { "type": "string" },
                    "custom_subject": { "type": "string" },
                    "notes": { "type": "string" },
                    "terms": { "type": "string" },
                    "shipping_charge": { "type": "number", "maxLength": 100 },
                    "adjustment": { "type": "number" },
                    "adjustment_description": { "type": "string" },
