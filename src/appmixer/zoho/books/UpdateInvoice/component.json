                            { "content": "substitution_previous_cfdi", "value": "substitution_previous_cfdi" },
                            { "content": "transfer_of_goods", "value": "transfer_of_goods" },
                            { "content": "invoice_generated_from_order", "value": "invoice_generated_from_order" },
