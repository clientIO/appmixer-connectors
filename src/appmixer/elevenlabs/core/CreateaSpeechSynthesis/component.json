{
    "name": "appmixer.elevenlabs.core.CreateaSpeechSynthesis",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Synthesizes a given text into speech using a specified voice.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:elevenlabs"
    },
    "quota": {
        "manager": "appmixer:elevenlabs",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "voice_id": {
                        "type": "string"
                    },
                    "text": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "voice_id": {
                        "type": "text",
                        "tooltip": "ID of the voice to be used for synthesis.",
                        "label": "Voice Id",
                        "index": 0
                    },
                    "text": {
                        "type": "text",
                        "tooltip": "The text content that will be synthesized into speech.",
                        "label": "Text",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Audio Url",
                    "value": "audio_url",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,PD94bW"
}