{
    "name": "appmixer.elevenlabs.core.CreateaSpeechSynthesis",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Synthesizes a given text into speech using a specified voice.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:elevenlabs"
    },
    "quota": {
        "manager": "appmixer:elevenlabs",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "voice_id": {
                        "type": "string"
                    },
                    "text": {
                        "type": "string"
                    },
                    "model_id": {
                        "type": "string"
                    },
                    "language_code": {
                        "type": "string"
                    },
                    "seed": {
                        "type": "number"
                    },
                    "voice_settings": {
                        "type": "object"
                    },
                    "previous_text": {
                        "type": "string"
                    },
                    "next_text": {
                        "type": "string"
                    },
                    "apply_language_text_normalization": {
                        "type": "boolean"
                    },
                    "apply_text_normalization": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "voice_id": {
                        "type": "text",
                        "tooltip": "ID of the voice to be used for synthesis. This value is embedded in the endpoint URL.",
                        "label": "Voice Id",
                        "index": 0
                    },
                    "text": {
                        "type": "text",
                        "tooltip": "The text content that will be synthesized into speech.",
                        "label": "Text",
                        "index": 1
                    },
                    "model_id": {
                        "type": "text",
                        "tooltip": "Identifier of the model to be used for text-to-speech synthesis, for example 'eleven_multilingual_v2'.",
                        "label": "Model Id",
                        "index": 2
                    },
                    "language_code": {
                        "type": "text",
                        "tooltip": "Optional language code for the synthesis.",
                        "label": "Language Code",
                        "index": 3
                    },
                    "seed": {
                        "type": "text",
                        "tooltip": "Seed value for randomization purposes.",
                        "label": "Seed",
                        "index": 4
                    },
                    "voice_settings": {
                        "type": "text",
                        "tooltip": "Optional voice settings as a JSON object to control properties of the generated voice.",
                        "label": "Voice Settings",
                        "index": 5
                    },
                    "previous_text": {
                        "type": "text",
                        "tooltip": "Optional text from the previous request used to contextually improve the synthesis.",
                        "label": "Previous Text",
                        "index": 6
                    },
                    "next_text": {
                        "type": "text",
                        "tooltip": "Optional text for the next request that may help smooth transitions in streaming scenarios.",
                        "label": "Next Text",
                        "index": 7
                    },
                    "apply_language_text_normalization": {
                        "type": "toggle",
                        "tooltip": "Flag to apply language text normalization.",
                        "label": "Apply Language Text Normalization",
                        "index": 8
                    },
                    "apply_text_normalization": {
                        "type": "text",
                        "tooltip": "Option for text normalization, for instance 'off'.",
                        "label": "Apply Text Normalization",
                        "index": 9
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "File ID",
                    "value": "fileId"
                },
                {
                    "label": "File Name",
                    "value": "filename"
                },
                {
                    "label": "Chunk Size",
                    "value": "chunkSize"
                },
                {
                    "label": "File Length",
                    "value": "length"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iODc2IiBoZWlnaHQ9Ijg3NiIgdmlld0JveD0iMCAwIDg3NiA4NzYiIGZpbGw9Im5vbmUiIHhtbG5zPSJodHRwOi8vd3d3LnczLm9yZy8yMDAwL3N2ZyI+CjxwYXRoIGQ9Ik00NjggMjkySDUyOFY1ODRINDY4VjI5MloiIGZpbGw9ImJsYWNrIi8+CjxwYXRoIGQ9Ik0zNDggMjkySDQwOFY1ODRIMzQ4VjI5MloiIGZpbGw9ImJsYWNrIi8+Cjwvc3ZnPgo="
}
