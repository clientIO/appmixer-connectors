{
    "name": "appmixer.evernote.list.ListTags",
    "author": "Tomasz Hencel <thencel@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "When triggered, returns a list of tags from your account.",
    "private": true,
    "auth": {
        "service": "appmixer:evernote"
    },
    "quota": {
        "manager": "appmixer:evernote:list",
        "resources": [ "tag/listTags" ],
        "scope": { "userId": "{{userId}}" }
    },
    "inPorts": [
        "in"
    ],
    "outPorts": [
        "tags"
    ]
}