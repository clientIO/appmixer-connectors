{
    "name": "appmixer.evernote.list.AppendToNote",
    "author": "Tomasz Hencel <thencel@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Append a content to an existing note in Evernote.",
    "private": false,
    "auth": {
        "service": "appmixer:evernote"
    },
    "quota": {
        "concurrency": 1,
        "manager": "appmixer:evernote:list",
        "resources": [ "note/getNote" ],
        "scope": { "userId": "{{userId}}" }
    },
    "inPorts": [
        {
            "name": "content",
            "schema": {
                "type": "object",
                "properties": {
                    "noteContent": { "type": "string" },
                    "noteId": { "type": "string" }
                },
                "required": [
                    "noteId",
                    "noteContent"
                ]
            },
            "inspector": {
                "inputs": {
                    "noteContent": {
                        "type": "textarea",
                        "group": "transformation",
                        "label": "Append content",
                        "index": 2,
                        "attrs": {
                            "label": {
                                "data-tooltip": "Text that will be appended to an existing note."
                            }
                        }
                    },
                    "noteId": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Note",
                        "index": 1,
                        "attrs": {
                            "label": {
                                "data-tooltip": "Select a Note."
                            }
                        }
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "note",
            "options": [
                { "label": "guid", "value": "guid" },
                { "label": "title", "value": "title" },
                { "label": "content", "value": "content" },
                { "label": "contentLength", "value": "contentLength" },
                { "label": "created", "value": "created" },
                { "label": "updated", "value": "updated" },
                { "label": "deleted", "value": "deleted" },
                { "label": "active", "value": "active" },
                { "label": "notebookGuid", "value": "notebookGuid" },
                { "label": "tagGuids", "value": "tagGuids" },
                { "label": "resources", "value": "resources" },
                { "label": "attributes", "value": "attributes" },
                { "label": "attributes.subjectDate", "value": "attributes.subjectDate" },
                { "label": "attributes.latitude", "value": "attributes.latitude" },
                { "label": "attributes.longitude", "value": "attributes.longitude" },
                { "label": "attributes.altitude", "value": "attributes.altitude" },
                { "label": "attributes.author", "value": "attributes.author" },
                { "label": "attributes.source", "value": "attributes.source" },
                { "label": "attributes.sourceURL", "value": "attributes.sourceURL" },
                { "label": "attributes.sourceApplication", "value": "attributes.sourceApplication" },
                { "label": "attributes.shareDate", "value": "attributes.shareDate" },
                { "label": "attributes.reminderOrder", "value": "attributes.reminderOrder" },
                { "label": "attributes.reminderDoneTime", "value": "attributes.reminderDoneTime" },
                { "label": "attributes.reminderTime", "value": "attributes.reminderTime" },
                { "label": "attributes.placeName", "value": "attributes.placeName" },
                { "label": "attributes.contentClass", "value": "attributes.contentClass" },
                { "label": "attributes.applicationData", "value": "attributes.applicationData" },
                { "label": "attributes.lastEditedBy", "value": "attributes.lastEditedBy" },
                { "label": "attributes.classifications", "value": "attributes.classifications" },
                { "label": "attributes.creatorId", "value": "attributes.creatorId" },
                { "label": "attributes.lastEditorId", "value": "attributes.lastEditorId" },
                { "label": "tagNames", "value": "tagNames" }
            ]
        }
    ]
}
