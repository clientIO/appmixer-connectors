{
    "name": "appmixer.salesforce.crm.NewEvent",
    "author": "Tomasz Hencel <thencel@ntrc.eu>",
    "description": "This trigger fires every time a new event is added.",
    "private": false,
    "auth": {
        "service": "appmixer:salesforce"
    },
    "quota": {
        "manager": "appmixer:salesforce",
        "resources": "requests"
    },
    "version": "1.1.2",
    "dependencies": {
        "service": "1.1.2"
    },
    "tick": true,
    "outPorts": [
        {
            "name": "event",
            "options": [
                { "label": "AccountId", "value": "AccountId" },
                { "label": "ActivityDate", "value": "ActivityDate" },
                { "label": "ActivityDateTime", "value": "ActivityDateTime" },
                { "label": "CreatedById", "value": "CreatedById" },
                { "label": "CreatedDate", "value": "CreatedDate" },
                { "label": "Description", "value": "Description" },
                { "label": "DurationInMinutes", "value": "DurationInMinutes" },
                { "label": "EndDateTime", "value": "EndDateTime" },
                { "label": "EventSubtype", "value": "EventSubtype" },
                { "label": "GroupEventType", "value": "GroupEventType" },
                { "label": "Id", "value": "Id" },
                { "label": "IsAllDayEvent", "value": "IsAllDayEvent" },
                { "label": "IsArchived", "value": "IsArchived" },
                { "label": "IsChild", "value": "IsChild" },
                { "label": "IsDeleted", "value": "IsDeleted" },
                { "label": "IsGroupEvent", "value": "IsGroupEvent" },
                { "label": "IsPrivate", "value": "IsPrivate" },
                { "label": "IsRecurrence", "value": "IsRecurrence" },
                { "label": "IsReminderSet", "value": "IsReminderSet" },
                { "label": "LastModifiedById", "value": "LastModifiedById" },
                { "label": "LastModifiedDate", "value": "LastModifiedDate" },
                { "label": "Location", "value": "Location" },
                { "label": "OwnerId", "value": "OwnerId" },
                { "label": "ReminderDateTime", "value": "ReminderDateTime" },
                { "label": "ShowAs", "value": "ShowAs" },
                { "label": "StartDateTime", "value": "StartDateTime" },
                { "label": "Subject", "value": "Subject" },
                { "label": "SystemModstamp", "value": "SystemModstamp" },
                { "label": "Type", "value": "Type" },
                { "label": "WhatCount", "value": "WhatCount" },
                { "label": "WhatId", "value": "WhatId" },
                { "label": "WhoCount", "value": "WhoCount" },
                { "label": "WhoId", "value": "WhoId" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
