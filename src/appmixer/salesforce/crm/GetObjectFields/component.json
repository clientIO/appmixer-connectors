{
    "name": "appmixer.salesforce.crm.GetObjectFields",
    "author": "Vladimir Talas <lades@client.io>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Get Object fields.",
    "auth": {
        "service": "appmixer:salesforce"
    },
    "quota": {
        "manager": "appmixer:salesforce",
        "resources": "requests"
    },
    "version": "1.1.2",
    "dependencies": {
        "service": "1.1.2"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "objectName": { "type": "string" }
                }
            },
            "inspector": {
                "inputs": {
                    "objectName": {
                        "type": "select",
                        "label": "Object Name",
                        "index": 1,
                        "tooltip": "The name of the object. For example, Account.",
                        "source": {
                            "url": "/component/appmixer/salesforce/crm/ListObjects?outPort=out",
                            "data": {
                                "transform": "./transformers#toSelectArray"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "fields", "value": "fields",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": { "label": "name", "value": "name" }
                            }
                        }
                    }
                }
            ]
        }
    ]
}
