{
    "name": "appmixer.azureCognitiveServices.computervision.DescribeImage",
    "author": "David Durman <david@client.io>",
    "description": "This operation generates a description of an image in human readable language with complete sentences.",
    "private": false,
    "quota": {
        "manager": "appmixer:azureCognitiveServices",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "properties": {
                    "imageUrl": {
                        "type": "string"
                    },
                    "language": {
                        "type": "string"
                    },
                    "maxCandidates": {
                        "type": "integer"
                    }
                },
                "required": [
                    "imageUrl"
                ]
            },
            "inspector": {
                "inputs": {
                    "imageUrl": {
                        "type": "text",
                        "label": "Image URL",
                        "index": 1,
                        "tooltip": "Specify a URL of an image to analyze."
                    },
                    "language": {
                        "type": "select",
                        "label": "Language",
                        "defaultValue": "en",
                        "options": [
                            { "value": "en", "content": "English" },
                            { "value": "es", "content": "Spanish" },
                            { "value": "ja", "content": "Japanese" },
                            { "value": "pt", "content": "Portuguese" },
                            { "value": "zh", "content": "Simplified Chinese" }
                        ],
                        "index": 2,
                        "tooltip": "The desired language for output generation. Default is English."
                    },
                    "maxCandidates": {
                        "type": "number",
                        "label": "Max Candidates",
                        "defaultValue": 1,
                        "index": 3,
                        "tooltip": "Maximum number of candidate descriptions to be returned. The default is 1."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "First Caption Text",
                    "value": "captions.[0].text"
                },
                {
                    "label": "First Caption Confidence",
                    "value": "captions.[0].confidence"
                },
                {
                    "label": "Captions",
                    "value": "captions"
                },
                {
                    "label": "Tags",
                    "value": "tags"
                },
                {
                    "label": "Width",
                    "value": "metadata.width"
                },
                {
                    "label": "Height",
                    "value": "metadata.height"
                },
                {
                    "label": "Format",
                    "value": "metadata.format"
                },
                {
                    "label": "Metadata",
                    "value": "metadata"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
