{
    "name": "appmixer.brevo.core.SendEmail",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Sends a transactional email.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:brevo"
    },
    "quota": {
        "manager": "appmixer:brevo",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "to": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                }
                            }
                        },
                        "description": "Recipients"
                    },
                    "email": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "subject": {
                        "type": "string",
                        "description": "Email subject"
                    },
                    "htmlContent": {
                        "type": "string",
                        "description": "HTML content"
                    },
                    "sender": {
                        "type": "object",
                        "properties": {
                            "email": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "description": "Sender info"
                    },
                    "cc": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "CC recipients"
                    },
                    "bcc": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        },
                        "description": "BCC recipients"
                    },
                    "replyTo": {
                        "type": "object",
                        "description": "Reply-To info"
                    },
                    "attachmentUrls": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "Attachment URLs"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "to": {
                        "type": "text",
                        "tooltip": "Recipients",
                        "label": "To",
                        "index": 0
                    },
                    "email": {
                        "type": "text",
                        "tooltip": "",
                        "label": "Email",
                        "index": 1
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "",
                        "label": "Name",
                        "index": 2
                    },
                    "subject": {
                        "type": "text",
                        "tooltip": "Email subject",
                        "label": "Subject",
                        "index": 3
                    },
                    "htmlContent": {
                        "type": "text",
                        "tooltip": "HTML content",
                        "label": "HtmlContent",
                        "index": 4
                    },
                    "sender": {
                        "type": "text",
                        "tooltip": "Sender info",
                        "label": "Sender",
                        "index": 5
                    },
                    "cc": {
                        "type": "text",
                        "tooltip": "CC recipients",
                        "label": "Cc",
                        "index": 6
                    },
                    "bcc": {
                        "type": "text",
                        "tooltip": "BCC recipients",
                        "label": "Bcc",
                        "index": 7
                    },
                    "replyTo": {
                        "type": "text",
                        "tooltip": "Reply-To info",
                        "label": "ReplyTo",
                        "index": 8
                    },
                    "attachmentUrls": {
                        "type": "text",
                        "tooltip": "Attachment URLs",
                        "label": "AttachmentUrls",
                        "index": 9
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message Id",
                    "value": "messageId",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}