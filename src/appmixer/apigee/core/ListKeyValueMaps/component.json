{
    "name": "appmixer.apigee.core.ListKeyValueMaps",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve a list of all key value maps in a specific environment. This endpoint is useful for obtaining the available maps that can contain IP blocking entries.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:apigee",
        "scope": ["https://www.googleapis.com/auth/cloud-platform"]
    },
    "quota": {
        "manager": "appmixer:apigee",
        "resources": "requests"
    },
    "properties": {
        "schema": {
            "type": "object",
            "properties": {
                "org": {
                    "type": "string"
                },
                "env": {
                    "type": "string"
                }
            }
        },
        "inspector": {
            "inputs": {
                "org": {
                    "type": "text",
                    "label": "Organization",
                    "tooltip": "The Apigee organization name.",
                    "index": 0,
                    "source": {
                        "url": "/component/appmixer/apigee/core/ListOrganizations?outPort=out",
                        "data": {
                            "transform": "./ListOrganizations#toSelectOptions"
                        }
                    }
                },
                "env": {
                    "type": "text",
                    "tooltip": "The environment containing the key value map.",
                    "label": "Environment",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/apigee/core/ListEnvironments?outPort=out",
                        "data": {
                            "properties": {
                                "org": "properties/org"
                            },
                            "transform": "./ListEnvironments#toSelectOptions"
                        }
                    }
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in"
        }
    ],
    "outPorts": [
        {
            "name": "out"
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}