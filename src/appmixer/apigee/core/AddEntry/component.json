{
    "name": "appmixer.apigee.core.AddEntry",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Add a new entry to the key value map to block an IP address with a specified TTL (time to live). This endpoint allows you to dynamically update Apigee configurations to enforce IP blocking rules.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:apigee"
    },
    "quota": {
        "manager": "appmixer:apigee",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "org": {
                        "type": "string"
                    },
                    "env": {
                        "type": "string"
                    },
                    "mapName": {
                        "type": "string"
                    },
                    "ip": {
                        "type": "string"
                    },
                    "ttl": {
                        "type": "integer"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "org": {
                        "type": "text",
                        "tooltip": "The organization identifier in Apigee.",
                        "label": "Org",
                        "index": 0
                    },
                    "env": {
                        "type": "text",
                        "tooltip": "The environment in which the key value map is maintained.",
                        "label": "Env",
                        "index": 1
                    },
                    "mapName": {
                        "type": "text",
                        "tooltip": "The name of the key value map containing the IP entries.",
                        "label": "Map Name",
                        "index": 2
                    },
                    "ip": {
                        "type": "text",
                        "tooltip": "The IP address to block.",
                        "label": "Ip",
                        "index": 3
                    },
                    "ttl": {
                        "type": "number",
                        "tooltip": "Time to live in seconds for the blocked IP entry.",
                        "label": "Ttl",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Entry",
                    "value": "entry",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "title": "Entry.Name"
                            },
                            "ip": {
                                "type": "string",
                                "title": "Entry.Ip"
                            },
                            "ttl": {
                                "type": "number",
                                "title": "Entry.Ttl"
                            },
                            "createdAt": {
                                "type": "string",
                                "title": "Entry.Created At"
                            }
                        }
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,",
    "genv2": true
}