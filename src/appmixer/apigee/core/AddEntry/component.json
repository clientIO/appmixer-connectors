{
    "name": "appmixer.apigee.core.AddEntry",
    "author": "Appmixer <info@appmixer.com>",
    "label": "Block IPs",
    "description": "Add a new entry to the key value map to block an IP address with a specified TTL (time to live). This endpoint allows you to dynamically update Apigee configurations to enforce IP blocking rules.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:apigee",
        "scope": ["https://www.googleapis.com/auth/cloud-platform"]
    },
    "quota": {
        "manager": "appmixer:apigee",
        "resources": "requests"
    },
    "properties": {
        "schema": {
            "type": "object",
            "properties": {
                "org": {
                    "type": "string"
                },
                "env": {
                    "type": "string"
                }
            }
        },
        "inspector": {
            "inputs": {
                "org": {
                    "type": "text",
                    "label": "Organization",
                    "tooltip": "The Apigee organization name.",
                    "index": 0,
                    "source": {
                        "url": "/component/appmixer/apigee/core/ListOrganizations?outPort=out",
                        "data": {
                            "transform": "./ListOrganizations#toSelectOptions"
                        }
                    }
                },
                "env": {
                    "type": "text",
                    "tooltip": "The environment containing the key value map.",
                    "label": "Environment",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/apigee/core/ListEnvironments?outPort=out",
                        "data": {
                            "properties": {
                                "org": "properties/org"
                            },
                            "transform": "./ListEnvironments#toSelectOptions"
                        }
                    }
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "mapName": {
                        "type": "string"
                    },
                    "ips": {
                        "type": "string"
                    },
                    "ttl": {
                        "type": "integer"
                    },
                    "comment": {
                        "type": "string"
                    }
                },
                "required": ["mapName", "ips"]
            },
            "inspector": {
                "inputs": {
                    "mapName": {
                        "type": "text",
                        "tooltip": "The name of the key value map containing the IP entries.",
                        "source": {
                            "url": "/component/appmixer/apigee/core/ListKeyValueMaps?outPort=out",
                            "data": {
                                "properties": {
                                    "org": "properties/org",
                                    "env": "properties/env"
                                },
                                "transform": "./ListKeyValueMaps#toSelectOptions"
                            }
                        },
                        "label": "Key Value Map Name ",
                        "index": 2
                    },
                    "ips": {
                        "type": "text",
                        "tooltip": "Enter a single IPv4 or IPv6 address, or multiple IP addresses separated by commas. JSON format is also supported, for example: [\"1.1.1.1\", \"2.2.2.2\"].",
                        "label": "IPs",
                        "index": 3
                    },
                    "ttl": {
                        "type": "number",
                        "label": "TTL",
                        "tooltip": "Time to live in seconds. The IPs will be automatically removed after this duration. If not set, the IPs will remain permanent.",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Entry",
                    "value": "entry",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "title": "Entry.Name"
                            },
                            "ip": {
                                "type": "string",
                                "title": "Entry.Ip"
                            },
                            "ttl": {
                                "type": "number",
                                "title": "Entry.Ttl"
                            },
                            "createdAt": {
                                "type": "string",
                                "title": "Entry.Created At"
                            }
                        }
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}