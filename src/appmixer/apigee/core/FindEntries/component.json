{
    "name": "appmixer.apigee.core.FindEntries",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve the list of IP block entries from a specified Apigee key value map. This action helps in viewing currently blocked IPs along with their TTL.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:apigee"
    },
    "quota": {
        "manager": "appmixer:apigee",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "organization": {
                        "type": "string"
                    },
                    "environment": {
                        "type": "string"
                    },
                    "mapName": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "organization": {
                        "type": "text",
                        "tooltip": "Apigee organization name",
                        "label": "Organization",
                        "index": 0
                    },
                    "environment": {
                        "type": "text",
                        "tooltip": "Apigee environment name",
                        "label": "Environment",
                        "index": 1
                    },
                    "mapName": {
                        "type": "text",
                        "tooltip": "Name of the Key Value Map in Apigee",
                        "label": "Map Name",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Entries",
                    "value": "entries",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "key": {
                                    "type": "string",
                                    "title": "Entries.Key"
                                },
                                "ttl": {
                                    "type": "number",
                                    "title": "Entries.Ttl"
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,",
    "genv2": true
}