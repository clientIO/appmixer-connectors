    {
        "name": "appmixer.apigee.core.ListEntries",
        "author": "Appmixer <info@appmixer.com>",
        "description": "Retrieve all entries from a specific key value map, which includes the list of currently blocked IP addresses.",
        "version": "1.0.0",
        "private": false,
        "auth": {
            "service": "appmixer:apigee"
        },
        "quota": {
            "manager": "appmixer:apigee",
            "resources": "requests"
        },
        "properties": {
            "schema": {
                "type": "object",
                "properties": {
                    "org": {
                        "type": "string"
                    },
                    "env": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "org": {
                        "type": "text",
                        "label": "Organization",
                        "tooltip": "The Apigee organization name.",
                        "index": 0,
                        "source": {
                            "url": "/component/appmixer/apigee/core/ListOrganizations?outPort=out",
                            "data": {
                                "transform": "./ListOrganizations#toSelectOptions"
                            }
                        }
                    },
                    "env": {
                        "type": "text",
                        "tooltip": "The environment containing the key value map.",
                        "label": "Env",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/apigee/core/ListEnvironments?outPort=out",
                            "data": {
                                "messages": {
                                    "org": "inputs/in/org"
                                },
                                "transform": "./ListEnvironments#toSelectOptions"
                            }
                        }
                    }
                }
            }
        },
        "inPorts": [
            {
                "name": "in",
                "schema": {
                    "type": "object",
                    "properties": {
                        "mapName": {
                            "type": "string"
                        }
                    },
                    "required": ["mapName"]
                },
                "inspector": {
                    "inputs": {
                        "mapName": {
                            "type": "text",
                            "tooltip": "The name of the key value map from which entries will be listed.",
                            "label": "Map Name",
                            "index": 2
                        }
                    }
                }
            }
        ],
        "outPorts": [
            {
                "name": "out",
                "options": [
                    {
                        "label": "Entries",
                        "value": "entries",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "ip": {
                                        "type": "string",
                                        "title": "Entries.Ip"
                                    },
                                    "ttl": {
                                        "type": "number",
                                        "title": "Entries.Ttl"
                                    },
                                    "createdAt": {
                                        "type": "string",
                                        "title": "Entries.Created At"
                                    }
                                }
                            }
                        }
                    },
                    {
                        "label": "Status",
                        "value": "status",
                        "schema": {
                            "type": "string"
                        }
                    }
                ]
            }
        ],
        "icon": "data:image/svg+xml;base64,"
    }