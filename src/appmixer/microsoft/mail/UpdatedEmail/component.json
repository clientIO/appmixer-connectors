{
    "name": "appmixer.microsoft.mail.UpdatedEmail",
    "author": "David Durman <david@client.io>",
    "description": "Watch your Inbox for any updates to your messages.",
    "webhook": true,
    "auth": {
        "service": "appmixer:microsoft",
        "scope": ["Mail.Read"]
    },
    "properties": {
        "schema": {
            "properties": {
                "resource": { "type": "string" }
            }
        },
        "inspector": {
            "inputs": {
                "resource": {
                    "type": "text",
                    "defaultValue": "/me/mailfolders('inbox')/messages",
                    "index": 1,
                    "label": "Resource",
                    "tooltip": "The URI of the resource that emitted the change notification relative to https://graph.microsoft.com. See more details in <a target=\"_blank\" href=\"https://docs.microsoft.com/en-us/graph/outlook-change-notifications-overview\">the Microsoft documentation</a>. For example, to watch for new messages in the Drafts folder, use <code>me/mailfolders('Drafts')/messages</code>."
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Message Id", "value": "id" },
                { "label": "Subject", "value": "subject" },
                { "label": "Body Preview", "value": "bodyPreview" },
                { "label": "Web Link", "value": "webLink" },
                { "label": "Body", "value": "body" },
                { "label": "Sender Email Address Name", "value": "sender.emailAddress.name" },
                { "label": "Sender Email Address", "value": "sender.emailAddress.address" },
                { "label": "From Email Address Name", "value": "from.emailAddress.name" },
                { "label": "From Email Address", "value": "from.emailAddress.address" },
                { "label": "To Recipients", "value": "toRecipients" },
                { "label": "CC Recipients", "value": "ccRecipients" },
                { "label": "BCC Recipients", "value": "bccRecipients" },
                { "label": "Reply To", "value": "replyTo" },
                { "label": "Created Time", "value": "createdDateTime" },
                { "label": "Last Modified Date Time", "value": "lastModifiedDateTime" },
                { "label": "Change Key", "value": "changeKey" },
                { "label": "Categories", "value": "categories" },
                { "label": "Received Date Time", "value": "receivedDateTime" },
                { "label": "Sent Date Time", "value": "sentDateTime" },
                { "label": "Has Attachments", "value": "hasAttachments" },
                { "label": "Internet Message Id", "value": "internetMessageId" },
                { "label": "Importance", "value": "importance" },
                { "label": "Parent Folder Id", "value": "parentFolderId" },
                { "label": "Conversation Id", "value": "conversationId" },
                { "label": "Is Delivery Receipt Requested", "value": "isDeliveryReceiptRequested" },
                { "label": "Is ReadReceipt Requested", "value": "isReadReceiptRequested" },
                { "label": "Is Read", "value": "isRead" },
                { "label": "Is Draft", "value": "isDraft" },
                { "label": "Web Link", "value": "webLink" },
                { "label": "Inference Classification", "value": "inferenceClassification" },
                { "label": "Flag Status", "value": "flag.flagStatus" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
