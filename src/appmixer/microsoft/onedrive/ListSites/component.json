{
  "name": "appmixer.microsoft.onedrive.ListSites",
  "author": "Harsha Surisetty <harsha@client.io>",
  "description": "Lists sites in Microsoft OneDrive.",
  "version": "1.1.0",
  "dependencies": {
    "module": "1.1.0"
  },
  "private": true,
  "auth": {
    "service": "appmixer:microsoft",
    "scope": ["Sites.Read.All"]
  },
  "inPorts": [
    {
      "name": "in",
      "schema": {
        "type": "object",
        "properties": {
          "limit": { "type": "number" }
        }
      },
      "inspector": {
        "inputs": {
          "limit": {
            "type": "number",
            "label": "Limit",
            "tooltip": "Limit the number of returned items. If you leave this field empty, first 100 items will be returned.",
            "defaultValue": 100,
            "index": 1
          }
        }
      }
    }
  ],
  "outPorts": [
    {
      "name": "out",
      "options": [{ "label": "sites", "value": "sites" }]
    }
  ],
  "icon": "data:image/svg+xml;charset=utf-8;base64,"
}
