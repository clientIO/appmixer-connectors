{
    "name": "appmixer.fakturoid.accounting.CreateContact",
    "author": "David Durman <david@client.io>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Create a new contact in your Fakturoid account.",
    "private": false,
    "auth": {
        "service": "appmixer:fakturoid"
    },
    "inPorts": [
        {
            "name": "contact",
            "schema": {
                "type": "object",
                "properties": {
                    "name": { "type": "string" },
                    "type_": { "type": "string" },
                    "street": { "type": "string" },
                    "street2": { "type": "string" },
                    "city": { "type": "string" },
                    "zip": { "type": "string" },
                    "country": { "type": "string" },
                    "registration_no": { "type": "string" },
                    "vat_no": { "type": "string" },
                    "bank_account": { "type": "string" },
                    "iban": { "type": "string" },
                    "variable_symbol": { "type": "string" },
                    "enabled_reminders": { "enum": [true, false] },
                    "full_name": { "type": "string" },
                    "email": { "type": "string" },
                    "email_copy": { "type": "string" },
                    "phone": { "type": "string" },
                    "web": { "type": "string" },
                    "private_note": { "type": "string" }
                },
                "required": [
                    "name"
                ]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Name",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Name of the company."
                            }
                        },
                        "index": 1
                    },
                    "type_": {
                        "type": "select",
                        "group": "transformation",
                        "label": "Type",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Type of your contact (customer/supplier/both)."
                            }
                        },
                        "options": [
                            { "value": "Customer", "content": "customer" },
                            { "value": "Supplier", "content": "supplier" },
                            { "value": "Both", "content": "both" }
                        ],
                        "index": 2
                    },
                    "street": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Street",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Street address."
                            }
                        },
                        "index": 3
                    },
                    "street2": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Street 2nd line",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Street address 2nd line."
                            }
                        },
                        "index": 4
                    },
                    "city": {
                        "type": "text",
                        "group": "transformation",
                        "label": "City",
                        "attrs": {
                            "label": {
                                "data-tooltip": "City."
                            }
                        },
                        "index": 5
                    },
                    "zip": {
                        "type": "text",
                        "group": "transformation",
                        "label": "ZIP",
                        "attrs": {
                            "label": {
                                "data-tooltip": "ZIP code."
                            }
                        },
                        "index": 6
                    },
                    "country": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Country (in ISO format)",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Country (default is same as account). Example: CZ, SK, ..."
                            }
                        },
                        "index": 7
                    },
                    "registration_no": {
                        "type": "text",
                        "group": "transformation",
                        "label": "IČ",
                        "attrs": {
                            "label": {
                                "data-tooltip": "IČ registration number"
                            }
                        },
                        "index": 8
                    },
                    "vat_no": {
                        "type": "text",
                        "group": "transformation",
                        "label": "DIČ",
                        "attrs": {
                            "label": {
                                "data-tooltip": "DIČ (VAT number)"
                            }
                        },
                        "index": 9
                    },
                    "bank_account": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Bank account",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Bank account."
                            }
                        },
                        "index": 10
                    },
                    "iban": {
                        "type": "text",
                        "group": "transformation",
                        "label": "IBAN",
                        "attrs": {
                            "label": {
                                "data-tooltip": "IBAN."
                            }
                        },
                        "index": 11
                    },
                    "variable_symbol": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Variable symbol",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Variable symbol."
                            }
                        },
                        "index": 12
                    },
                    "enabled_reminders": {
                        "type": "toggle",
                        "group": "transformation",
                        "label": "Reminders enabled",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Reminders enabled."
                            }
                        },
                        "index": 13
                    },
                    "full_name": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Full name",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Contact full name."
                            }
                        },
                        "index": 14
                    },
                    "email": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Email address",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Email address for sending invoices."
                            }
                        },
                        "index": 15
                    },
                    "email_copy": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Email address CC",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Email address for sending copy of invoices."
                            }
                        },
                        "index": 16
                    },
                    "phone": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Phone number",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Phone number"
                            }
                        },
                        "index": 17
                    },
                    "web": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Web address",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Web address (URL)"
                            }
                        },
                        "index": 18
                    },
                    "private_note": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Private note",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Private note"
                            }
                        },
                        "index": 19
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newContact",
            "options": [
                { "label": "Contact ID", "value": "id" },
                { "label": "Type", "value": "type" },
                { "label": "Name", "value": "name" },
                { "label": "Street", "value": "street" },
                { "label": "Street 2nd line", "value": "street2" },
                { "label": "City", "value": "city" },
                { "label": "ZIP", "value": "zip" },
                { "label": "Country", "value": "country" },
                { "label": "IČ", "value": "registration_no" },
                { "label": "DIČ", "value": "vat_no" },
                { "label": "Bank account", "value": "bank_account" },
                { "label": "IBAN", "value": "iban" },
                { "label": "Variable symbol", "value": "variable_symbol" },
                { "label": "Reminders enabled", "value": "enabled_reminders" },
                { "label": "Full name", "value": "full_name" },
                { "label": "Email address", "value": "email" },
                { "label": "Email address CC", "value": "email_copy" },
                { "label": "Phone number", "value": "phone" },
                { "label": "Web address", "value": "web" },
                { "label": "Private note", "value": "private_note" }
            ]
        }
    ],
    "properties": {
    }
}
