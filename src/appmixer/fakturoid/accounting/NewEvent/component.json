{
    "name": "appmixer.fakturoid.accounting.NewEvent",
    "author": "David Durman <david@client.io>",
    "icon": "data:image/svg+xml;base64,",
    "description": "This trigger fires every time a new event occured on any of your invoices. The full list of supported events: generated, sent, accepted, sent_reminder, overdue, paid, paid_bank, payment_removed, unpaired_payment, invoice_removed, invoice_locked, invoice_unlocked.",
    "private": false,
    "tick": true,
    "auth": {
        "service": "appmixer:fakturoid"
    },
    "outPorts": [
        {
            "name": "event",
            "options": [
                { "value": "name", "label": "Event name" },
                { "value": "created_at", "label": "Time of event" },
                { "value": "invoice_id", "label": "Invoice ID" },
                { "value": "subject_id", "label": "Subject ID" },
                { "value": "text", "label": "Event text" },
                { "value": "invoice_url", "label": "Invoice URL" },
                { "value": "subject_url", "label": "Subject URL" },
                { "value": "user.id", "label": "User ID" },
                { "value": "user.full_name", "label": "User full name" },
                { "value": "user.avatar", "label": "User avatar URL" }
            ]
        }
    ],
    "properties": {
    }
}
