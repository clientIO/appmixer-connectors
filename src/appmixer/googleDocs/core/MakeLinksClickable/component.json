{
    "name": "appmixer.googleDocs.core.MakeLinksClickable",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Makes all text links in a document clickable by converting plain text URLs to hyperlinks.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:googleDocs",
        "scope": ["https://www.googleapis.com/auth/documents"]
    },
    "quota": {
        "manager": "appmixer:googleDocs",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "documentId": {
                        "type": "string"
                    }
                },
                "required": ["documentId"]
            },
            "inspector": {
                "inputs": {
                    "documentId": {
                        "type": "text",
                        "tooltip": "The ID of the document to make links clickable in.",
                        "label": "Document ID",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Document ID",
                    "value": "documentId",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Links Made Clickable",
                    "value": "linksCount",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Success",
                    "value": "success",
                    "schema": {
                        "type": "boolean"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,",
    "genv2": true
}