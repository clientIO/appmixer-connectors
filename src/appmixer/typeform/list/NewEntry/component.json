{
    "name": "appmixer.typeform.list.NewEntry",
    "author": "Martin Krcmar <martin@client.io>",
    "icon": "data:image/svg+xml;base64,",
    "description": "This trigger fires every time a user fills out a specific typeform.",
    "private": false,
    "auth": {
        "service": "appmixer:typeform",
        "scope": [
            "forms:read",
            "webhooks:read",
            "webhooks:write"
        ]
    },
    "webhook": true,
    "outPorts": [
        {
            "name": "entry",
            "source": {
                "url": "/component/appmixer/typeform/list/Questions?outPort=out",
                "data": {
                    "properties": {
                        "UID": "properties/UID"
                    },
                    "transform": "./Questions#toSelectArray"
                }
            }
        }
    ],
    "properties": {
        "schema": {
            "properties": {
                "UID": { "type": "string" }
            },
            "required": [
                "UID"
            ]
        },
        "inspector": {
            "inputs": {
                "UID": {
                    "type": "select",
                    "label": "Form",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/typeform/list/ListForms?outPort=forms",
                        "data": {
                            "transform": "./ListForms#toSelectArray"
                        }
                    },
                    "tooltip": "Select a form."
                }
            }
        }
    }
}
