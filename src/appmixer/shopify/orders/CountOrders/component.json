{
    "name": "appmixer.shopify.orders.CountOrders",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "description": "When triggered, retrieves a count of all orders.",
    "icon": "data:image/svg+xml;base64,",
    "private": false,
    "auth": {
        "service": "appmixer:shopify",
        "scope": [
            "read_orders"
        ]
    },
    "quota": {
        "manager": "appmixer:shopify",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [ 
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "status": { "type": "string" },
                    "financial_status": { "type": "string" },
                    "fulfillment_status": { "type": "string" },
                    "created_at_min": { "type": "string" },
                    "created_at_max": { "type": "string" },
                    "updated_at_min": { "type": "string" },
                    "updated_at_max": { "type": "string" }
                }
            },
            "inspector": {
                "inputs": {
                    "status": {
                        "type": "select",
                        "label": "Order Status",
                        "index": 1,
                        "options": [
                            { "content": "Any", "value": "any" },
                            { "content": "Open", "value": "open" },
                            { "content": "Closed", "value": "closed" }
                        ],
                        "tooltip": "Filter orders by a given status."
                    },
                    "financial_status": {
                        "type": "select",
                        "label": "Financial Status",
                        "index": 2,
                        "options": [
                            { "content": "Any", "value": "any" },
                            { "content": "Authorized", "value": "authorized" },
                            { "content": "Pending", "value": "pending" },
                            { "content": "Paid", "value": "paid" },
                            { "content": "Refunded", "value": "refunded" },
                            { "content": "Voided", "value": "voided" }
                        ],
                        "tooltip": "Filter orders by a given financial status."
                    },
                    "fulfillment_status": {
                        "type": "select",
                        "label": "Fulfillment Status",
                        "index": 3,
                        "options": [
                            { "content": "Any", "value": "any" },
                            { "content": "Shipped", "value": "shipped" },
                            { "content": "Partial", "value": "partial" },
                            { "content": "Unshipped", "value": "unshipped" }
                        ],
                        "tooltip": "Filter orders by a given fulfillment status."
                    },
                    "created_at_min": {
                        "type": "date-time",
                        "label": "Min Date Created",                  
                        "index": 4,
                        "tooltip": "Filter orders created after date."
                    },
                    "created_at_max": {
                        "type": "date-time",
                        "label": "Max Date Created",                  
                        "index": 5,
                        "tooltip": "Filter orders created before date."
                    },
                    "updated_at_min": {
                        "type": "date-time",
                        "label": "Min Date Updated",                  
                        "index": 6,
                        "tooltip": "Filter orders last updated after date."
                    },
                    "updated_at_max": {
                        "type": "date-time",
                        "label": "Max Date Updated",                  
                        "index": 7,
                        "tooltip": "Filter orders last updated before date."
                    }
                }
            }
        } 
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Total Number of Orders", "value": "count" }
            ]
        }
    ]
}
