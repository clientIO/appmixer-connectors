{
    "name": "appmixer.shopify.reports.DeleteReport",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "description": "When triggered, deletes a specific report.",
    "icon": "data:image/svg+xml;base64,",
    "private": false,
    "auth": {
        "service": "appmixer:shopify",
        "scope": [
            "write_reports"
        ]
    },
    "quota": {
        "manager": "appmixer:shopify",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [{
        "name": "in",
        "schema": {
            "type": "object",
            "properties": {
                "id": { "type": "string" }
            },
            "required": ["id"]
        },
        "inspector": {
            "inputs": {
                "id": {
                    "type": "text",
                    "label": "Report ID",
                    "source": {
                        "url": "/component/appmixer/shopify/reports/ListReports?outPort=reports",
                        "data": {
                            "properties": { "sendWholeArray": true },
                            "transform": "./ListReports#reportsToSelectArray"
                        }
                    },                    
                    "index": 0
                }
            }
        }

    }],
    "outPorts": [
        {
            "name": "deleted",
            "options": [
                { "label": "ID", "value": "id" }
            ]
        }
    ]
}
