{
    "name": "appmixer.hubspot.engagements.UpdateNote",
    "author": "Appmixer <info@appmixer.com>",
    "label": "Engagements - UpdateNote",
    "inspector": {
        "inputs": {
            "noteId": {
                "type": "text",
                "label": "Note ID",
                "tooltip": "The ID of the note to update.",
                "index": 1
            },
            "hsTimestamp": {
                "type": "text",
                "label": "Note Timestamp",
                "tooltip": "This field marks the note's time of creation and determines where the note sits on the record timeline. You can use either a Unix timestamp in milliseconds or UTC format.",
                "index": 2
            },
            "hsNoteTitle": {
                "type": "text",
                "label": "Note Title",
                "tooltip": "The title of the note.",
                "index": 3
            },
            "hsNoteBody": {
                "type": "text",
                "label": "Note Body",
                "tooltip": "The body content of the note.",
                "index": 4
            }
        }
    },
    "outPorts": [
        { "name": "out" }
    ],
    "version": "2.2.0",
    "dependencies": {
        "service": "1.0.1"
    }
}