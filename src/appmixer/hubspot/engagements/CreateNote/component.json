{
    "name": "appmixer.hubspot.engagements.CreateNote",
    "author": "Appmixer <info@appmixer.com>",
    "description": "This endpoint allows you to create a note as an engagement in HubSpot, linking it to specific contacts, companies, or deals.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:hubspot"
    },
    "quota": {
        "manager": "appmixer:hubspot",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "objectId": {
                        "type": "string"
                    },
                    "hsNoteBody": {
                        "type": "string"
                    }
                },
                "required": [
                    "objectId", "hsNoteBody"
                ],
            },
            "inspector": {
                "inputs": {
                    "objectId": {
                        "type": "text",
                        "tooltip": "Id of the object to associate with the note",
                        "label": "Object Id",
                        "index": 0
                    },
                    "hsNoteBody": {
                        "type": "text",
                        "tooltip": "Content of the note",
                        "label": "Body",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Properties",
                    "value": "properties",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "hs_body_preview": {
                                "type": "string",
                                "title": "Properties.Hs Body Preview"
                            },
                            "hs_body_preview_html": {
                                "type": "string",
                                "title": "Properties.Hs Body Preview Html"
                            },
                            "hs_body_preview_is_truncated": {
                                "type": "string",
                                "title": "Properties.Hs Body Preview Is Truncated"
                            },
                            "hs_createdate": {
                                "type": "string",
                                "title": "Properties.Hs Createdate"
                            },
                            "hs_lastmodifieddate": {
                                "type": "string",
                                "title": "Properties.Hs Lastmodifieddate"
                            },
                            "hs_note_body": {
                                "type": "string",
                                "title": "Properties.Hs Note Body"
                            },
                            "hs_obj_coords": {
                                "type": "string",
                                "title": "Properties.Hs Obj Coords"
                            },
                            "hs_object_id": {
                                "type": "string",
                                "title": "Properties.Hs Object Id"
                            },
                            "hs_object_source": {
                                "type": "string",
                                "title": "Properties.Hs Object Source"
                            },
                            "hs_object_source_id": {
                                "type": "string",
                                "title": "Properties.Hs Object Source Id"
                            },
                            "hs_object_source_label": {
                                "type": "string",
                                "title": "Properties.Hs Object Source Label"
                            },
                            "hs_timestamp": {
                                "type": "string",
                                "title": "Properties.Hs Timestamp"
                            }
                        }
                    }
                },
                {
                    "label": "Created At",
                    "value": "createdAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updatedAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Archived",
                    "value": "archived",
                    "schema": {
                        "type": "boolean"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}