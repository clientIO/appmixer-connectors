{
  "name": "appmixer.highrise.list.NewDeal",
  "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
  "icon": "data:image/svg+xml;base64,",
  "description": "This trigger fires any time a new deal is added to Highrise.",
  "private": false,
  "auth": {
    "service": "appmixer:highrise"
  },
  "quota": {
    "manager": "appmixer:highrise",
    "resources": "requests",
    "scope": {
      "userId": "{{userId}}"
    }
  },
  "tick": true,
  "outPorts": [
    {
      "name": "deal",
      "options": [
        { "label": "id", "value": "id" },
        { "label": "name", "value": "name" },
        { "label": "background", "value": "background" },
        { "label": "price", "value": "price" },
        { "label": "price_type", "value": "price_type" },
        { "label": "currency", "value": "currency" },
        { "label": "duration", "value": "duration" },
        { "label": "status", "value": "status" },
        { "label": "category.name", "value": "category.name" },
        { "label": "responsible_party-id", "value": "responsible_party-id" },
        { "label": "party.first_name", "value": "party.first_name" },
        { "label": "party.last_name", "value": "party.last_name" },
        { "label": "created_at", "value": "created_at" }
      ]
    }
  ],
  "properties": {
    "schema": {
      "properties": {
        "companyId": { "type": "string" }
      },
      "required": [
        "companyId"
      ]
    },
    "inspector": {
      "inputs": {
        "companyId": {
          "type": "select",
          "group": "config",
          "label": "Company",
          "index": 1,
          "source": {
            "url": "/component/appmixer/highrise/list/ListUserCompanies?outPort=companies",
            "data": {
              "transform": "./transformers#companyNamesToSelectArray"
            }
          },
          "attrs": {
            "label": {
              "data-tooltip": "Select a company."
            }
          }
        }
      },
      "groups": {
        "config": {
          "label": "Configuration",
          "index": 1
        }
      }
    }
  }
}
