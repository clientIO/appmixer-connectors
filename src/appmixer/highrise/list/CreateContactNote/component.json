{
    "name": "appmixer.highrise.list.CreateContactNote",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Add a note to an existing contact.",
    "private": false,
    "auth": {
        "service": "appmixer:highrise"
    },
    "quota": {
        "manager": "appmixer:highrise",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "note",
            "schema": {
                "type": "object",
                "properties": {
                    "body": { "type": "string" },
                    "subjectId": { "type": "string" }
                },
                "required": [
                    "body",
                    "subjectId"
                ]
            },
            "inspector": {
                "inputs": {
                    "subjectId": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Contact",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/highrise/list/ListPeople?outPort=people",
                            "data": {
                                "properties": {
                                    "companyId": "properties/companyId"
                                },
                                "transform": "./transformers#contactsToSelectArray"
                            }
                        },
                        "attrs": {
                            "label": {
                                "data-tooltip": "Select a person."
                            }
                        }
                    },
                    "body": {
                        "type": "textarea",
                        "group": "transformation",
                        "label": "Note",
                        "index": 2
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newNote",
            "options": [
                { "label": "author_id", "value": "author_id" },
                { "label": "collection_id", "value": "collection_id" },
                { "label": "collection_type", "value": "collection_type" },
                { "label": "created_at", "value": "created_at" },
                { "label": "group_id", "value": "group_id" },
                { "label": "id", "value": "id" },
                { "label": "owner_id", "value": "owner_id" },
                { "label": "subject_id", "value": "subject_id" },
                { "label": "subject_type", "value": "subject_type" },
                { "label": "updated_at", "value": "updated_at" },
                { "label": "visible_to", "value": "visible_to" },
                { "label": "body", "value": "body" },
                { "label": "subject_name", "value": "subject_name" }
            ]
        }
    ],
    "properties": {
        "schema": {
            "properties": {
                "companyId": { "type": "string" },
                "visibleTo": { "type": "string" }
            },
            "required": [
                "companyId"
            ]
        },
        "inspector": {
            "inputs": {
                "companyId": {
                    "type": "select",
                    "group": "config",
                    "label": "Company",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/highrise/list/ListUserCompanies?outPort=companies",
                        "data": {
                            "transform": "./transformers#companyNamesToSelectArray"
                        }
                    },
                    "attrs": {
                        "label": {
                            "data-tooltip": "Select a company."
                        }
                    }
                },
                "visibleTo": {
                    "type": "select",
                    "options": [ "Everyone", "Owner" ],
                    "defaultValue": "Everyone",
                    "group": "config",
                    "label": "Note visibility",
                    "index": 3,
                    "attrs": {
                        "label": {
                            "data-tooltip": "Select who should see."
                        }
                    }
                }
            },
            "groups": {
                "config": {
                    "label": "Configuration",
                    "index": 1
                }
            }
        }
    }
}
