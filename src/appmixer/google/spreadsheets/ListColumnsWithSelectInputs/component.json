{
    "name": "appmixer.google.spreadsheets.ListColumnsWithSelectInputs",
    "author": "Harsha Surisetty <harsha@client.io>",
    "description": "When triggered, returns a list of column names in your spreadsheet.",
    "private": true,
    "inPorts": [
        "in"
    ],
    "outPorts": [
        "out"
    ],
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/spreadsheets",
            "https://www.googleapis.com/auth/drive.metadata.readonly"
        ]
    },
    "quota": {
        "manager": "appmixer:google:spreadsheets",
        "resources": "read",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "properties": {
        "schema": {
            "properties": {
                "sheetId": {
                    "type": "string"
                },
                "worksheetId": {
                    "type": "string"
                },
                "fileId": {
                    "type": "string"
                },
                "delimiter": {
                    "type": "string"
                }
            },
            "oneOf": [
                {
                    "required": [
                        "sheetId",
                        "worksheetId"
                    ]
                },
                {
                    "required": [
                        "fileId"
                    ]
                }
            ]
        },
        "inspector": {
            "inputs": {
                "sheetId": {
                    "type": "select",
                    "label": "Spreadsheet",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/google/spreadsheets/ListSheets?outPort=out",
                        "data": {
                            "transform": "./ListSheets#sheetsToSelectArray"
                        }
                    }
                },
                "worksheetId": {
                    "type": "select",
                    "label": "Worksheet",
                    "index": 2,
                    "source": {
                        "url": "/component/appmixer/google/spreadsheets/ListWorksheets?outPort=out",
                        "data": {
                            "properties": {
                                "sheetId": "properties/sheetId"
                            },
                            "transform": "ListWorksheets#worksheetsToSelectArray"
                        }
                    }
                },
                "fileId": {
                    "type": "filepicker",
                    "index": 3,
                    "label": "File ID",
                    "tooltip": "File ID assigned to file once stored in this application or select a file from your computer."
                },
                "delimiter": {
                    "type": "text",
                    "label": "Delimiter",
                    "index": 4,
                    "defaultValue": ",",
                    "tooltip": "Character to use as a delimiter between columns"
                  }
            }
        }
    },
