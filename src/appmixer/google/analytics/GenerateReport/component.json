{
    "name": "appmixer.google.analytics.GenerateReport",
    "author": "Zbynek Pelunek <zbynek.pelunek@client.io>",
    "description": "Returns a customized pivot report of your Google Analytics event data. Works on GA4 version only.",
    "auth": {
        "service": "appmixer:google:analytics",
        "scope": [
            "https://www.googleapis.com/auth/analytics.readonly"
        ]
    },
    "properties": {
        "schema": {
            "type": "object",
            "properties": {
                "propertyId": {
                    "type": "string"
                }
            },
            "required": [
                "propertyId"
            ]
        },
        "inspector": {
            "inputs": {
                "propertyId": {
                    "type": "text",
                    "label": "Property ID",
                    "index": 0,
                    "tooltip": "To find your Google Analytics 4 property ID, open the property selector (on the right from the Analytics logo) and look for the needed property in the second column (Properties & Apps). The ID will be a nine-digit number. Make sure to provide the Google Analytics 4 property ID for this field and NOT Universal Analytics property IDs, which start with 'UA'."
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "dateRanges": {
                        "type": "string",
                        "enum": [
                            "yesterday",
                            "thisWeekFromSunday",
                            "thisWeekFromMunday",
                            "lastSevenDays",
                            "last28Days",
                            "last30Days",
                            "custom"
                        ]
                    },
                    "customDateRanges": {
                        "type": "object",
                        "properties": {
                            "startDate": {
                                "type": "string"
                            },
                            "endDate": {
                                "type": "string"
                            }
                        }
                    },
                    "dimensions": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "metrics": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "limit": {
                        "type": "number",
                        "max": 250000
                    },
                    "keepEmptyRows": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "dimensions",
                    "metrics",
                    "keepEmptyRows"
                ]
            },
            "inspector": {
                "inputs": {
                    "dateRanges": {
                        "type": "select",
                        "index": 0,
                        "label": "Date Ranges",
                        "tooltip": "If a date range is not provided, the default date range will be last 7 days.",
                        "options": [
                            {
                                "content": "Yesterday",
                                "value": "yesterday"
                            },
                            {
                                "content": "Last 7 days",
                                "value": "lastSevenDays"
                            },
                            {
                                "content": "Last 28 days",
                                "value": "last28Days"
                            },
                            {
                                "content": "Last 30 days",
                                "value": "last30Days"
                            },
                            {
                                "content": "Custom",
                                "value": "custom"
                            }
                        ]
                    },
                    "customDateRanges": {
                        "when": {
                            "eq": {
                                "dateRanges": "custom"
                            }
                        },
                        "type": "expression",
                        "label": "Custom Date Ranges",
                        "tooltip": "",
                        "levels": [
                            "ADD"
                        ],
                        "index": 1,
                        "fields": {
                            "startDate": {
                                "type": "date-time",
                                "label": "Start Date",
                                "index": 1,
                                "config": {
                                    "format": "YYYY-MM-DD",
                                    "enableTime": false
                                }
                            },
                            "endDate": {
                                "type": "date-time",
                                "label": "End Date",
                                "index": 2,
                                "config": {
                                    "format": "YYYY-MM-DD",
                                    "enableTime": false
                                }
                            }
                        }
                    },
                    "dimensions": {
                        "type": "multiselect",
                        "label": "Dimensions",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/google/analytics/ListPropertiesMetadata?outPort=out",
                            "data": {
                                "properties": {
                                    "variableFetch": true,
                                    "propertyId": "properties/propertyId"
                                },
                                "transform": "./ListPropertiesMetadata#dimensionsToMultiselectArray"
                            }
                        }
                    },
                    "metrics": {
                        "type": "multiselect",
                        "label": "Metrics",
                        "index": 3,
                        "source": {
                            "url": "/component/appmixer/google/analytics/ListPropertiesMetadata?outPort=out",
                            "data": {
                                "properties": {
                                    "variableFetch": true,
                                    "propertyId": "properties/propertyId"
                                },
                                "transform": "./ListPropertiesMetadata#metricsToMultiselectArray"
                            }
                        }
                    },
                    "limit": {
                        "type": "number",
                        "label": "Limit",
                        "tooltip": "The number of rows to return. If unspecified, 10,000 rows are returned. Max number of returned results is 250,000 rows.",
                        "index": 4
                    },
                    "keepEmptyRows": {
                        "type": "toggle",
                        "label": "Keep Empty Rows",
                        "defaultValue": false,
                        "index": 5
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/google/analytics/ListPropertiesMetadata?outPort=out",
                "data": {
                    "properties": {
                        "variableFetch": true,
                        "propertyId": "properties/propertyId"
                    },
                    "transform": "./ListPropertiesMetadata#dimensionsAndMetricsArray"
                }
            }
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
