{
    "name": "appmixer.google.blogger.RevertPost",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Reverts a published post to draft state.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:google:blogger",
        "scope": ["https://www.googleapis.com/auth/blogger"]
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "blogId": {
                        "type": "string",
                        "description": "The ID of the blog containing the post."
                    },
                    "postId": {
                        "type": "string",
                        "description": "The ID of the post to revert to draft."
                    }
                },
                "required": ["blogId", "postId"]
            },
            "inspector": {
                "inputs": {
                    "blogId": {
                        "type": "text",
                        "tooltip": "The ID of the blog containing the post.",
                        "label": "Blog Id",
                        "index": 0
                    },
                    "postId": {
                        "type": "text",
                        "tooltip": "The ID of the post to revert to draft.",
                        "label": "Post Id",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Kind",
                    "value": "kind",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Blog",
                    "value": "blog",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "title": "Blog.Id"
                            }
                        }
                    }
                },
                {
                    "label": "Published",
                    "value": "published",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated",
                    "value": "updated",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Url",
                    "value": "url",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Self Link",
                    "value": "selfLink",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Title",
                    "value": "title",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Content",
                    "value": "content",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Author",
                    "value": "author",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "title": "Author.Id"
                            },
                            "displayName": {
                                "type": "string",
                                "title": "Author.Display Name"
                            },
                            "url": {
                                "type": "string",
                                "title": "Author.Url"
                            },
                            "image": {
                                "type": "object",
                                "properties": {
                                    "url": {
                                        "type": "string",
                                        "title": "Author.Image.Url"
                                    }
                                },
                                "title": "Author.Image"
                            }
                        }
                    }
                },
                {
                    "label": "Replies",
                    "value": "replies",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "totalItems": {
                                "type": "string",
                                "title": "Replies.Total Items"
                            },
                            "selfLink": {
                                "type": "string",
                                "title": "Replies.Self Link"
                            }
                        }
                    }
                },
                {
                    "label": "Reader Comments",
                    "value": "readerComments",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Etag",
                    "value": "etag",
