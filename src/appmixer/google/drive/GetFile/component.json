{
    "name": "appmixer.google.drive.GetFile",
    "author": "Alexander Benson <abenson@shipserv.com>",
    "description": "Gets a file in your drive.",
    "private": false,
    "version": "1.0.1",
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/drive",
            "https://www.googleapis.com/auth/drive.appdata",
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "quota": {
        "manager": "appmixer:google:drive",
        "resources": "readandwrite",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "fileId": { "type": "string" }
                },
                "required": ["fileId"]
            },
            "inspector": {
                "inputs": {
                    "fileId": {
                        "type": "text",
                        "index": 1,
                        "label": "File ID",
                        "tooltip": "The ID of the file."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Google Drive File ID", "value": "googleDriveFileId" },
                { "label": "File Name", "value": "fileName" },
                { "label": "Mime Type", "value": "mimeType" },
                { "label": "Web Link", "value": "webViewLink" },
                { "label": "Time Created", "value": "createdTime" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
