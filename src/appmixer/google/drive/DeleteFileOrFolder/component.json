{
    "name": "appmixer.google.drive.DeleteFileOrFolder",
    "author": "David Durman <david@client.io>",
    "description": "Delete a file/folder in your drive.",
    "private": false,
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/drive",
            "https://www.googleapis.com/auth/drive.appdata",
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "fileId": { "type": "string" }
                },
                "required": ["fileId"]
            },
            "inspector": {
                "inputs": {
                    "fileId": {
                        "type": "text",
                        "index": 1,
                        "label": "Google Drive File/Folder ID",
                        "tooltip": "The ID of the file/folder to delete."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Google Drive File ID", "value": "googleDriveFileId" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
