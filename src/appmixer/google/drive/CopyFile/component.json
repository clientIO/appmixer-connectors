{
    "name": "appmixer.google.drive.CopyFile",
    "author": "David Durman <david@client.io>",
    "description": "Copy a file within your Google Drive.",
    "private": false,
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/drive",
            "https://www.googleapis.com/auth/drive.appdata",
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "quota": {
        "manager": "appmixer:google:drive",
        "resources": "readandwrite",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "fileId": { "type": "string" },
                    "fileName": { "type": "string" },
                    "folderLocation": {
                        "oneOf": [
                            { "type": "string" },
                            { "type": "object" }
                        ]
                    }
                },
                "required": ["fileId"]
            },
            "inspector": {
                "inputs": {
                    "fileId": {
                        "type": "text",
                        "index": 1,
                        "label": "Google Drive File ID",
                        "tooltip": "The ID of the Google Drive file you wish to copy."
                    },
                    "fileName": {
                        "type": "text",
                        "index": 2,
                        "label": "New File Name",
                        "tooltip": "The name of the copied file. Leave blank to retain the original file name."
                    },
                    "folderLocation": {
                        "type": "googlepicker",
                        "index": 2,
                        "label": "Destination Folder",
                        "placeholder": "Choose a folder...",
                        "tooltip": "Select the destination folder for the copied file. Leave blank to store the copy of the file in the same folder as the original file.",
                        "view": "FOLDERS"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Google Drive File ID", "value": "googleDriveFileId" },
                { "label": "File Name", "value": "fileName" },
                { "label": "Mime Type", "value": "mimeType" },
                { "label": "Web Link", "value": "webViewLink" },
                { "label": "Time Created", "value": "createdTime" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
