{
    "name": "appmixer.google.drive.ExportFile",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "description": "Export file from your Google drive.",
    "private": false,
    "webhook": true,
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/drive",
            "https://www.googleapis.com/auth/drive.appdata",
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "quota": {
        "manager": "appmixer:google:drive",
        "resources": "readandwrite",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "isToken": {
                        "type": "boolean"
                    },
                    "file": {
                        "oneOf": [
                            { "type": "string" },
                            { "type": "object" }
                        ]
                    }
                },
                "oneOf": [
                    { "required": [ "isToken" ] },
                    { "required": [ "file" ] }
                ]
            },
            "inspector": {
                "inputs": {
                    "file": {
                        "type": "googlepicker",
                        "index": 1,
                        "label": "File",
                        "placeholder": "Choose a file...",
                        "tooltip": "Choose a file to export from Google Drive."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "File ID", "value": "fileId" },
                { "label": "File Name", "value": "fileName" },
                { "label": "Mime Type", "value": "mimeType" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
