{
    "name": "appmixer.google.contacts.UpdateContact",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Updates an existing contact.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:google:contacts"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "resourceName": {
                        "type": "string",
                        "description": "The resource name of the contact to update, e.g. 'people/c1234567890'"
                    },
                    "updatePersonFields": {
                        "type": "string",
                        "description": "Comma-separated list of fields to update, e.g. 'names,emailAddresses'"
                    },
                    "person": {
                        "type": "object",
                        "description": "Person object containing updated contact details"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "resourceName": {
                        "type": "text",
                        "tooltip": "The resource name of the contact to update, e.g. 'people/c1234567890'",
                        "label": "ResourceName",
                        "index": 0
                    },
                    "updatePersonFields": {
                        "type": "text",
                        "tooltip": "Comma-separated list of fields to update, e.g. 'names,emailAddresses'",
                        "label": "UpdatePersonFields",
                        "index": 1
                    },
                    "person": {
                        "type": "text",
                        "tooltip": "Person object containing updated contact details",
                        "label": "Person",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Resource Name",
                    "value": "resourceName",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Etag",
                    "value": "etag",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Names",
                    "value": "names",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "displayName": {
                                    "type": "string",
                                    "title": "Names.Display Name"
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
