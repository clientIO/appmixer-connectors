{
    "name": "appmixer.google.bigquery.AddRow",
    "author": "Camilo Manrique <camilo@client.io>",
    "description": "Add a new row inside a BigQuery table.",
    "private": false,
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/bigquery"
        ]
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object"
            },
            "source": {
                "url": "/component/appmixer/google/bigquery/CreateInspector?outPort=out",
                "data": {
                    "properties": {
                        "projectId": "inputs/in/projectId",
                        "datasetId": "inputs/in/datasetId",
                        "tableId": "inputs/in/tableId",
                        "insertMode": "inputs/in/insertMode",
                        "timePartition": "inputs/in/timePartition"
                    },
                    "transform": "./CreateInspector#addRowInspector"
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "New row", "value": "row" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
