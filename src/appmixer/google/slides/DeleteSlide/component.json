{
    "name": "appmixer.google.slides.DeleteSlide",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Deletes an existing slide on the specified presentation.",
    "version": "1.0.0",
    "private": false,
    "label": "Delete Slide",
    "auth": {
        "service": "appmixer:google:slides",
        "scope": [
            "https://www.googleapis.com/auth/presentations"
        ]
    },
    "quota": {
        "manager": "appmixer:google:slides",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "presentationId": {
                        "type": "string"
                    },
                    "slideId": {
                        "type": "string"
                    }
                },
                "required": [
                    "presentationId", "slideId"
                ]
            },
            "inspector": {
                "inputs": {
                    "presentationId": {
                        "type": "text",
                        "tooltip": "The ID of the presentation.",
                        "label": "Presentation ID",
                        "index": 0
                    },
                    "slideId": {
                        "type": "text",
                        "tooltip": "The ID of the slide to delete.",
                        "label": "Slide ID",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Presentation ID",
                    "value": "presentationId",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Revision ID",
                    "value": "revisionId",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
