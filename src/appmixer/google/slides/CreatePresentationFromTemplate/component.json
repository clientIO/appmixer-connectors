{
    "name": "appmixer.google.slides.CreatePresentationFromTemplate",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new presentation by replacing all tags (e.g. {{!notfound:Name}}, {{!notfound:Email}}) in a template with provided data.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:google:slides"
    },
    "quota": {
        "manager": "appmixer:google:slides",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "templateId": {
                        "type": "string"
                    },
                    "replacements": {
                        "type": "object"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "templateId": {
                        "type": "text",
                        "tooltip": "The ID of the template presentation.",
                        "label": "Template Id",
                        "index": 0
                    },
                    "replacements": {
                        "type": "text",
                        "tooltip": "Key/value pairs for tag replacements within the template.",
                        "label": "Replacements",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Presentation Id",
                    "value": "presentationId",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Title",
                    "value": "title",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
