{
    "name": "appmixer.google.slides.CreateSlideFromTemplateSlide",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new slide by copying and replacing tags (e.g. {{!notfound:name}}, {{!notfound:email}}) from a template slide within a specified presentation.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:google:slides"
    },
    "quota": {
        "manager": "appmixer:google:slides",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "presentationId": {
                        "type": "string"
                    },
                    "templateSlideId": {
                        "type": "string"
                    },
                    "replacements": {
                        "type": "object"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "presentationId": {
                        "type": "text",
                        "tooltip": "The ID of the target presentation.",
                        "label": "Presentation Id",
                        "index": 0
                    },
                    "templateSlideId": {
                        "type": "text",
                        "tooltip": "The ID of the template slide to copy and modify.",
                        "label": "Template Slide Id",
                        "index": 1
                    },
                    "replacements": {
                        "type": "text",
                        "tooltip": "Key/value pairs for tag replacements on the new slide.",
                        "label": "Replacements",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Replies",
                    "value": "replies",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "result": {
                                    "type": "string",
                                    "title": "Replies.Result"
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
