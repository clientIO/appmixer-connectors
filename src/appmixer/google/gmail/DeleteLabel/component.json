{
    "name": "appmixer.google.gmail.DeleteLabel",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Delete label.",
    "auth": {
        "service": "appmixer:google:gmail",
        "scope": [
            "https://www.googleapis.com/auth/gmail.labels"
        ]
    },
    "quota": {
        "manager": "appmixer:google:gmail",
        "resources": "messages.polling",
        "maxWait": 5000,
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "labelId": {
                        "type": "string"
                    }
                },
                "required": [
                    "labelId"
                ]
            },
            "inspector": {
                "inputs": {
                    "labelId": {
                        "type": "text",
                        "label": "Label",
                        "tooltip": "Select the Label to delete",
                        "index": 1,
                        "source": { 
                            "url": "/component/appmixer/google/gmail/ListLabels?outPort=out",
                            "data": {
                                "properties": { "sendWholeArray": true },
                                "transform": "./ListLabels#userLabelsToSelectArray"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "deleted",
            "options": [
                { "label": "Label ID", "value": "id" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
