{
    "name": "appmixer.google.gmail.MoveEmail",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Moves a email to another folder.",
    "private": false,
    "version": "1.0.1",
    "auth": {
        "service": "appmixer:google:gmail",
        "scope": ["https://www.googleapis.com/auth/gmail.modify"]
    },
    "quota": {
        "manager": "appmixer:google:gmail",
        "maxWait": 5000,
        "resources": "messages.modify",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "emailId": {
                        "type": "string"
                    },
                    "folder": {
                        "type": "string"
                    },
                    "destinationFolder": {
                        "type": "string"
                    }
                },
                "required": ["emailId"]
            },
            "inspector": {
                "inputs": {
                    "emailId": {
                        "type": "text",
                        "label": "Email Message ID",
                        "index": 1,
                        "tooltip": "Insert an email message ID to move."
                    },
                    "folder": {
                        "type": "text",
                        "label": "Folder",
                        "tooltip": "Select a name of folder where you want to move the email from.",
                        "index": 2,
                        "source": { 
                            "url": "/component/appmixer/google/gmail/ListLabels?outPort=out",
                            "data": {
                                "properties": { "sendWholeArray": true },
                                "transform": "./ListLabels#labelsToSelectArrayFiltered"
                            }
                        }
                    },
                    "destinationFolder": {
                        "type": "select",
                        "label": "Destination Folder",
                        "index": 3,
                        "tooltip": "Select a name of the destination folder where you want to move the email.",
                        "source": { 
                            "url": "/component/appmixer/google/gmail/ListLabels?outPort=out",
                            "data": {
                                "properties": { "sendWholeArray": true },
                                "transform": "./ListLabels#labelsToSelectArrayFiltered"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/svg+xml;base64,"
}
