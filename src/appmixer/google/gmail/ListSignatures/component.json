{
    "name": "appmixer.google.gmail.ListSignatures",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieves default signature from your Gmail.",
    "private": true,
    "version": "1.0.1",
    "auth": {
        "service": "appmixer:google",
        "scope": ["https://www.googleapis.com/auth/gmail.readonly"]
    },
    "quota": {
        "manager": "appmixer:google:gmail",
        "maxWait": 5000,
        "resources": "sendAs.list",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        "in"
    ],
    "outPorts": ["out"],
    "icon": "data:image/svg+xml;base64,"
}
