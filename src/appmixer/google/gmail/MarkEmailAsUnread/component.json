{
    "name": "appmixer.google.gmail.MarkEmailAsUnread",
    "author": "Harsha Surisetty <harsha@client.io>",
    "description": "Marks an email or draft as unread.",
    "private": false,
    "version": "1.0.1",
    "auth": {
        "service": "appmixer:google",
        "scope": ["https://www.googleapis.com/auth/gmail.modify"]
    },
    "quota": {
        "manager": "appmixer:google:gmail",
        "maxWait": 5000,
        "resources": "messages.polling",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "emailId": {
                        "type": "string"
                    }
                },
                "required": ["emailId"]
            },
            "inspector": {
                "inputs": {
                    "emailId": {
                        "type": "text",
                        "index": 1,
                        "label": "Email ID",
                        "tooltip": "Email ID of the message that you want to retrieve."
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/svg+xml;base64,"
}
