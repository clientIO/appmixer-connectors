{
    "name": "appmixer.google.calendar.CreateCalendar",
    "author": "Pawel Putko <pawel.putko@neoteric.eu>",
    "description": "Add a calendar to your calendars.",
    "auth": {
        "service": "appmixer:google",
        "scope": [
            "https://www.googleapis.com/auth/calendar",
            "https://www.googleapis.com/auth/calendar.readonly"
        ]
    },
    "quota": {
        "manager": "appmixer:google:calendar",
        "maxWait": 5000,
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "inspector": {
                "inputs": {
                    "summary": {
                        "type": "text",
                        "index": 1,
                        "label": "Calendar name",
                        "tooltip": "Name of the calendar."
                    },
                    "description": {
                        "type": "text",
                        "index": 2,
                        "label": "Description",
                        "tooltip": "Description of the calendar."
                    },
                    "location": {
                        "type": "text",
                        "index": 3,
                        "label": "Location",
                        "tooltip": "Geographic location of the calendar as free-form text."
                    },
                    "timeZone": {
                        "type": "text",
                        "index": 4,
                        "label": "Time zone",
                        "tooltip": "The time zone of the calendar. (Formatted as an IANA Time Zone Database name, e.g. \"Europe/Zurich.\")."
                    }
                }
            },
            "schema": {
                "type": "object",
                "properties": {
                    "summary": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "location": {
                        "type": "string"
                    },
                    "timeZone": {
                        "type": "string"
                    }
                },
                "required": [
                    "summary"
                ]
            }
        }
    ],
    "outPorts": [
        {
            "name": "calendar",
            "options": [
                { "label": "id", "value": "id" },
                { "label": "description", "value": "description" },
                { "label": "etag", "value": "etag" },
                { "label": "kind", "value": "kind" },
                { "label": "summary", "value": "summary" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
