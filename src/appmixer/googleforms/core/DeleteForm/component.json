{
  "name": "appmixer.googleforms.core.DeleteForm",
  "label": "Delete Form",
  "description": "Removes a Google Form and moves it to trash. Note: This uses Google Drive API as Forms API doesn't have a direct delete endpoint.",
  "author": "Appmixer <info@appmixer.com>",
  "version": "1.0.0",
  "auth": {
    "service": "appmixer:googleforms",
    "scope": [
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/forms.body"
    ]
  },
  "quota": {
    "manager": "appmixer:googleforms",
    "resources": "forms.api",
    "scope": {
      "userId": "{{userId}}"
    }
  },
  "inPorts": [
    {
      "name": "in",
      "inspector": {
        "inputs": {
          "formId": {
            "type": "text",
            "label": "Form ID",
            "tooltip": "The ID of the form to delete",
            "index": 1,
            "required": true
          }
        }
      }
    }
  ],
  "outPorts": [
    {
      "name": "out",
      "options": [
        {
          "label": "Success",
          "value": "success"
        },
        {
          "label": "Form ID",
          "value": "formId"
        },
        {
          "label": "Message",
          "value": "message"
        }
      ]
    }
  ],
  "icon": "data:image/svg+xml;base64,"
}