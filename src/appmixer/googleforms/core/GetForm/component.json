{
    "name": "appmixer.googleforms.core.GetForm",
    "label": "Get Form",
    "description": "Retrieves an existing Google Form by its ID.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:googleforms",
        "scope": [
            "https://www.googleapis.com/auth/forms.body.readonly",
            "https://www.googleapis.com/auth/drive.readonly"
        ]
    },
    "quota": {
        "manager": "appmixer:googleforms",
        "resources": "forms.api",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "inspector": {
                "inputs": {
                    "formId": {
                        "type": "text",
                        "label": "Form ID",
                        "tooltip": "The ID of the form to retrieve",
                        "index": 1,
                        "required": true
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Form ID",
                    "value": "formId"
                },
                {
                    "label": "Response URL",
                    "value": "responderUri"
                },
                {
                    "label": "Edit URL",
                    "value": "info.documentProperties.editorUrl"
                },
                {
                    "label": "Published URL",
                    "value": "publishedUrl"
                },
                {
                    "label": "Title",
                    "value": "info.title"
                },
                {
                    "label": "Document Title",
                    "value": "info.documentTitle"
                },
                {
                    "label": "Description",
                    "value": "info.description"
                },
                {
                    "label": "Is Quiz",
                    "value": "settings.quizSettings.isQuiz"
                },
                {
                    "label": "Items Count",
                    "value": "items.length"
                },
                {
                    "label": "Created Time",
                    "value": "info.documentProperties.createdTime"
                },
                {
                    "label": "Last Modified Time",
                    "value": "info.documentProperties.lastModifiedTime"
                },
                {
                    "label": "Linked Sheet ID",
                    "value": "linkedSheetId"
                },
                {
                    "label": "Form",
                    "value": "form",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "formId": {
                                "type": "string",
                                "title": "Form ID"
                            },
                            "info": {
                                "type": "object",
                                "title": "Form Info",
                                "properties": {
                                    "title": {
                                        "type": "string"
                                    },
                                    "documentTitle": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    },
                                    "documentProperties": {
                                        "type": "object",
                                        "properties": {
                                            "editorUrl": {
                                                "type": "string"
                                            },
                                            "createdTime": {
                                                "type": "string"
                                            },
                                            "lastModifiedTime": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            },
                            "settings": {
                                "type": "object",
                                "title": "Form Settings",
                                "properties": {
                                    "quizSettings": {
                                        "type": "object",
                                        "properties": {
                                            "isQuiz": {
                                                "type": "boolean"
                                            }
                                        }
                                    }
                                }
                            },
                            "items": {
                                "type": "array",
                                "title": "Form Items",
                                "items": {
                                    "type": "object"
                                }
                            },
                            "responderUri": {
                                "type": "string",
                                "title": "Response URL"
                            },
                            "publishedUrl": {
                                "type": "string",
                                "title": "Published URL"
                            },
                            "linkedSheetId": {
                                "type": "string",
                                "title": "Linked Sheet ID"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}