{
  "name": "appmixer.googleforms.core.GetResponse",
  "label": "Get Response",
  "description": "Retrieves the details of a form response by its ID.",
  "author": "Appmixer <info@appmixer.com>",
  "version": "1.0.0",
  "auth": {
    "service": "appmixer:googleforms",
    "scope": [
      "https://www.googleapis.com/auth/forms.responses.readonly"
    ]
  },
  "quota": {
    "manager": "appmixer:googleforms",
    "resources": "forms.api",
    "scope": {
      "userId": "{{userId}}"
    }
  },
  "inPorts": [
    {
      "name": "in",
      "inspector": {
        "inputs": {
          "formId": {
            "type": "text",
            "label": "Form ID",
            "tooltip": "The ID of the form containing the response",
            "index": 1,
            "required": true
          },
          "responseId": {
            "type": "text",
            "label": "Response ID",
            "tooltip": "The ID of the response to retrieve",
            "index": 2,
            "required": true
          }
        }
      }
    }
  ],
  "outPorts": [
    {
      "name": "out",
      "options": [
        {
          "label": "Response ID",
          "value": "responseId"
        },
        {
          "label": "Create Time",
          "value": "createTime"
        },
        {
          "label": "Last Submitted Time",
          "value": "lastSubmittedTime"
        },
        {
          "label": "Respondent Email",
          "value": "respondentEmail"
        },
        {
          "label": "Total Score",
          "value": "totalScore"
        },
        {
          "label": "Answers",
          "value": "answers",
          "schema": {
            "type": "object",
            "additionalProperties": {
              "type": "object",
              "properties": {
                "questionId": {
                  "type": "string"
                },
                "textAnswers": {
                  "type": "object",
                  "properties": {
                    "answers": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "value": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                },
                "grade": {
                  "type": "object",
                  "properties": {
                    "score": {
                      "type": "number"
                    },
                    "correct": {
                      "type": "boolean"
                    },
                    "feedback": {
                      "type": "object",
                      "properties": {
                        "text": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        {
          "label": "Response",
          "value": "response",
          "schema": {
            "type": "object",
            "properties": {
              "responseId": {
                "type": "string",
                "title": "Response ID"
              },
              "createTime": {
                "type": "string",
                "title": "Create Time"
              },
              "lastSubmittedTime": {
                "type": "string",
                "title": "Last Submitted Time"
              },
              "respondentEmail": {
                "type": "string",
                "title": "Respondent Email"
              },
              "answers": {
                "type": "object",
                "title": "Answers"
              },
              "totalScore": {
                "type": "number",
                "title": "Total Score"
              }
            }
          }
        }
      ]
    }
  ],
  "icon": "data:image/svg+xml;base64,"
}