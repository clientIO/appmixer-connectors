{
    "name": "appmixer.googleforms.core.ListForms",
    "label": "List Forms",
    "description": "Lists all Google Forms owned by or shared with the authenticated user.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:googleforms",
        "scope": [
            "https://www.googleapis.com/auth/drive.readonly",
            "https://www.googleapis.com/auth/forms.body.readonly"
        ]
    },
    "quota": {
        "manager": "appmixer:googleforms",
        "resources": "forms.api",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "inspector": {
                "inputs": {
                    "pageSize": {
                        "type": "number",
                        "label": "Page Size",
                        "tooltip": "Maximum number of forms to return (1-1000, default: 100)",
                        "index": 1,
                        "defaultValue": 100
                    },
                    "pageToken": {
                        "type": "text",
                        "label": "Page Token",
                        "tooltip": "Token for pagination to get the next page of results",
                        "index": 2
                    },
                    "orderBy": {
                        "type": "select",
                        "label": "Order By",
                        "tooltip": "Sort order for the results",
                        "index": 3,
                        "defaultValue": "modifiedTime desc",
                        "options": [
                            { "label": "Last Modified (Newest First)", "value": "modifiedTime desc" },
                            { "label": "Last Modified (Oldest First)", "value": "modifiedTime" },
                            { "label": "Created Time (Newest First)", "value": "createdTime desc" },
                            { "label": "Created Time (Oldest First)", "value": "createdTime" },
                            { "label": "Name (A-Z)", "value": "name" },
                            { "label": "Name (Z-A)", "value": "name desc" }
                        ]
                    },
                    "searchQuery": {
                        "type": "text",
                        "label": "Search Query",
                        "tooltip": "Optional search query to filter forms by name",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Forms",
                    "value": "forms",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "title": "Form ID"
                                },
                                "name": {
                                    "type": "string",
                                    "title": "Form Name"
                                },
                                "mimeType": {
                                    "type": "string",
                                    "title": "MIME Type"
                                },
                                "createdTime": {
                                    "type": "string",
                                    "title": "Created Time"
                                },
                                "modifiedTime": {
                                    "type": "string",
                                    "title": "Modified Time"
                                },
                                "webViewLink": {
                                    "type": "string",
                                    "title": "Web View Link"
                                },
                                "iconLink": {
                                    "type": "string",
                                    "title": "Icon Link"
                                },
                                "thumbnailLink": {
                                    "type": "string",
                                    "title": "Thumbnail Link"
                                },
                                "owners": {
                                    "type": "array",
                                    "title": "Owners",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "displayName": {
                                                "type": "string"
                                            },
                                            "emailAddress": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                },
                                "lastModifyingUser": {
                                    "type": "object",
                                    "title": "Last Modifying User",
                                    "properties": {
                                        "displayName": {
                                            "type": "string"
                                        },
                                        "emailAddress": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "shared": {
                                    "type": "boolean",
                                    "title": "Is Shared"
                                },
                                "ownedByMe": {
                                    "type": "boolean",
                                    "title": "Owned By Me"
                                },
                                "capabilities": {
                                    "type": "object",
                                    "title": "Capabilities",
                                    "properties": {
                                        "canEdit": {
                                            "type": "boolean"
                                        },
                                        "canShare": {
                                            "type": "boolean"
                                        },
                                        "canDelete": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Next Page Token",
                    "value": "nextPageToken"
                },
                {
                    "label": "Total Forms Count",
                    "value": "totalForms"
                },
                {
                    "label": "Has More Pages",
                    "value": "hasMorePages"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}