{
    "name": "appmixer.googleforms.core.CreateForm",
    "label": "Create Form",
    "description": "Creates a new Google Form with the specified title and optional document title.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:googleforms",
        "scope": [
            "https://www.googleapis.com/auth/forms.body",
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "quota": {
        "manager": "appmixer:googleforms",
        "resources": "forms.api",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "inspector": {
                "inputs": {
                    "title": {
                        "type": "text",
                        "label": "Form Title",
                        "tooltip": "The title of the form which is visible to responders",
                        "index": 1,
                        "required": true
                    },
                    "documentTitle": {
                        "type": "text",
                        "label": "Document Title",
                        "tooltip": "The title of the document which is visible in Drive (optional, defaults to Form Title)",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Form ID",
                    "value": "formId"
                },
                {
                    "label": "Response URL",
                    "value": "responderUri"
                },
                {
                    "label": "Edit URL",
                    "value": "info.documentProperties.editorUrl"
                },
                {
                    "label": "Published URL",
                    "value": "publishedUrl"
                },
                {
                    "label": "Title",
                    "value": "info.title"
                },
                {
                    "label": "Document Title",
                    "value": "info.documentTitle"
                },
                {
                    "label": "Description",
                    "value": "info.description"
                },
                {
                    "label": "Is Quiz",
                    "value": "settings.quizSettings.isQuiz"
                },
                {
                    "label": "Created Time",
                    "value": "info.documentProperties.createdTime"
                },
                {
                    "label": "Last Modified Time",
                    "value": "info.documentProperties.lastModifiedTime"
                },
                {
                    "label": "Form",
                    "value": "form",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "formId": {
                                "type": "string",
                                "title": "Form ID"
                            },
                            "info": {
                                "type": "object",
                                "title": "Form Info",
                                "properties": {
                                    "title": {
                                        "type": "string"
                                    },
                                    "documentTitle": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                }
                            },
                            "responderUri": {
                                "type": "string",
                                "title": "Response URL"
                            },
                            "publishedUrl": {
                                "type": "string",
                                "title": "Published URL"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}