{
  "name": "appmixer.googleforms.core.UpdateForm",
  "label": "Update Form",
  "description": "Updates an existing Google Form's title, description, or document title using batch update.",
  "author": "Appmixer <info@appmixer.com>",
  "version": "1.0.0",
  "auth": {
    "service": "appmixer:googleforms",
    "scope": [
      "https://www.googleapis.com/auth/forms.body"
    ]
  },
  "quota": {
    "manager": "appmixer:googleforms",
    "resources": "forms.api",
    "scope": {
      "userId": "{{userId}}"
    }
  },
  "inPorts": [
    {
      "name": "in",
      "inspector": {
        "inputs": {
          "formId": {
            "type": "text",
            "label": "Form ID",
            "tooltip": "The ID of the form to update",
            "index": 1,
            "required": true
          },
          "title": {
            "type": "text",
            "label": "Form Title",
            "tooltip": "New title for the form (optional)",
            "index": 2
          },
          "description": {
            "type": "text",
            "label": "Form Description",
            "tooltip": "New description for the form (optional)",
            "index": 3
          },
          "documentTitle": {
            "type": "text",
            "label": "Document Title",
            "tooltip": "New document title visible in Drive (optional)",
            "index": 4
          }
        }
      }
    }
  ],
  "outPorts": [
    {
      "name": "out",
      "options": [
        {
          "label": "Form ID",
          "value": "formId"
        },
        {
          "label": "Success",
          "value": "success"
        },
        {
          "label": "Message",
          "value": "message"
        },
        {
          "label": "Updated Fields",
          "value": "updatedFields",
          "schema": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      ]
    }
  ],
  "icon": "data:image/svg+xml;base64,"
}