{
    "name": "appmixer.line.core.CreateRichMenu",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates a new rich menu.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:line"
    },
    "quota": {
        "manager": "appmixer:line",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "size": {
                        "type": "object",
                        "properties": {
                            "width": {
                                "type": "integer",
                                "description": "Width of the rich menu in pixels"
                            },
                            "height": {
                                "type": "integer",
                                "description": "Height of the rich menu in pixels"
                            }
                        },
                        "description": "Size of the rich menu"
                    },
                    "width": {
                        "type": "integer"
                    },
                    "height": {
                        "type": "integer"
                    },
                    "selected": {
                        "type": "boolean",
                        "description": "Whether the rich menu is displayed by default"
                    },
                    "name": {
                        "type": "string",
                        "description": "Name of the rich menu"
                    },
                    "chatBarText": {
                        "type": "string",
                        "description": "Text displayed in the chat bar"
                    },
                    "areas": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "bounds": {
                                    "type": "object",
                                    "properties": {
                                        "x": {
                                            "type": "integer"
                                        },
                                        "y": {
                                            "type": "integer"
                                        },
                                        "width": {
                                            "type": "integer"
                                        },
                                        "height": {
                                            "type": "integer"
                                        }
                                    },
                                    "description": "Area bounds"
                                },
                                "action": {
                                    "type": "object",
                                    "description": "Action performed when area is tapped"
                                }
                            }
                        },
                        "description": "Areas of the rich menu"
                    },
                    "bounds": {
                        "type": "object",
                        "properties": {
                            "x": {
                                "type": "integer"
                            },
                            "y": {
                                "type": "integer"
                            },
                            "width": {
                                "type": "integer"
                            },
                            "height": {
                                "type": "integer"
                            }
                        },
                        "description": "Area bounds"
                    },
                    "x": {
                        "type": "integer"
                    },
                    "y": {
                        "type": "integer"
                    },
                    "action": {
                        "type": "object",
                        "description": "Action performed when area is tapped"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "size": {
                        "type": "text",
                        "tooltip": "Size of the rich menu",
                        "label": "Size",
                        "index": 0
                    },
                    "width": {
                        "type": "number",
                        "tooltip": "",
                        "label": "Width",
                        "index": 1
                    },
                    "height": {
                        "type": "number",
                        "tooltip": "",
                        "label": "Height",
                        "index": 2
                    },
                    "selected": {
                        "type": "toggle",
                        "tooltip": "Whether the rich menu is displayed by default",
                        "label": "Selected",
                        "index": 3
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the rich menu",
                        "label": "Name",
                        "index": 4
                    },
                    "chatBarText": {
                        "type": "text",
                        "tooltip": "Text displayed in the chat bar",
                        "label": "ChatBarText",
                        "index": 5
                    },
                    "areas": {
                        "type": "text",
                        "tooltip": "Areas of the rich menu",
                        "label": "Areas",
                        "index": 6
                    },
                    "bounds": {
                        "type": "text",
                        "tooltip": "Area bounds",
                        "label": "Bounds",
                        "index": 7
                    },
                    "x": {
                        "type": "number",
                        "tooltip": "",
                        "label": "X",
                        "index": 8
                    },
                    "y": {
                        "type": "number",
                        "tooltip": "",
                        "label": "Y",
                        "index": 9
                    },
                    "action": {
                        "type": "text",
                        "tooltip": "Action performed when area is tapped",
                        "label": "Action",
                        "index": 10
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Rich Menu Id",
                    "value": "richMenuId",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}