{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.1",
        "title": "Voys",
        "description": "Voys is the top business VoIP provider in SA. The full-featured phone system for modern business. Free national calls and unmatched quality!",
        "x-connector-icon": "data:image/jpeg;base64,",
        "x-connector-service": "voys",
        "x-connector-module": "freedom"
    },
    "servers": [
        {
            "url": "https://freedom.voys.nl/api"
        }
    ],
    "x-connector-connection": {
        "type": "apiKey",
        "in": "header",
        "name": "Authorization",
        "value": "token {username}:{apiKey}",
        "schema": {
            "type": "object",
            "properties": {
                "username": {
                    "type": "string",
                    "title": "Username"
                },
                "apiKey": {
                    "type": "string",
                    "title": "API Key"
                }
            }
        },
        "check": {
            "method": "GET",
            "url": "/callnotification/callnotification/",
            "headers": {
                "Authorization": "token {username}:{apiKey}"
            },
            "expect": {
                "status": 405
            }
        },
        "profile": "{username}"
    },
    "security": [
        {
            "api_key_header": [],
            "username": []
        }
    ],
    "paths": {
        "/calls": {
            "x-connector-webhook-create": {
                "operationId": "WatchCall",
                "description": "Triggers when a new call is created, ringing, in progress or ended.",
                "x-connector-type": "trigger",
                "x-connector-webhook": {
                    "type": "subscription",
                    "subscribe": {
                        "url": "{$baseUrl}/callnotification/callnotification/",
                        "method": "POST",
                        "body": {
                            "target_url": "{$webhookUrl}"
                        }
                    },
                    "unsubscribe": {
                        "url": "{$baseUrl}/callnotification/callnotification/{$response.body#/id}/",
                        "method": "DELETE",
                        "body": null
                    },
                    "output": "$request.body",
                    "outputCondition": "$boolean(status=$parameters.status)"
                },
                "parameters": [
                    {
                        "name": "status",
                        "in": "body",
                        "description": "Select an event that you want to observe on calls.",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "created",
                                "ringing",
                                "in-progress",
                                "ended"
                            ],
                            "default": "created"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "call_id": {
                                        "type": "string"
                                    },
                                    "timestamp": {
                                        "type": "string"
                                    },
                                    "status": {
                                        "type": "string"
                                    },
                                    "reason": {
                                        "type": "string"
                                    },
                                    "version": {
                                        "type": "string"
                                    },
                                    "direction": {
                                        "type": "string"
                                    },
                                    "caller": {
                                        "type": "object",
                                        "properties": {
                                            "number": {
                                                "type": "string"
                                            },
                                            "name": {
                                                "type": "string"
                                            },
                                            "account_number": {
                                                "type": "string"
                                            },
                                            "user_numbers": {
                                                "type": "array",
                                                "items": {}
                                            },
                                            "user_emails": {
                                                "type": "array",
                                                "items": {}
                                            }
                                        }
                                    },
                                    "destination": {
                                        "type": "object",
                                        "properties": {
                                            "number": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "username": {
                "type": "apiKey",
                "name": "username",
                "in": "header"
            },
            "api_key_header": {
                "type": "apiKey",
                "name": "apiKey",
                "in": "header"
            }
        },
        "schemas": {
            "CallNotification": {
                "type": "object",
                "properties": {
                    "call_id": {
                        "type": "string"
                    },
                    "timestamp": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    },
                    "reason": {
                        "type": "string"
                    },
                    "version": {
                        "type": "string"
                    },
                    "direction": {
                        "type": "string"
                    },
                    "caller": {
                        "type": "object",
                        "properties": {
                            "number": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            },
                            "account_number": {
                                "type": "string"
                            },
                            "user_numbers": {
                                "type": "array",
                                "items": {}
                            },
                            "user_emails": {
                                "type": "array",
                                "items": {}
                            }
                        }
                    },
                    "destination": {
                        "type": "object",
                        "properties": {
                            "number": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    }
}