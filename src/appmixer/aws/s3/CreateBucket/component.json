{
    "name": "appmixer.aws.s3.CreateBucket",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "description": "Create bucket on AWS S3.",
    "private": false,
    "auth": {
        "service": "appmixer:aws"
    },
    "inPorts": [{
        "name": "in",
        "schema": {
            "type": "object",
            "properties": {
                "name": { "type": "string" },
                "acl": { "type": "string" },
                "versionEnabled": { "type": "boolean" },
                "lockEnabled": { "type": "boolean" }
            },
            "required": [
                "name",
                "versionEnabled"
            ]
        },
        "inspector": {
            "inputs": {
                "name": {
                    "type": "text",
                    "label": "Bucket Name.",
                    "index": 1,
                    "tooltip": "The name of the bucket to create. It must contain a lowercase letter, number or dash(-)."
                },
                "acl": {
                    "type": "select",
                    "label": "Access Control.",
                    "index": 2,
                    "tooltip": "The canned ACL to apply to the bucket.",
                    "options": [
                        { "clearItem": true, "content": "Clear" },
                        { "value": "private", "content": "private" },
                        { "value": "public-read", "content": "public-read" },
                        { "value": "public-read-write", "content": "public-read-write" },
                        { "value": "authenticated-read", "content": "authenticated-read" }
                    ]
                },
                "versionEnabled": {
                    "type": "toggle",
                    "label": "Enable Versioning.",
                    "defaultValue": true,
                    "tooltip": "Specifies whether you want to keep multiple versions of an object in the same bucket.",
                    "index": 3
                },
                "lockEnabled": {
                    "type": "toggle",
                    "label": "Enable Lock.",
                    "defaultValue": false,
                    "tooltip": "Specifies whether you want S3 Object Lock to be enabled for the new bucket.",
                    "index": 4
                }
            }
        }
    }],
    "properties": {
        "schema": {
            "properties": {
                "region": { "type": "string" }
            }
        },
        "inspector": {
            "inputs": {
                "region": {
                    "type": "text",
                    "label": "Region",
                    "index": 1,
                    "tooltip": "Your AWS instance region. If not select, us-east-1 will be used as default.",
                    "source": {
                        "url": "/component/appmixer/aws/s3/ListRegions?outPort=out"
                    }
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "bucket",
            "options": [
                { "label": "Bucket Name", "value": "Name" },
                { "label": "Bucket Location", "value": "Location" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
