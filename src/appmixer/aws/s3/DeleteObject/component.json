{
    "name": "appmixer.aws.s3.DeleteObject",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "description": "Delete object from bucket.",
    "private": false,
    "auth": {
        "service": "appmixer:aws"
    },
    "inPorts": [{
        "name": "in",
        "schema": {
            "type": "object",
            "properties": {
                "bucket": { "type": "string" },
                "key": { "type": "string" }
            },
            "required": [
                "bucket",
                "key"
            ]
        },
        "inspector": {
            "inputs": {
                "bucket": {
                    "type": "select",
                    "label": "Bucket",
                    "index": 1,
                    "tooltip": "Your AWS S3 Bucket.",
                    "source": {
                        "url": "/component/appmixer/aws/s3/ListBuckets?outPort=bucket",
                        "data": {
                            "properties": { "sendWholeArray": true },
                            "transform": "./ListBuckets#bucketsToSelectArray"
                        }
                    }
                },
                "key": {
                    "type": "text",
                    "label": "Object Key",
                    "index": 2,
                    "tooltip": "They key of the object to delete."
                }
            }
        }
    }],
    "properties": {
        "schema": {
            "properties": {
                "region": { "type": "string" }
            }
        },
        "inspector": {
            "inputs": {
                "region": {
                    "type": "text",
                    "label": "Region",
                    "index": 1,
                    "tooltip": "Your AWS instance region. If not select, us-east-1 will be used as default.",
                    "source": {
                        "url": "/component/appmixer/aws/s3/ListRegions?outPort=out"
                    }
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "deleted",
            "options": [
                { "label": "Bucket Name", "value": "Bucket" },
                { "label": "Object Key", "value": "Key" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
