{
    "name": "appmixer.userengage.emails.NewTemplate",
    "author": "Tomasz Hencel <thencel@ntrc.eu>",
    "description": "This trigger fires every time a new template is added.",
    "private": false,
    "tick": true,
    "auth": {
        "service": "appmixer:userengage"
    },
    "quota": {
        "manager": "appmixer:userengage",
        "resources": "requests"
    },
    "outPorts": [
        {
            "name": "template",
            "options": [
                { "label": "content", "value": "content" },
                { "label": "created_at", "value": "created_at" },
                { "label": "desc", "value": "desc" },
                { "label": "id", "value": "id" },
                { "label": "name", "value": "name" },
                { "label": "updated_at", "value": "updated_at" }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
