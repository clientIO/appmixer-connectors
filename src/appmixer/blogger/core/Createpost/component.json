{
    "name": "appmixer.blogger.core.Createpost",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Creates and publishes a new post or creates a new post as a draft.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:blogger",
        "scope": ["https://www.googleapis.com/auth/blogger"]
    },
    "inPorts": [
        {
            "name": "in",
            "shema": {
                "type": "object",
                "properties": {
                    "blogId": {
                        "type": "string",
                        "description": "The ID of the blog where the post will be created."
                    },
                    "isDraft": {
                        "type": "boolean",
                        "description": "Whether to create the post as a draft (true) or publish immediately (false)."
                    },
                    "title": {
                        "type": "string",
                        "description": "The title of the post."
                    },
                    "content": {
                        "type": "string",
                        "description": "The content body of the post."
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "blogId": {
                        "type": "text",
                        "tooltip": "The ID of the blog where the post will be created.",
                        "label": "BlogId",
                        "index": 0
                    },
                    "isDraft": {
                        "type": "toggle",
                        "tooltip": "Whether to create the post as a draft (true) or publish immediately (false).",
                        "label": "IsDraft",
                        "index": 1
                    },
                    "title": {
                        "type": "text",
                        "tooltip": "The title of the post.",
                        "label": "Title",
                        "index": 2
                    },
                    "content": {
                        "type": "text",
                        "tooltip": "The content body of the post.",
                        "label": "Content",
                        "index": 3
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": []
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}