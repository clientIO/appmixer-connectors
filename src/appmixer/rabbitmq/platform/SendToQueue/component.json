{
    "name": "appmixer.rabbitmq.platform.SendToQueue",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Send a message to a queue.",
    "private": false,
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:rabbitmq"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "queue": { "type": "string" },
                    "message": { "type": "string" }
                },
                "required": ["queue", "message"]
            },
            "inspector": {
                "inputs": {
                    "queue": {
                        "type": "text",
                        "index": 1,
                        "label": "Queue",
                        "tooltip": "Enter a queue name."
                    },
                    "message": {
                        "type": "textarea",
                        "index": 2,
                        "label": "Message",
                        "tooltip": "Enter a message."
                    }
                }
            }
        }
    ],
    "outPorts": [{
        "name": "out",
        "options": [{
            "label": "Content",
            "value": "content",
            "schema": { "type": "string", "examples": ["my content"] }
        }, {
            "label": "Fields",
            "value": "fields",
            "schema": { "type": "object" }
        }, {
            "label": "Properties",
            "value": "properties",
            "schema": { "type": "object" }
        }]
    }],
    "icon": "data:image/png;base64,"
}
