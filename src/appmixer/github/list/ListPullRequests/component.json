{
    "name": "appmixer.github.list.ListPullRequests",
    "author": "Appmixer <info@appmixer.com>",
    "icon": "data:image/svg+xml;base64,",
    "description": "When triggered, returns a list of pull requests from a repository.",
    "version": "1.0.1",
    "private": false,
    "auth": {
        "service": "appmixer:github",
        "scope": [
            "user",
            "public_repo",
            "repo",
            "notifications",
            "gist"
        ]
    },
    "quota": {
        "manager": "appmixer:github",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "repositoryId": {
                        "type": "string",
                        "pattern": "[^\/]+[\/][^\/]+"
                    },
                    "state": {
                        "type": "string"
                    },
                    "outputType": {
                        "type": "string"
                    }
                },
                "required": [
                    "repositoryId"
                ]
            },
            "inspector": {
                "inputs": {
                    "repositoryId": {
                        "type": "select",
                        "group": "config",
                        "label": "Repository",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/github/list/ListRepos?outPort=repositories",
                            "data": {
                                "transform": "./transformers#reposToSelectArray"
                            }
                        },
                        "tooltip": "Select a repository."
                    },
                    "state": {
                        "type": "select",
                        "group": "config",
                        "label": "State",
                        "index": 2,
                        "options": [
                            {
                                "label": "Open",
                                "value": "open"
                            },
                            {
                                "label": "Closed",
                                "value": "closed"
                            },
                            {
                                "label": "All",
                                "value": "all"
                            }
                        ],
                        "defaultValue": "open",
                        "tooltip": "Choose whether you want to receive open, closed or all pull requests. Default is open."
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 3,
                        "tooltip": "Choose whether you want to receive the result set as one complete list of pull requests, one PR item at a time, stream the PR items to a file or the first PR out of the result.",
                        "defaultValue": "file",
                        "options": [
                            {
                                "label": "First PR",
                                "value": "first"
                            },
                            {
                                "label": "One at a time",
                                "value": "object"
                            },
                            {
                                "label": "All at once",
                                "value": "array"
                            },
                            {
                                "label": "Save to file",
                                "value": "file"
                            }
                        ]
                    }
                },
                "groups": {
                    "config": {
                        "label": "Configuration",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "pullRequests",
            "source": {
                "url": "/component/appmixer/github/list/ListPullRequests?outPort=pullRequests",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType",
                        "in/state": "inputs/in/state",
                        "in/repositoryId": "inputs/in/repositoryId"
                    }
                }
            }
        },
        {
            "name": "notFound"
        }
    ]
}