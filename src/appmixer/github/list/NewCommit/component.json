{
    "name": "appmixer.github.list.NewCommit",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Triggered when a new commit is created.",
    "private": false,
    "tick": true,
    "auth": {
        "service": "appmixer:github",
        "scope": [
            "user",
            "public_repo",
            "repo",
            "notifications",
            "gist"
        ]
    },
    "quota": {
        "manager": "appmixer:github",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "outPorts": [
        {
            "name": "commit",
            "options": [
                { "label": "sha", "value": "sha" },
                { "label": "html_url", "value": "html_url" },
                { "label": "commit.message", "value": "commit.message" },
                { "label": "commit.author.name", "value": "commit.author.name" },
                { "label": "commit.author.email", "value": "commit.author.email" },
                { "label": "commit.author.date", "value": "commit.author.date" },
                { "label": "commit.committer.name", "value": "commit.committer.name" },
                { "label": "commit.committer.email", "value": "commit.committer.email" },
                { "label": "commit.committer.date", "value": "commit.committer.date" },
                { "label": "commit.tree.sha", "value": "commit.tree.sha" }
            ]
        }
    ],
    "properties": {
        "schema": {
            "properties": {
                "repositoryId": {
                    "type": "string",
                    "pattern": "[^\/]+[\/][^\/]+"
                },
                "branchId": { "type": "string" }
            },
            "required": [
                "repositoryId",
                "branchId"
            ]
        },
        "inspector": {
            "inputs": {
                "repositoryId": {
                    "type": "select",
                    "group": "config",
                    "label": "Repository",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/github/list/ListRepos?outPort=repositories",
                        "data": {
                            "transform": "./transformers#reposToSelectArray"
                        }
                    },
                    "tooltip": "Select a repository."
                },
                "branchId": {
                    "type": "select",
                    "group": "config",
                    "label": "Branch",
                    "index": 2,
                    "source": {
                        "url": "/component/appmixer/github/list/ListBranches?outPort=branches",
                        "data": {
                            "properties": {
                                "repositoryId": "properties/repositoryId"
                            },
                            "transform": "./transformers#branchesToSelectArray"
                        }
                    },
                    "tooltip": "Select a branch."
                }
            },
            "groups": {
                "config": {
                    "label": "Configuration",
                    "index": 1
                }
            }
        }
    }
}
