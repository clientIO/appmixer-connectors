{
    "name": "appmixer.github.list.NewPullRequest",
    "author": "Robert Beinaravicius <rbeinaravicius@ntrc.eu>",
    "icon": "data:image/svg+xml;base64,",
    "description": "Triggered when a new pull request is created.",
    "private": false,
    "tick": true,
    "auth": {
        "service": "appmixer:github",
        "scope": [
            "user",
            "public_repo",
            "repo",
            "notifications",
            "gist"
        ]
    },
    "quota": {
        "manager": "appmixer:github",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "outPorts": [
        {
            "name": "pullRequest",
            "options": [
                { "label": "id", "value": "id" },
                { "label": "title", "value": "title" },
                { "label": "body", "value": "body" },
                { "label": "number", "value": "number" },
                { "label": "state", "value": "state" },
                { "label": "html_url", "value": "html_url" },
                { "label": "diff_url", "value": "diff_url" },
                { "label": "patch_url", "value": "patch_url" },
                { "label": "merge_commit_sha", "value": "merge_commit_sha" },
                { "label": "created_at", "value": "created_at" },
                { "label": "base.label", "value": "base.label" },
                { "label": "base.ref", "value": "base.ref" },
                { "label": "base.sha", "value": "base.sha" },
                { "label": "base.repo.html_url", "value": "base.repo.html_url" },
                { "label": "head.label", "value": "head.label" },
                { "label": "head.ref", "value": "head.ref" },
                { "label": "head.sha", "value": "head.sha" },
                { "label": "head.repo.html_url", "value": "head.repo.html_url" },
                { "label": "head.user.login", "value": "head.user.login" },
                { "label": "head.user.html_url", "value": "head.user.html_url" },
                { "label": "head.user.type", "value": "head.user.type" }
            ]
        }
    ],
    "properties": {
        "schema": {
            "properties": {
                "repositoryId": {
                    "type": "string",
                    "pattern": "[^\/]+[\/][^\/]+"
                }
            },
            "required": [
                "repositoryId"
            ]
        },
        "inspector": {
            "inputs": {
                "repositoryId": {
                    "type": "select",
                    "group": "config",
                    "label": "Repository",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/github/list/ListRepos?outPort=repositories",
                        "data": {
                            "transform": "./transformers#reposToSelectArray"
                        }
                    },
                    "tooltip": "Select a repository."
                }
            },
            "groups": {
                "config": {
                    "label": "Configuration",
                    "index": 1
                }
            }
        }
    }
}
