{
    "name": "appmixer.clearbit.enrichment.FindCompany",
    "author": "Tomasz Hencel <thencel@ntrc.eu>",
    "description": "Find company via clearbit.",
    "webhook": true,
    "private": false,
    "auth": {
        "service": "appmixer:clearbit"
    },
    "quota": {
        "manager": "appmixer:clearbit",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "attributes",
            "schema": {
                "type": "object",
                "properties": {
                    "domain": { "type": "string" },
                    "companyName": { "type": "string" },
                    "linkedIn": { "type": "string" },
                    "twitter": { "type": "string" },
                    "facebook": { "type": "string" }
                },
                "required": [
                    "domain"
                ]
            },
            "inspector": {
                "inputs": {
                    "domain": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Company domain",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Domain of company you are looking for."
                            }
                        },
                        "index": 1
                    },
                    "companyName": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Name",
                        "attrs": {
                            "label": {
                                "data-tooltip": "Name of company you are looking for."
                            }
                        },
                        "index": 2
                    },
                    "linkedIn": {
                        "type": "text",
                        "group": "transformation",
                        "label": "LinkedIn link",
                        "attrs": {
                            "label": {
                                "data-tooltip": "The LinkedIn URL for the company."
                            }
                        },
                        "index": 3
                    },
                    "twitter": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Twitter link",
                        "attrs": {
                            "label": {
                                "data-tooltip": "The Twitter handle for the company."
                            }
                        },
                        "index": 4
                    },
                    "facebook": {
                        "type": "text",
                        "group": "transformation",
                        "label": "Facebook link",
                        "attrs": {
                            "label": {
                                "data-tooltip": "The Facebook URL for the company."
                            }
                        },
                        "index": 5
                    }
                },
                "groups": {
                    "transformation": {
                        "label": "Transformation",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "company",
            "options": [
                { "label": "category.industry", "value": "category.industry" },
                { "label": "category.industryGroup", "value": "category.industryGroup" },
                { "label": "category.sector", "value": "category.sector" },
                { "label": "category.subIndustry", "value": "category.subIndustry" },
                { "label": "crunchbase.handle", "value": "crunchbase.handle" },
                { "label": "description", "value": "description" },
                { "label": "domain", "value": "domain" },
                { "label": "domainAliases", "value": "domainAliases" },
                { "label": "emailProvider", "value": "emailProvider" },
                { "label": "facebook.handle", "value": "facebook.handle" },
                { "label": "foundedYear", "value": "foundedYear" },
                { "label": "geo.city", "value": "geo.city" },
                { "label": "geo.country", "value": "geo.country" },
                { "label": "geo.countryCode", "value": "geo.countryCode" },
                { "label": "geo.lat", "value": "geo.lat" },
                { "label": "geo.lng", "value": "geo.lng" },
                { "label": "geo.postalCode", "value": "geo.postalCode" },
                { "label": "geo.state", "value": "geo.state" },
                { "label": "geo.stateCode", "value": "geo.stateCode" },
                { "label": "geo.streetName", "value": "geo.streetName" },
                { "label": "geo.streetNumber", "value": "geo.streetNumber" },
                { "label": "geo.subPremise", "value": "geo.subPremise" },
                { "label": "id", "value": "id" },
                { "label": "indexedAt", "value": "indexedAt" },
                { "label": "legalName", "value": "legalName" },
                { "label": "linkedin.handle", "value": "linkedin.handle" },
                { "label": "location", "value": "location" },
                { "label": "logo", "value": "logo" },
                { "label": "metrics.alexaGlobalRank", "value": "metrics.alexaGlobalRank" },
                { "label": "metrics.alexaUsRank", "value": "metrics.alexaUsRank" },
                { "label": "metrics.annualRevenue", "value": "metrics.annualRevenue" },
                { "label": "metrics.employees", "value": "metrics.employees" },
                { "label": "metrics.employeesRange", "value": "metrics.employeesRange" },
                { "label": "metrics.googleRank", "value": "metrics.googleRank" },
                { "label": "metrics.marketCap", "value": "metrics.marketCap" },
                { "label": "raised", "value": "raised" },
                { "label": "name", "value": "name" },
                { "label": "phone", "value": "phone" },
                { "label": "site.emailAddresses", "value": "site.emailAddresses" },
                { "label": "site.h1", "value": "site.h1" },
                { "label": "site.metaAuthor", "value": "site.metaAuthor" },
                { "label": "site.metaDescription", "value": "site.metaDescription" },
                { "label": "site.phoneNumbers", "value": "site.phoneNumbers" },
                { "label": "site.title", "value": "site.title" },
                { "label": "site.url", "value": "site.url" },
                { "label": "tags", "value": "tags" },
                { "label": "tech", "value": "tech" },
                { "label": "ticker", "value": "ticker" },
                { "label": "timeZone", "value": "timeZone" },
                { "label": "twitter.avatar", "value": "twitter.avatar" },
                { "label": "twitter.bio", "value": "twitter.bio" },
                { "label": "twitter.followers", "value": "twitter.followers" },
                { "label": "twitter.following", "value": "twitter.following" },
                { "label": "twitter.handle", "value": "twitter.handle" },
                { "label": "twitter.id", "value": "twitter.id" },
                { "label": "twitter.location", "value": "twitter.location" },
                { "label": "twitter.site", "value": "twitter.site" },
                { "label": "type", "value": "type" },
                { "label": "url", "value": "url" },
                { "label": "utcOffset", "value": "utcOffset" }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
