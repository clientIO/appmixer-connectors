{
    "lists.lists": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists?count={{count}}&offset={{offset}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.list": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.addSubscriber": {
        "method": "PUT",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/members/{{subscriberHash}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.updateSubscriber": {
        "method": "PATCH",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/members/{{subscriberHash}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.members": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/members?count={{count}}&offset={{offset}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.createWebhook": {
        "method": "POST",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/webhooks",
        "json": true,
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.deleteWebhook": {
        "method": "DELETE",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/webhooks/{{webhookId}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.getMergeFields": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/merge-fields?count={{count}}&offset={{offset}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "search.members": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/search-members?query={{query}}&list_id={{listId}}&offset={{offset}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "campaigns.campaigns": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/campaigns?count={{count}}&offset={{offset}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "fileManager.files": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/file-manager/files?count={{count}}&offset={{offset}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "fileManager.deleteFile": {
        "method": "DELETE",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/file-manager/files/{{fileId}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "fileManager.files": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/file-manager/files",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "lists.deleteSubscriber": {
        "method": "DELETE",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/lists/{{listId}}/members/{{subscriberId}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "campaigns.campaigns": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/campaigns?offset={{offset}}?count={{count}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    },
    "reports.findReport": {
        "method": "GET",
        "uri": "https://{{dc}}.api.mailchimp.com/3.0/reports/{{campaignId}}",
        "auth": {
            "user": "anystring",
            "pass": "{{auth.accessToken}}"
        }
    }
}
