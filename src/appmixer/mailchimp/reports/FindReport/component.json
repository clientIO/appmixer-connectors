          "value": "facebook_likes.unique_likes"
        },
        {
          "label": "forwards.forwards_count",
          "value": "forwards.forwards_count"
        },
        {
          "label": "forwards.forwards_opens",
          "value": "forwards.forwards_opens"
        },
        { "label": "list_id", "value": "list_id" },
        { "label": "list_name", "value": "list_name" },
        { "label": "list_stats.click_rate", "value": "list_stats.click_rate" },
        { "label": "list_stats.open_rate", "value": "list_stats.open_rate" },
        { "label": "list_stats.sub_rate", "value": "list_stats.sub_rate" },
        { "label": "list_stats.unsub_rate", "value": "list_stats.unsub_rate" },
        { "label": "opens.last_open", "value": "opens.last_open" },
        { "label": "opens.opens_total", "value": "opens.opens_total" },
        { "label": "opens.open_rate", "value": "opens.open_rate" },
        { "label": "opens.unique_opens", "value": "opens.unique_opens" },
        { "label": "preview_text", "value": "preview_text" },
        { "label": "send_time", "value": "send_time" },
