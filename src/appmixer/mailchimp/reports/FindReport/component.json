{
  "name": "appmixer.mailchimp.reports.FindReport",
  "author": "Harsha Surisetty<harsha@client.io>",
  "description": "Find a report based on campaign.",
  "icon": "data:image/svg+xml;base64,",
  "auth": { "service": "appmixer:mailchimp" },
  "inPorts": [
    {
      "name": "in",
      "schema": {
        "type": "object",
        "properties": {
          "campaignId": { "type": "string" }
        },
        "required": ["campaignId"]
      },
      "inspector": {
        "inputs": {
          "campaignId": {
            "type": "text",
            "index": 1,
            "group": "transformation",
            "label": "Campaign id",
            "source": {
              "url": "/component/appmixer/mailchimp/campaigns/ListCampaigns?outPort=out",
              "data": {
                "transform": "./transformers#campaignsToSelectArray"
              }
            }
          }
        },
        "groups": {
          "transformation": {
            "label": "Transformation",
            "index": 1
          }
        }
      }
    }
  ],
  "outPorts": [
    {
      "name": "report",
      "options": [
        { "label": "id", "value": "id" },
        { "label": "abuse_reports", "value": "abuse_reports" },
        { "label": "bounces.hard_bounces", "value": "bounces.hard_bounces" },
        { "label": "bounces.soft_bounces", "value": "bounces.soft_bounces" },
        { "label": "bounces.syntax_errors", "value": "bounces.syntax_errors" },
        { "label": "campaign_title", "value": "campaign_title" },
        { "label": "clicks.clicks_total", "value": "clicks.clicks_total" },
        { "label": "clicks.click_rate", "value": "clicks.click_rate" },
        { "label": "clicks.last_click", "value": "clicks.last_click" },
        { "label": "clicks.unique_clicks", "value": "clicks.unique_clicks" },
        {
          "label": "clicks.unique_subscriber_clicks",
          "value": "clicks.unique_subscriber_clicks"
        },
        {
          "label": "delivery_status.enabled",
          "value": "delivery_status.enabled"
        },
        {
          "label": "ecommerce.total_orders",
          "value": "ecommerce.total_orders"
        },
        {
          "label": "ecommerce.total_revenue",
          "value": "ecommerce.total_revenue"
        },
        { "label": "ecommerce.total_spent", "value": "ecommerce.total_spent" },
        { "label": "emails_sent", "value": "emails_sent" },
        {
          "label": "facebook_likes.facebook_likes",
          "value": "facebook_likes.facebook_likes"
        },
        {
          "label": "facebook_likes.recipient_likes",
          "value": "facebook_likes.recipient_likes"
        },
        {
