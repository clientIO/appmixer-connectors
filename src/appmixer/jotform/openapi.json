{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.0",
        "title": "Jotform",
        "description": "Jotform is a powerful online application that allows anyone to quickly create custom online forms. Its intuitive drag-and-drop user interface makes form building incredibly simple, and doesn't require you to write a single line of code.",
        "x-connector-icon": "https://www.jotform.com/resources/assets/svg/jotform-icon-white.svg",
        "x-connector-service": "jotform",
        "x-connector-module": "core"
    },
    "servers": [{
        "url": "https://{regionPrefix}.jotform.com",
        "variables": {
            "regionPrefix": {
                "enum": ["api", "eu-api"],
                "default": "api",
                "description": "Type \"eu-api\" if your account is in EU Safe mode or \"api\" otherwise."
            }
        }
    }],
    "security": [{
        "api_key_query": []
    }],
    "paths": {
        "/user": {
            "get": {
                "summary": "Get User Information",
                "operationId": "GetUser",
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "responseCode": {
                                            "type": "integer"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "content": {
                                            "$ref": "#/components/schemas/User"
                                        },
                                        "limit-left": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/forms": {
            "get": {
                "summary": "Get User Forms",
                "operationId": "GetUserForms",
                "x-connector-pagination": {
                    "type": "page",
                    "parameters": {
                        "offset": "offset",
                        "limit": "limit",
                        "page": 20,
                        "results": "content",
                        "count": "resultSet.count"
                    }
                },
                "parameters": [
                    {
                        "name": "orderby",
                        "in": "query",
                        "description": "Order By",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "id",
                                "username",
                                "title",
                                "status",
                                "created_at",
                                "updated_at",
                                "new",
                                "count",
                                "slug"
                            ]
                        }
                    },
                    {
                        "name": "filter",
                        "in": "query",
                        "description": "Filter. Filters the query results to fetch a specific form range. Example: {\"new\":\"1\"}. You can also use gt(greater than), lt(less than), ne(not equal to) commands to get more advanced filtering : Example: {\"created_at:gt\":\"2013-01-01 00:00:00\"}",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "responseCode": {
                                            "type": "integer"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "content": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Form"
                                            }
                                        },
                                        "limit-left": {
                                            "type": "integer"
                                        },
                                        "resultSet": {
                                            "$ref": "#/components/schemas/ResultSet"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/reports": {
            "get": {
                "summary": "Get User Reports",
                "operationId": "GetUserReports",
                "x-connector-pagination": {
                    "type": "once",
                    "parameters": {
                        "results": "content"
                    }
                },
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "responseCode": {
                                            "type": "integer"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "content": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Report"
                                            }
                                        },
                                        "limit-left": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/form/{id}": {
            "delete": {
                "summary": "Delete Form",
                "operationId": "DeleteForm",
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "description": "Form ID",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "responseCode": {
                                            "type": "integer"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "content": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "string"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "title": {
                                                    "type": "string"
                                                },
                                                "height": {
                                                    "type": "string"
                                                },
                                                "status": {
                                                    "type": "string"
                                                },
                                                "created_at": {
                                                    "type": "string"
                                                },
                                                "updated_at": {
                                                    "type": "string"
                                                },
                                                "new": {
                                                    "type": "string"
                                                },
                                                "count": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "limit-left": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "x-connector-webhook-create": {
                "operationId": "NewFormSubmission",
                "description": "Triggers when a form is submitted.",
                "x-connector-type": "trigger",
                "x-connector-webhook": {
                    "type": "subscription",
                    "subscribeRequestUrl": "{$baseUrl}/form/{$parameters.formId}/webhooks",
                    "subscribeRequestMethod": "POST",
                    "subscribeRequestHeaders": { "Content-Type": "application/x-www-form-urlencoded" },
                    "subscribeRequestBody": { "webhookURL": "{$webhookUrl}" },
                    "unsubscribeRequestUrl": "{$baseUrl}/form/{$parameters.formId}/webhooks/{$response.transform#$keys(data.content) ~> $filter(function ($key) { $lookup(data.content, $key) = \"{$webhookUrl}\"})}",
                    "unsubscribeRequestMethod": "DELETE",
                    "unsubscribeRequestBody": null,
                    "output": "$request.body",
                    "outputCondition": "$exists(submissionID)",
                },
                "parameters": [{
                    "name": "formId",
                    "in": "body",
                    "description": "Form ID",
                    "required": true,
                    "schema": {
                        "type": "string",
                        "x-connector-source": {
                            "operationId": "GetUserForms",
                            "transform": "result[].{value: id, label: title }"
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/WebhookSubmission"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/submission/{id}": {
            "get": {
                "summary": "Get Form Submission",
                "operationId": "GetFormSubmission",
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "description": "Submission ID",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "responseCode": {
                                            "type": "integer"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "content": {
                                            "$ref": "#/components/schemas/Submission"
                                        },
                                        "limit-left": {
                                            "type": "integer"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/form/{id}/submissions": {
            "get": {
                "summary": "Get Form Submissions",
                "operationId": "GetFormSubmissions",
                "x-connector-pagination": {
                    "type": "page",
                    "parameters": {
                        "offset": "offset",
                        "limit": "limit",
                        "page": 20,
                        "results": "content",
                        "count": "resultSet.count"
                    }
                },
                "parameters": [{
                    "name": "id",
                    "in": "path",
                    "description": "Form ID",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }, {
                    "name": "orderby",
                    "in": "query",
                    "description": "Order By",
                    "required": false,
                    "schema": {
                        "type": "string",
                        "enum": [
                            "id",
                            "username",
                            "title",
                            "status",
                            "created_at",
                            "updated_at",
                            "new",
                            "count",
                            "slug"
                        ]
                    }
                }, {
                        "name": "filter",
                        "in": "query",
                        "description": "Filters the query results to fetch a specific submissions range. Example: {\"id:gt\":\"31974353596870\"} You can also use gt(greater than), lt(less than), ne(not equal to) commands to get more advanced filtering : Example: {\"created_at:gt\":\"2013-01-01 00:00:00\"} Example: {\"workflowStatus:ne\":\"Approve\"}",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                }],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "responseCode": {
                                            "type": "integer"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "content": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Submission"
                                            }
                                        },
                                        "limit-left": {
                                            "type": "integer"
                                        },
                                        "resultSet": {
                                            "$ref": "#/components/schemas/ResultSet"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "api_key_query": {
                "type": "apiKey",
                "name": "apiKey",
                "in": "query",
                "x-connector-connection-check": {
                    "method": "GET",
                    "url": "/user?apiKey={apiKey}"
                },
                "x-connector-connection-profile": {
                    "method": "GET",
                    "url": "/user?apiKey={apiKey}",
                    "transform": "content.name"
                }
            }
        },
        "schemas": {
            "User": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "website": {
                        "type": "string"
                    },
                    "time_zone": {
                        "type": "string"
                    },
                    "account_type": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    },
                    "created_at": {
