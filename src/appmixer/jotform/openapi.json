                    },
                    "folderLayout": {
                        "type": "string"
                    },
                    "language": {
                        "type": "string"
                    },
                    "avatarUrl": {
                        "type": "string"
                    }
                }
            },
            "Form": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "height": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string"
                    },
                    "updated_at": {
                        "type": "string"
                    },
                    "last_submission": {
