                    },
                    "count": {
                        "type": "integer"
                    }
                }
            },
            "WebhookSubmission": {

                "type": "object",
                "properties": {
                    "action": {
                        "type": "string"
                    },
                    "webhookURL": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    },
                    "formID": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "customParams": {
                        "type": "string"
