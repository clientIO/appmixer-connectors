{
    "name": "appmixer.hubsync.grid.GetItemById",
    "description": "Gets an item by ID from HubSync.",
    "author": "Petr Olivka <petr@appmixer.com>",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:hubsync"
    },
    "properties": {
        "schema": {
            "properties": {
                "workspaceId": {
                    "type": "string"
                },
                "databaseId": {
                    "type": "string"
                },
                "sheetId": {
                    "type": "string"
                }
            },
            "required": [
                "workspaceId",
                "databaseId",
                "sheetId"
            ]
        },
        "inspector": {
            "inputs": {
                "workspaceId": {
                    "type": "select",
                    "label": "Workspace ID",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/hubsync/common/FetchWorkspaces?outPort=workspaces",
                        "data": {
                            "transform": "./FetchWorkspaces#workspacesToSelectArray"
                        }
                    },
                    "tooltip": "Select a workspace.",
                    "group": "configuration"
                },
                "databaseId": {
                    "type": "select",
                    "label": "Database ID",
                    "index": 2,
                    "source": {
                        "url": "/component/appmixer/hubsync/grid/FetchDatabases?outPort=databases",
                        "data": {
                            "messages": {
                                "in/workspaceId": "properties/workspaceId"
                            },
                            "transform": "./FetchDatabases#databasesToSelectArray"
                        }
                    },
                    "tooltip": "Select a database.",
                    "group": "configuration"
                },
                "sheetId": {
                    "type": "select",
                    "label": "Sheet ID",
                    "index": 3,
                    "source": {
                        "url": "/component/appmixer/hubsync/grid/FetchSheets?outPort=sheets",
                        "data": {
                            "messages": {
                                "in/workspaceId": "properties/workspaceId",
                                "in/databaseId": "properties/databaseId"
                            },
                            "transform": "./FetchSheets#sheetsToSelectArray"
                        }
                    },
                    "tooltip": "Select a sheet.",
                    "group": "configuration"
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "itemId": {
                        "type": "string"
                    },
                    "itemGlobalId": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "itemId": {
                        "type": "number",
                        "label": "Item ID",
                        "index": 1,
                        "tooltip": "Enter the ID of the item."
                    },
                    "itemGlobalId": {
                        "type": "text",
                        "label": "Item Global ID",
                        "index": 2,
                        "tooltip": "Enter the global ID of the item."
                    }
                }
            }
        }
    ],
    "outPorts": [
        "item"
    ],
    "icon": "data:image/png;base64,"
}