{
    "name": "appmixer.hubsync.grid.CreateNewItem",
    "description": "Creates a new item in HubSync.",
    "author": "Petr Olivka <petr@appmixer.com>",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:hubsync"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "properties": {
                    "workspaceId": {
                        "type": "string"
                    },
                    "databaseId": {
                        "type": "string"
                    },
                    "sheetId": {
                        "type": "string"
                    },
                    "viewId": {
                        "type": "string"
                    },
                    "fields": {
                        "type": "object",
                        "additionalProperties": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "workspaceId",
                    "databaseId",
                    "sheetId",
                    "viewId",
                    "fields"
                ]
            },
            "inspector": {
                "inputs": {
                    "workspaceId": {
                        "type": "select",
                        "label": "Workspace ID",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/hubsync/common/FetchWorkspaces?outPort=workspaces",
                            "data": {
                                "transform": "./FetchWorkspaces#workspacesToSelectArray"
                            }
                        },
                        "tooltip": "Select a workspace."
                    },
                    "databaseId": {
                        "type": "select",
                        "label": "Database ID",
                        "index": 2,
                        "source": {
                            "url": "/component/appmixer/hubsync/grid/FetchDatabases?outPort=databases",
                            "data": {
                                "messages": {
                                    "in/workspaceId": "inputs/in/workspaceId"
                                },
                                "transform": "./FetchDatabases#databasesToSelectArray"
                            }
                        },
                        "tooltip": "Select a database."
                    },
                    "sheetId": {
                        "type": "select",
                        "label": "Sheet ID",
                        "index": 3,
                        "source": {
                            "url": "/component/appmixer/hubsync/grid/FetchSheets?outPort=sheets",
                            "data": {
                                "messages": {
                                    "in/workspaceId": "inputs/in/workspaceId",
                                    "in/databaseId": "inputs/in/databaseId"
                                },
                                "transform": "./FetchSheets#sheetsToSelectArray"
                            }
                        },
                        "tooltip": "Select a sheet."
                    },
                    "viewId": {
                        "type": "select",
                        "label": "View ID",
                        "index": 4,
                        "source": {
                            "url": "/component/appmixer/hubsync/grid/FetchViews?outPort=views",
                            "data": {
                                "messages": {
                                    "in/workspaceId": "inputs/in/workspaceId",
                                    "in/databaseId": "inputs/in/databaseId",
                                    "in/sheetId": "inputs/in/sheetId"
                                },
                                "transform": "./FetchViews#viewsToSelectArray"
                            }
                        },
                        "tooltip": "Select a view."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "newItem",
            "options": [
                {
                    "label": "Id",
                    "value": "id"
                },
                {
                    "label": "Global ID",
                    "value": "globalId"
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}