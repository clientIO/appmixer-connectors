{
    "name": "appmixer.hubsync.grid.CreateNewItem",
    "description": "Creates a new item in HubSync.",
    "author": "Petr Olivka <petr@appmixer.com>",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:hubsync",
        "scope": [
            "write:hubsync",
            "read:hubsync"
        ]
    },
    "properties": {
        "schema": {
            "properties": {
                "project": {
                    "type": "string"
                },
                "issueType": {
                    "type": "string"
                }
            },
            "required": [
                "project",
                "issueType"
            ]
        },
        "inspector": {
            "inputs": {
                "project": {
                    "type": "text",
                    "label": "Project ID",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/jira/issues/IssueMetadata?outPort=out",
                        "data": {
                            "transform": "./IssueMetadata#projectsToSelectArray"
                        }
                    },
                    "tooltip": "Select a project."
                },
                "issueType": {
                    "type": "select",
                    "label": "Issue Type",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/jira/issues/IssueMetadata?outPort=out",
                        "data": {
                            "messages": {
                                "in/project": "properties/project"
                            },
                            "transform": "./IssueMetadata#issueTypesToSelectArray"
                        }
                    },
                    "tooltip": "Select an issue type."
                }
            }
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object"
            },
            "source": {
                "url": "/component/appmixer/jira/issues/IssueMetadata?outPort=out",
                "data": {
                    "messages": {
                        "in/project": "properties/project",
                        "in/issueType": "properties/issueType"
                    },
                    "transform": "./IssueMetadata#createMetaToInspector"
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id"
                },
                {
                    "label": "Key",
                    "value": "key"
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}