{
    "name": "appmixer.hubsync.grid.FetchViews",
    "description": "Fetches all views from HubSync database and sheet.",
    "author": "Petr Olivka <petr@appmixer.com>",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:hubsync"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "workspaceId": {
                        "type": "string",
                        "description": "The ID of the workspace from which to fetch databases."
                    },
                    "databaseId": {
                        "type": "string",
                        "description": "The ID of the database from which to fetch sheets."
                    },
                    "sheetId": {
                        "type": "string",
                        "description": "The ID of the sheet to fetch."
                    }
                },
                "required": [
                    "workspaceId",
                    "databaseId",
                    "sheetId"
                ]
            },
            "inspector": {
                "inputs": {
                    "workspaceId": {
                        "type": "select",
                        "label": "Workspace ID",
                        "index": 1,
                        "tooltip": "Enter the ID of the workspace from which to fetch databases.",
                        "source": {
                            "url": "/component/appmixer/hubsync.common.FetchWorkspaces?outPort=workspaces",
                            "data": {
                                "transform": "./FetchWorkspaces#workspacesToSelectArray"
                            }
                        }
                    },
                    "databaseId": {
                        "type": "select",
                        "label": "Database ID",
                        "index": 2,
                        "tooltip": "Select the database from which to fetch sheets.",
                        "source": {
                            "url": "/component/appmixer/hubsync.grid.FetchDatabases?outPort=databases",
                            "data": {
                                "messages": {
                                    "in/workspaceId": "properties/workspaceId"
                                },
                                "transform": "./FetchDatabases#databasesToSelectArray"
                            }
                        }
                    },
                    "sheetId": {
                        "type": "select",
                        "label": "Sheet ID",
                        "index": 3,
                        "tooltip": "Select the sheet to fetch.",
                        "source": {
                            "url": "/component/appmixer/hubsync.grid.FetchSheets?outPort=sheets",
                            "data": {
                                "messages": {
                                    "in/workspaceId": "properties/workspaceId",
                                    "in/databaseId": "properties/databaseId"
                                },
                                "transform": "./FetchSheets#sheetsToSelectArray"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": [
        "views"
    ],
    "icon": "data:image/png;base64,"
}