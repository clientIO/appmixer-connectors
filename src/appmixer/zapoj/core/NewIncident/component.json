{
    "name": "appmixer.zapoj.core.NewIncident",
    "author": "Appmixer <info@appmixer.com>",
    "description": "This trigger fires every time a new incident is added.",
    "private": false,
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:zapoj"
    },
    "tick": true,
    "quota": {
        "manager": "appmixer:zapoj",
        "resources": "request",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "outPorts": [
        {
            "name": "out"
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
