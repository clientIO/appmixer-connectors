{
    "name": "appmixer.zapoj.core.GetIncidentStatistics",
    "author": "Appmixer <info@appmixer.com>",
    "label": "Get Incident Statistics",
    "description": "Get IT Event Management aggregate counts.",
    "private": false,
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:zapoj"
    },
    "quota": {
        "manager": "appmixer:zapoj",
        "resources": "request",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [{ "name": "in" }],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Alerts", "value": "alerts", "schema": { "type": "number" } },
                { "label": "Templates", "value": "templates", "schema": { "type": "number" } },
                { "label": "Active", "value": "active", "schema": { "type": "number" } },
                { "label": "History", "value": "history", "schema": { "type": "number" } }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
